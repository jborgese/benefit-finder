        }
      ]
    },
    {
      "id": "tanf-federal-residence",
      "programId": "tanf-federal",
      "name": "TANF Residence Requirements",
      "description": "Must live in the state where you apply",
      "ruleLogic": {
        "var": "livesInState"
      },
      "ruleType": "eligibility",
      "explanation": "You must live in the state where you apply for TANF benefits. You cannot receive TANF from multiple states at the same time.",
      "requiredFields": [
        "livesInState"
      ],
      "requiredDocuments": [
        {
          "id": "proof-residence",
          "name": "Proof of Residence",
          "description": "Utility bills, lease agreement, or other proof of address",
          "required": true,
          "alternatives": [
            "Driver's license",
            "Bank statements",
            "Mail with current address"
          ],
          "where": "Utility company, landlord, DMV, or bank"
        }
      ],
      "nextSteps": [
        {
          "step": "If you live in the state, proceed to other eligibility tests",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "TANF Residence Requirements",
          "url": "https://www.acf.hhs.gov/ofa/programs/tanf",
          "legalReference": "45 CFR § 260.30",
          "date": "2024-10-01",
          "notes": "Must be resident of the state where applying"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 6,
      "testCases": [
        {
          "id": "test-eligible-resident",
          "description": "State resident - should qualify",
          "input": {
            "livesInState": true,
            "hasChildren": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "resident"
          ]
        },
        {
          "id": "test-not-eligible-non-resident",
          "description": "Non-resident - should not qualify",
          "input": {
            "livesInState": false,
            "hasChildren": true
          },
          "expected": false,
          "tags": [
            "not-eligible",
            "non-resident"
          ]
        }
      ]
    },
    {
      "id": "tanf-federal-time-limit",
      "programId": "tanf-federal",
      "name": "TANF Time Limit Check",
      "description": "Must not have exceeded lifetime time limits (typically 60 months)",
      "ruleLogic": {
        "<": [
          {
            "var": "monthsOnTANF"
          },
          60
        ]
      },
      "ruleType": "eligibility",
      "explanation": "TANF has lifetime time limits. Most states limit TANF benefits to 60 months (5 years) in a lifetime. Some states have shorter limits. If you've already received TANF for the maximum time, you may not be eligible for additional benefits.",
      "requiredFields": [
        "monthsOnTANF"
      ],
      "requiredDocuments": [
        {
          "id": "proof-tanf-history",
          "name": "Proof of TANF History",
          "description": "Previous TANF benefit statements or case records",
          "required": false,
          "alternatives": [
            "Case worker verification",
            "State benefit records"
          ],
          "where": "Previous state TANF office or case worker"
        }
      ],
      "nextSteps": [
        {
          "step": "If you haven't exceeded time limits, you may be eligible for TANF benefits",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "TANF Time Limits",
          "url": "https://www.acf.hhs.gov/ofa/programs/tanf",
          "legalReference": "45 CFR § 264.1",
          "date": "2024-10-01",
          "notes": "60-month lifetime limit for TANF benefits"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 5,
      "testCases": [
        {
          "id": "test-eligible-within-limits",
          "description": "Within time limits - should qualify",
          "input": {
            "monthsOnTANF": 24,
            "hasChildren": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "within-limits"
          ]
        },
        {
          "id": "test-not-eligible-exceeded-limits",
