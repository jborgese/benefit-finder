    {
      "id": "wic-federal-residence",
      "programId": "wic-federal",
      "name": "WIC Residence Requirement",
      "description": "Must reside in the state where applying for WIC",
      "ruleLogic": {
        "var": "state"
      },
      "ruleType": "eligibility",
      "explanation": "You must live in the state where you apply for WIC benefits. You do not need proof of residency or a permanent address. Migrant farm workers and homeless individuals can receive WIC. You can only receive WIC from one state at a time.",
      "requiredFields": [
        "state"
      ],
      "requiredDocuments": [
        {
          "id": "proof-residence-wic",
          "name": "Proof of Residence",
          "description": "Mail, utility bill, or statement confirming you live in the state",
          "required": false,
          "alternatives": [
            "Driver's license",
            "Homeless shelter statement",
            "Letter from someone you live with",
            "Migrant worker documentation",
            "Verbal statement may be accepted"
          ],
          "where": "Varies - often verbal statement is sufficient"
        }
      ],
      "nextSteps": [
        {
          "step": "WIC is very flexible with residency proof - even a verbal statement may be accepted",
          "priority": "medium"
        }
      ],
