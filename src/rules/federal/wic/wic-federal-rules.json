    {
      "id": "wic-federal-nutritional-risk",
      "programId": "wic-federal",
      "name": "WIC Nutritional Risk Requirement",
      "description": "Must be determined to be at nutritional risk by a health professional",
      "ruleLogic": {
        "var": "hasNutritionalRisk"
      },
      "ruleType": "eligibility",
      "explanation": "To receive WIC, you must be determined by a health professional (doctor, nurse, or nutritionist) to be at 'nutritional risk.' This means having a medical or dietary condition that could affect your nutrition, such as anemia, underweight, overweight, poor diet, or certain medical conditions. This assessment is done for free at the WIC clinic during your appointment. Most applicants meet this requirement.",
      "requiredFields": [],
      "requiredDocuments": [
        {
          "id": "medical-records-wic",
          "name": "Medical Records (if available)",
          "description": "Any medical records showing nutritional concerns",
          "required": false,
          "alternatives": [
            "Doctor's note",
            "Hospital records",
            "Immunization records",
            "Lab results showing anemia",
            "Growth charts"
          ],
          "where": "Your healthcare provider"
        }
      ],
      "nextSteps": [
        {
          "step": "WIC staff will conduct a free nutrition screening at your appointment - no prior documentation required",
          "priority": "medium",
          "estimatedTime": "Completed during WIC appointment"
        },
        {
          "step": "Common nutritional risks include anemia, being underweight or overweight, inadequate diet, or pregnancy complications",
          "priority": "low"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
