    {
      "id": "medicaid-ma-application-process",
      "programId": "medicaid-massachusetts",
      "name": "MassHealth Application Process",
      "description": "Information about applying for MassHealth in Massachusetts",
      "ruleLogic": {
        "var": "livesInMassachusetts"
      },
      "ruleType": "conditional",
      "explanation": "Massachusetts uses the Massachusetts Health Connector for MassHealth and marketplace applications. You can apply: 1) **Online through Massachusetts Health Connector** (recommended - fastest, available 24/7, integrated marketplace and MassHealth), 2) **By phone** at MassHealth Enrollment Center: 1-888-665-9993, 3) **In-person** at a MassHealth Enrollment Center. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Massachusetts offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInMassachusetts"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Massachusetts Health Connector (recommended - handles both MassHealth and marketplace plans)",
          "url": "https://www.mahealthconnector.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call MassHealth Enrollment Center: 1-888-665-9993",
          "priority": "high"
        },
        {
          "step": "Find a MassHealth Enrollment Center near you",
          "url": "https://www.mass.gov/locations/masshealth-enrollment-center",
          "priority": "medium"
        }
