  "rules": [
    {
      "id": "medicaid-ma-expansion-adults",
      "programId": "medicaid-massachusetts",
      "name": "Massachusetts Medicaid for ACA Expansion Adults (MassHealth)",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMassachusetts"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Massachusetts has expanded Medicaid under the Affordable Care Act (MassHealth). Adults age 19-64 are eligible for MassHealth if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through the Massachusetts Health Connector or MassHealth Enrollment Center.",
      "requiredFields": [
        "livesInMassachusetts",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ma-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ma-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Massachusetts Health Connector (recommended - integrated marketplace and MassHealth)",
          "url": "https://www.mahealthconnector.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or call MassHealth Enrollment Center: 1-888-665-9993",
          "priority": "high"
        },
        {
          "step": "Find a MassHealth Enrollment Center near you",
          "url": "https://www.mass.gov/locations/masshealth-enrollment-center",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
