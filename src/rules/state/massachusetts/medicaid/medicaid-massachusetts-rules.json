    {
      "id": "medicaid-ma-children",
      "programId": "medicaid-massachusetts",
      "name": "MassHealth for Children",
      "description": "Children under 19 qualify for MassHealth with income up to 300% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMassachusetts"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4435
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Massachusetts covers children under age 19 through MassHealth if household income is at or below 300% of the federal poverty level. For 2024, this means: 1 child = $4,435/month, 2 children = $6,006/month, 3 children = $7,577/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInMassachusetts",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ma",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ma",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Massachusetts Health Connector",
          "url": "https://www.mahealthconnector.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
