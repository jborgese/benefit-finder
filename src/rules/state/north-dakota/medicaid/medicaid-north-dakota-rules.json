          "input": {
            "livesInNorthDakota": true,
            "age": 18,
            "householdIncome": 1900,
            "householdSize": 1
          },
          "expected": false,
          "tags": [
            "ineligible",
            "age-limit",
            "use-child-rules"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
