    {
      "id": "medicaid-nd-pregnant-women",
      "programId": "medicaid-north-dakota",
      "name": "North Dakota Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 147% FPL qualify for Medicaid in North Dakota",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNorthDakota"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2175
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "North Dakota covers pregnant women through Medicaid if household income is at or below 147% of the federal poverty level. For 2024, this means: 1 person = $2,175/month, 2 people = $2,947/month, 3 people = $3,719/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInNorthDakota",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-nd",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-nd",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through Apply for Benefits ND",
          "url": "https://applyforbenefits.nd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "North Dakota offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
