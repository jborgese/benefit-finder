  "metadata": {
    "id": "medicaid-north-dakota-rules-2024",
    "name": "North Dakota Medicaid Eligibility Rules (2024)",
    "description": "North Dakota-specific Medicaid eligibility rules for 2024. North Dakota expanded Medicaid under the ACA in 2014, so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
