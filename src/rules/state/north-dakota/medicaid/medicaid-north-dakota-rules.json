      "priority": 30,
      "testCases": [
        {
          "id": "test-nd-expansion-eligible",
          "description": "Adult under 138% FPL",
          "input": {
            "livesInNorthDakota": true,
            "age": 35,
            "householdIncome": 1900,
            "householdSize": 1
          },
          "expected": true,
          "tags": [
            "eligible",
            "expansion",
            "138-percent-fpl"
          ]
        },
        {
          "id": "test-nd-expansion-high-income",
          "description": "Adult over 138% FPL",
          "input": {
            "livesInNorthDakota": true,
            "age": 35,
            "householdIncome": 2500,
            "householdSize": 1
          },
