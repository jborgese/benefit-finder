    {
      "id": "medicaid-nd-children",
      "programId": "medicaid-north-dakota",
      "name": "North Dakota Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 175% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNorthDakota"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2590
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "North Dakota covers children under age 19 through Medicaid if household income is at or below 175% of the federal poverty level. For 2024, this means: 1 child = $2,590/month, 2 children = $3,509/month, 3 children = $4,428/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInNorthDakota",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-nd",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-nd",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Apply for Benefits ND",
          "url": "https://applyforbenefits.nd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
