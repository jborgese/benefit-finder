  "rules": [
    {
      "id": "medicaid-nd-expansion-adults",
      "programId": "medicaid-north-dakota",
      "name": "North Dakota Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNorthDakota"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "North Dakota expanded Medicaid under the Affordable Care Act in 2014. Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through Apply for Benefits ND.",
      "requiredFields": [
        "livesInNorthDakota",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-nd-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-nd-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Apply for Benefits ND (recommended)",
          "url": "https://applyforbenefits.nd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find your local County Social Service office",
          "url": "https://www.nd.gov/dhs/locations/countysocialserv/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
