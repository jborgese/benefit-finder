    {
      "id": "medicaid-nd-application-process",
      "programId": "medicaid-north-dakota",
      "name": "North Dakota Medicaid Application Process",
      "description": "Information about applying for Medicaid in North Dakota",
      "ruleLogic": {
        "var": "livesInNorthDakota"
      },
      "ruleType": "conditional",
      "explanation": "North Dakota uses Apply for Benefits ND for Medicaid applications. You can apply: 1) **Online through Apply for Benefits ND** (recommended - fastest, available 24/7), 2) **By phone** at 1-866-240-9403, 3) **In-person** at your local County Social Service office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. North Dakota offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInNorthDakota"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Apply for Benefits ND (recommended)",
          "url": "https://applyforbenefits.nd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call North Dakota Medicaid: 1-866-240-9403",
          "priority": "high"
        },
        {
          "step": "Find your local County Social Service office",
          "url": "https://www.nd.gov/dhs/locations/countysocialserv/",
          "priority": "medium"
        }
      ],
