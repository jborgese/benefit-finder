    {
      "id": "medicaid-nv-children",
      "programId": "medicaid-nevada",
      "name": "Nevada Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 205% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNevada"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3030
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Nevada covers children under age 19 through Medicaid if household income is at or below 205% of the federal poverty level. For 2024, this means: 1 child = $3,030/month, 2 children = $4,105/month, 3 children = $5,180/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInNevada",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-nv",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-nv",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Access Nevada or Nevada Health Link",
          "url": "https://accessnevada.dwss.nv.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
