    {
      "id": "medicaid-nv-application-process",
      "programId": "medicaid-nevada",
      "name": "Nevada Medicaid Application Process",
      "description": "Information about applying for Medicaid in Nevada",
      "ruleLogic": {
        "var": "livesInNevada"
      },
      "ruleType": "conditional",
      "explanation": "Nevada uses Access Nevada and Nevada Health Link for Medicaid applications. You can apply: 1) **Online through Access Nevada** (recommended - fastest, available 24/7), 2) **Online through Nevada Health Link** for health coverage, 3) **By phone** at 1-866-536-5929, 4) **In-person** at your local DWSS office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Nevada offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInNevada"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Access Nevada (recommended)",
          "url": "https://accessnevada.dwss.nv.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through Nevada Health Link",
          "url": "https://www.nevadahealthlink.com/",
          "priority": "high"
        },
        {
          "step": "Find your local DWSS office",
          "url": "https://dwss.nv.gov/Offices/",
          "priority": "medium"
        }
