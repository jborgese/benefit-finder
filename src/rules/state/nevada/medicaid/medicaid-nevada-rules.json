    {
      "id": "medicaid-nv-pregnant-women",
      "programId": "medicaid-nevada",
      "name": "Nevada Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 160% FPL qualify for Medicaid in Nevada",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNevada"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2364
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Nevada covers pregnant women through Medicaid if household income is at or below 160% of the federal poverty level. For 2024, this means: 1 person = $2,364/month, 2 people = $3,202/month, 3 people = $4,040/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInNevada",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-nv",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-nv",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through Access Nevada or Nevada Health Link",
          "url": "https://accessnevada.dwss.nv.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Nevada offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
