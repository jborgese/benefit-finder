    {
      "id": "medicaid-sd-application-process",
      "programId": "medicaid-south-dakota",
      "name": "South Dakota Medicaid Application Process",
      "description": "Information about applying for Medicaid in South Dakota",
      "ruleLogic": {
        "var": "livesInSouthDakota"
      },
      "ruleType": "conditional",
      "explanation": "South Dakota uses the Department of Social Services for Medicaid applications. You can apply: 1) **Online through the South Dakota Department of Social Services portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-452-4643, 3) **In-person** at your local Department of Social Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. South Dakota offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInSouthDakota"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the South Dakota Department of Social Services portal (recommended)",
          "url": "https://dss.sd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call South Dakota Department of Social Services: 1-800-452-4643",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Social Services office",
          "url": "https://dss.sd.gov/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
