  "metadata": {
    "id": "medicaid-south-dakota-rules-2024",
    "name": "South Dakota Medicaid Eligibility Rules (2024)",
    "description": "South Dakota-specific Medicaid eligibility rules for 2024. South Dakota expanded Medicaid under the ACA in July 2023, so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
