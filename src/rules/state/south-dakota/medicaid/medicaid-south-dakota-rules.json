  "rules": [
    {
      "id": "medicaid-sd-expansion-adults",
      "programId": "medicaid-south-dakota",
      "name": "South Dakota Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInSouthDakota"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "South Dakota expanded Medicaid under the Affordable Care Act in July 2023. Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through the South Dakota Department of Social Services.",
      "requiredFields": [
        "livesInSouthDakota",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-sd-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-sd-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the South Dakota Department of Social Services portal (recommended)",
          "url": "https://dss.sd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find your local Department of Social Services office",
          "url": "https://dss.sd.gov/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "South Dakota Medicaid Expansion",
          "url": "https://dss.sd.gov/medicaid/",
          "date": "2024-10-01",
          "notes": "South Dakota expanded Medicaid in July 2023, adults eligible up to 138% FPL"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-sd-expansion-eligible",
          "description": "Adult under 138% FPL",
          "input": {
            "livesInSouthDakota": true,
            "age": 35,
            "householdIncome": 1900,
            "householdSize": 1
          },
          "expected": true,
          "tags": [
            "eligible",
            "expansion",
            "138-percent-fpl"
          ]
        },
        {
          "id": "test-sd-expansion-high-income",
          "description": "Adult over 138% FPL",
          "input": {
            "livesInSouthDakota": true,
            "age": 35,
            "householdIncome": 2500,
            "householdSize": 1
          },
          "expected": false,
          "tags": [
            "ineligible",
            "high-income"
          ]
        },
        {
          "id": "test-sd-expansion-age-limit",
          "description": "Person under 19 not eligible under expansion rules",
          "input": {
            "livesInSouthDakota": true,
            "age": 18,
            "householdIncome": 1900,
            "householdSize": 1
          },
          "expected": false,
          "tags": [
            "ineligible",
            "age-limit",
            "use-child-rules"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "expansion",
        "adults",
        "138-percent-fpl"
      ],
      "category": "categorical-eligibility",
      "jurisdiction": "US-SD"
    },
    {
      "id": "medicaid-sd-pregnant-women",
      "programId": "medicaid-south-dakota",
      "name": "South Dakota Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 194% FPL qualify for Medicaid in South Dakota",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInSouthDakota"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2870
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "South Dakota covers pregnant women through Medicaid if household income is at or below 194% of the federal poverty level. For 2024, this means: 1 person = $2,870/month, 2 people = $3,886/month, 3 people = $4,902/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInSouthDakota",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-sd",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-sd",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through the South Dakota Department of Social Services portal",
          "url": "https://dss.sd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "South Dakota offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "South Dakota Medicaid for Pregnant Women",
          "url": "https://dss.sd.gov/medicaid/",
          "date": "2024-10-01",
          "notes": "South Dakota covers pregnant women up to 194% FPL"
        }
      ],
      "active": true,
