    {
      "id": "medicaid-sd-children",
      "programId": "medicaid-south-dakota",
      "name": "South Dakota Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 209% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInSouthDakota"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3091
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "South Dakota covers children under age 19 through Medicaid if household income is at or below 209% of the federal poverty level. For 2024, this means: 1 child = $3,091/month, 2 children = $4,187/month, 3 children = $5,283/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInSouthDakota",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-sd",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-sd",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the South Dakota Department of Social Services portal",
          "url": "https://dss.sd.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
