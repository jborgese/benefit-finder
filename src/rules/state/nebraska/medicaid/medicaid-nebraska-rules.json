    {
      "id": "medicaid-ne-pregnant-women",
      "programId": "medicaid-nebraska",
      "name": "Nebraska Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 194% FPL qualify for Medicaid in Nebraska",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNebraska"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2867
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Nebraska covers pregnant women through Medicaid if household income is at or below 194% of the federal poverty level. For 2024, this means: 1 person = $2,867/month, 2 people = $3,882/month, 3 people = $4,897/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInNebraska",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ne",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ne",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through ACCESSNebraska",
          "url": "https://access.ne.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Nebraska offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
