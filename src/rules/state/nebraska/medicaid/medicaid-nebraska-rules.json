    {
      "id": "medicaid-ne-application-process",
      "programId": "medicaid-nebraska",
      "name": "Nebraska Medicaid Application Process",
      "description": "Information about applying for Medicaid in Nebraska",
      "ruleLogic": {
        "var": "livesInNebraska"
      },
      "ruleType": "conditional",
      "explanation": "Nebraska uses ACCESSNebraska for all Medicaid applications. You can apply: 1) **Online through ACCESSNebraska** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-383-4278, 3) **In-person** at your local DHHS office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Nebraska offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInNebraska"
      ],
      "nextSteps": [
        {
          "step": "Apply online through ACCESSNebraska (recommended)",
          "url": "https://access.ne.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Nebraska DHHS: 1-800-383-4278",
          "priority": "high"
        },
        {
          "step": "Find your local DHHS office",
          "url": "https://dhhs.ne.gov/Pages/DHHS-Locations.aspx",
          "priority": "medium"
        }
