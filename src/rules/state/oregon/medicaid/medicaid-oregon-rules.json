    {
      "id": "medicaid-or-application-process",
      "programId": "medicaid-oregon",
      "name": "Oregon Medicaid Application Process",
      "description": "Information about applying for Medicaid (Oregon Health Plan) in Oregon",
      "ruleLogic": {
        "var": "livesInOregon"
      },
      "ruleType": "conditional",
      "explanation": "Oregon uses the ONE system (ONE.Oregon.gov) for Oregon Health Plan (OHP) applications. You can apply: 1) **Online through ONE.Oregon.gov** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-699-9075, 3) **In-person** at your local Oregon Department of Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Oregon offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInOregon"
      ],
      "nextSteps": [
        {
          "step": "Apply online through ONE.Oregon.gov (recommended)",
          "url": "https://one.oregon.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Oregon Health Plan: 1-800-699-9075",
          "priority": "high"
        },
        {
          "step": "Find your local Oregon Department of Human Services office",
          "url": "https://www.oregon.gov/odhs/Pages/contact.aspx",
          "priority": "medium"
        }
