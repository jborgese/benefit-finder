    {
      "id": "medicaid-or-children",
      "programId": "medicaid-oregon",
      "name": "Oregon Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 305% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInOregon"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4506
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Oregon covers children under age 19 through the Oregon Health Plan (OHP) if household income is at or below 305% of the federal poverty level. For 2024, this means: 1 child = $4,506/month, 2 children = $6,103/month, 3 children = $7,700/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInOregon",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-or",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-or",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through ONE.Oregon.gov",
          "url": "https://one.oregon.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
