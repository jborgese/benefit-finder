    {
      "id": "medicaid-or-pregnant-women",
      "programId": "medicaid-oregon",
      "name": "Oregon Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 185% FPL qualify for Medicaid in Oregon",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInOregon"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2736
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Oregon covers pregnant women through the Oregon Health Plan (OHP) if household income is at or below 185% of the federal poverty level. For 2024, this means: 1 person = $2,736/month, 2 people = $3,704/month, 3 people = $4,672/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInOregon",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-or",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-or",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through ONE.Oregon.gov",
          "url": "https://one.oregon.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Oregon offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
