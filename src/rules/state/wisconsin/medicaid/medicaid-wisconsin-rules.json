    {
      "id": "medicaid-wi-application-process",
      "programId": "medicaid-wisconsin",
      "name": "Wisconsin Medicaid Application Process",
      "description": "Information about applying for Medicaid (BadgerCare Plus) in Wisconsin",
      "ruleLogic": {
        "var": "livesInWisconsin"
      },
      "ruleType": "conditional",
      "explanation": "Wisconsin uses ACCESS Wisconsin for Medicaid (BadgerCare Plus) applications. You can apply: 1) **Online through ACCESS Wisconsin** (recommended - fastest, available 24/7), 2) **By phone** at 1-888-794-5780, 3) **In-person** at your local Department of Health Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Wisconsin offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInWisconsin"
      ],
      "nextSteps": [
        {
          "step": "Apply online through ACCESS Wisconsin (recommended)",
          "url": "https://access.wisconsin.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Wisconsin Department of Health Services: 1-888-794-5780",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Health Services office",
          "url": "https://www.dhs.wisconsin.gov/forwardhealth/findoffices.htm",
          "priority": "medium"
        }
      ],
