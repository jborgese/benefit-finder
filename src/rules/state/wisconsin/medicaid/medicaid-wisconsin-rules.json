    {
      "id": "medicaid-wi-pregnant-women",
      "programId": "medicaid-wisconsin",
      "name": "Wisconsin Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 306% FPL qualify for Medicaid in Wisconsin",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWisconsin"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4529
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Wisconsin covers pregnant women through BadgerCare Plus if household income is at or below 306% of the federal poverty level. For 2024, this means: 1 person = $4,529/month, 2 people = $6,132/month, 3 people = $7,735/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInWisconsin",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-wi",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-wi",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through ACCESS Wisconsin",
          "url": "https://access.wisconsin.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Wisconsin offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
