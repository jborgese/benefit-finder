      "programId": "medicaid-wisconsin",
      "name": "Wisconsin Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 306% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWisconsin"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4529
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Wisconsin covers children under age 19 through BadgerCare Plus if household income is at or below 306% of the federal poverty level. For 2024, this means: 1 child = $4,529/month, 2 children = $6,132/month, 3 children = $7,735/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInWisconsin",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-wi",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-wi",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through ACCESS Wisconsin",
          "url": "https://access.wisconsin.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
