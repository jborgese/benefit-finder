  "rules": [
    {
      "id": "medicaid-wi-partial-expansion-adults",
      "programId": "medicaid-wisconsin",
      "name": "Wisconsin Medicaid for Adults (Partial Expansion)",
      "description": "Adults 19-64 eligible through BadgerCare Plus up to 100% FPL (Wisconsin has partial expansion, not full ACA expansion)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWisconsin"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  1480
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Wisconsin has partial Medicaid expansion through BadgerCare Plus. Adults age 19-64 are eligible if household income is at or below 100% of the federal poverty level (NOT 138% like full expansion states). For 2024, this means: 1 person = $1,480/month, 2 people = $2,004/month, 3 people = $2,528/month, 4 people = $3,052/month. This creates a coverage gap for adults with income between 100-138% FPL. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through ACCESS Wisconsin.",
      "requiredFields": [
        "livesInWisconsin",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-wi-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-wi-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through ACCESS Wisconsin (recommended)",
          "url": "https://access.wisconsin.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Note: If your income is between 100-138% FPL, you may need to apply for Marketplace coverage with subsidies",
          "priority": "medium"
        },
        {
          "step": "Find your local Department of Health Services office",
          "url": "https://www.dhs.wisconsin.gov/forwardhealth/findoffices.htm",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Wisconsin BadgerCare Plus (Partial Expansion)",
          "url": "https://www.dhs.wisconsin.gov/badgercareplus/index.htm",
          "date": "2024-10-01",
          "notes": "Wisconsin covers adults up to 100% FPL through BadgerCare Plus - partial expansion, not full ACA expansion"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-wi-partial-expansion-eligible",
          "description": "Adult under 100% FPL",
          "input": {
            "livesInWisconsin": true,
            "age": 35,
            "householdIncome": 1400,
            "householdSize": 1
          },
          "expected": true,
          "tags": [
            "eligible",
            "partial-expansion",
            "100-percent-fpl"
          ]
        },
        {
          "id": "test-wi-partial-expansion-coverage-gap",
          "description": "Adult in coverage gap (100-138% FPL)",
