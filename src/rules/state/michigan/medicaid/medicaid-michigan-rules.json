    {
      "id": "medicaid-mi-pregnant-women",
      "programId": "medicaid-michigan",
      "name": "Michigan Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 195% FPL qualify for Medicaid in Michigan",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMichigan"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2883
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Michigan covers pregnant women through Medicaid if household income is at or below 195% of the federal poverty level. For 2024, this means: 1 person = $2,883/month, 2 people = $3,903/month, 3 people = $4,923/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInMichigan",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-mi",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-mi",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through MI Bridges",
          "url": "https://www.mibridges.michigan.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Michigan offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
