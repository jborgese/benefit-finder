    {
      "id": "medicaid-mi-children",
      "programId": "medicaid-michigan",
      "name": "Michigan Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 212% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMichigan"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3136
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Michigan covers children under age 19 through Medicaid if household income is at or below 212% of the federal poverty level. For 2024, this means: 1 child = $3,136/month, 2 children = $4,246/month, 3 children = $5,356/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInMichigan",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-mi",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-mi",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through MI Bridges",
          "url": "https://www.mibridges.michigan.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
