    {
      "id": "medicaid-mi-application-process",
      "programId": "medicaid-michigan",
      "name": "Michigan Medicaid Application Process",
      "description": "Information about applying for Medicaid in Michigan",
      "ruleLogic": {
        "var": "livesInMichigan"
      },
      "ruleType": "conditional",
      "explanation": "Michigan uses MI Bridges for all Medicaid applications. You can apply: 1) **Online through MI Bridges** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-275-6424, 3) **In-person** at your local Michigan Department of Health and Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Michigan offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInMichigan"
      ],
      "nextSteps": [
        {
          "step": "Apply online through MI Bridges (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://www.mibridges.michigan.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Michigan Department of Health and Human Services: 1-855-275-6424",
          "priority": "high"
        },
        {
          "step": "Find your local Michigan Department of Health and Human Services office",
          "url": "https://www.michigan.gov/mdhhs/keep-mi-healthy/localoffices",
          "priority": "medium"
        }
