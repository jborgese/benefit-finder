  "rules": [
    {
      "id": "medicaid-nh-expansion-adults",
      "programId": "medicaid-new-hampshire",
      "name": "New Hampshire Medicaid for ACA Expansion Adults (Granite Advantage)",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNewHampshire"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "New Hampshire expanded Medicaid under the Affordable Care Act in 2014 (Granite Advantage). Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through NH EASY or NH Health Protection.",
      "requiredFields": [
        "livesInNewHampshire",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-nh-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-nh-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through NH EASY (recommended)",
          "url": "https://nheasy.nh.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through NH Health Protection",
          "url": "https://nheasy.nh.gov/",
          "priority": "high"
        },
        {
          "step": "Find your local District Office",
          "url": "https://www.dhhs.nh.gov/contact-us/district-offices",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
