    {
      "id": "medicaid-nh-children",
      "programId": "medicaid-new-hampshire",
      "name": "New Hampshire Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 318% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNewHampshire"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4700
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "New Hampshire covers children under age 19 through Medicaid if household income is at or below 318% of the federal poverty level. For 2024, this means: 1 child = $4,700/month, 2 children = $6,366/month, 3 children = $8,032/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInNewHampshire",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-nh",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-nh",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through NH EASY",
          "url": "https://nheasy.nh.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
