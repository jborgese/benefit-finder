    {
      "id": "medicaid-nh-application-process",
      "programId": "medicaid-new-hampshire",
      "name": "New Hampshire Medicaid Application Process",
      "description": "Information about applying for Medicaid in New Hampshire",
      "ruleLogic": {
        "var": "livesInNewHampshire"
      },
      "ruleType": "conditional",
      "explanation": "New Hampshire uses NH EASY for all Medicaid applications. You can apply: 1) **Online through NH EASY** (recommended - fastest, available 24/7), 2) **By phone** at 1-844-275-3447, 3) **In-person** at your local District Office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. New Hampshire offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInNewHampshire"
      ],
      "nextSteps": [
        {
          "step": "Apply online through NH EASY (recommended)",
          "url": "https://nheasy.nh.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call New Hampshire DHHS: 1-844-275-3447",
          "priority": "high"
        },
        {
          "step": "Find your local District Office",
          "url": "https://www.dhhs.nh.gov/contact-us/district-offices",
          "priority": "medium"
        }
      ],
