    {
      "id": "medicaid-la-application-process",
      "programId": "medicaid-louisiana",
      "name": "Louisiana Medicaid Application Process",
      "description": "Information about applying for Medicaid in Louisiana",
      "ruleLogic": {
        "var": "livesInLouisiana"
      },
      "ruleType": "conditional",
      "explanation": "Louisiana uses the CAFÉ (Common Access Front End) portal for all Medicaid applications. You can apply: 1) **Online through the Louisiana CAFÉ portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-888-LAHELP-U (1-888-524-3578), 3) **In-person** at your local Louisiana DCFS office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Louisiana offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInLouisiana"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Louisiana CAFÉ portal (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://cafe.dcfs.la.gov/selfservice/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Louisiana DCFS: 1-888-LAHELP-U (1-888-524-3578)",
          "priority": "high"
        },
        {
          "step": "Find your local Louisiana DCFS office",
          "url": "https://www.dcfs.louisiana.gov/page/office-locations",
          "priority": "medium"
        }
      ],
