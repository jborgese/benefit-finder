    {
      "id": "medicaid-la-pregnant-women",
      "programId": "medicaid-louisiana",
      "name": "Louisiana Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 200% FPL qualify for Medicaid in Louisiana",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInLouisiana"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2956
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Louisiana covers pregnant women through Medicaid if household income is at or below 200% of the federal poverty level. For 2024, this means: 1 person = $2,956/month, 2 people = $4,004/month, 3 people = $5,052/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInLouisiana",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-la",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-la",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through the Louisiana CAFÉ portal",
          "url": "https://cafe.dcfs.la.gov/selfservice/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Louisiana offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
