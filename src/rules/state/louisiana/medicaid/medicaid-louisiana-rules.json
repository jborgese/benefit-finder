    {
      "id": "medicaid-la-children",
      "programId": "medicaid-louisiana",
      "name": "Louisiana Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 212% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInLouisiana"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3133
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Louisiana covers children under age 19 through Medicaid if household income is at or below 212% of the federal poverty level. For 2024, this means: 1 child = $3,133/month, 2 children = $4,244/month, 3 children = $5,355/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInLouisiana",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-la",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-la",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Louisiana CAFÉ portal",
          "url": "https://cafe.dcfs.la.gov/selfservice/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
