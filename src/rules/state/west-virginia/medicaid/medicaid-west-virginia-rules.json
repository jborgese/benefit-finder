    {
      "id": "medicaid-wv-application-process",
      "programId": "medicaid-west-virginia",
      "name": "West Virginia Medicaid Application Process",
      "description": "Information about applying for Medicaid in West Virginia",
      "ruleLogic": {
        "var": "livesInWestVirginia"
      },
      "ruleType": "conditional",
      "explanation": "West Virginia uses WV PATH for Medicaid applications. You can apply: 1) **Online through WV PATH** (recommended - fastest, available 24/7), 2) **By phone** at 1-877-716-1212, 3) **In-person** at your local Department of Health and Human Resources office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. West Virginia offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInWestVirginia"
      ],
      "nextSteps": [
        {
          "step": "Apply online through WV PATH (recommended)",
          "url": "https://www.wvpath.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call West Virginia Department of Health and Human Resources: 1-877-716-1212",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Health and Human Resources office",
          "url": "https://dhhr.wv.gov/bms/Pages/default.aspx",
          "priority": "medium"
        }
      ],
