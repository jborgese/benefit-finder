  "metadata": {
    "id": "medicaid-west-virginia-rules-2024",
    "name": "West Virginia Medicaid Eligibility Rules (2024)",
    "description": "West Virginia-specific Medicaid eligibility rules for 2024. West Virginia expanded Medicaid under the ACA, so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
