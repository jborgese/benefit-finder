  "rules": [
    {
      "id": "medicaid-wv-expansion-adults",
      "programId": "medicaid-west-virginia",
      "name": "West Virginia Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWestVirginia"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "West Virginia expanded Medicaid under the Affordable Care Act. Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through WV PATH.",
      "requiredFields": [
        "livesInWestVirginia",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-wv-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-wv-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through WV PATH (recommended)",
          "url": "https://www.wvpath.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find your local Department of Health and Human Resources office",
          "url": "https://dhhr.wv.gov/bms/Pages/default.aspx",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
