    {
      "id": "medicaid-wv-children",
      "programId": "medicaid-west-virginia",
      "name": "West Virginia Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 305% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWestVirginia"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4514
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "West Virginia covers children under age 19 through Medicaid if household income is at or below 305% of the federal poverty level. For 2024, this means: 1 child = $4,514/month, 2 children = $6,112/month, 3 children = $7,710/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInWestVirginia",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-wv",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-wv",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through WV PATH",
          "url": "https://www.wvpath.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
