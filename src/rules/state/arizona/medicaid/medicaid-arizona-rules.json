  "rules": [
    {
      "id": "medicaid-az-expansion-adults",
      "programId": "medicaid-arizona",
      "name": "Arizona Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Arizona's ACA expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInArizona"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  1746
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Arizona has expanded Medicaid under the Affordable Care Act. Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage through AHCCCS (Arizona Health Care Cost Containment System). This means: 1 person = $1,746/month, 2 people = $2,359/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, and preventive services.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInArizona"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-az-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-az-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport"
          ],
          "where": "DMV or vital records office"
        },
        {
          "id": "proof-citizenship-az",
          "name": "Proof of Citizenship or Legal Status",
          "description": "Birth certificate, passport, or immigration documents",
          "required": true,
          "alternatives": [
            "Naturalization certificate",
            "Permanent resident card"
          ],
          "where": "Vital records office or immigration services"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Health-e-Arizona Plus",
          "url": "https://www.healthearizonaplus.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call AHCCCS Member Services: 1-855-432-7587",
          "priority": "high"
        },
        {
          "step": "Visit your local DES office for in-person application",
          "url": "https://www.azahcccs.gov/Members/GetStarted/HowToApply.html",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
