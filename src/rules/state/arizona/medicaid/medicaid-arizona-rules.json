    {
      "id": "medicaid-az-application-process",
      "programId": "medicaid-arizona",
      "name": "Arizona Medicaid Application Process",
      "description": "Information about applying for Medicaid in Arizona",
      "ruleLogic": {
        "var": "livesInArizona"
      },
      "ruleType": "conditional",
      "explanation": "To apply for Medicaid (AHCCCS) in Arizona: 1) Apply online at Health-e-Arizona Plus (fastest), 2) Apply by phone at 1-855-432-7587, 3) Visit a local DES office, or 4) Download and mail a paper application. You will need to provide information about household income, assets, and household members. Processing typically takes 45 days for regular applications, or 24 hours for emergency situations. Once approved, you'll receive an AHCCCS ID card in the mail.",
      "requiredFields": [
        "livesInArizona"
      ],
      "requiredDocuments": [
        {
          "id": "az-medicaid-application-documents",
          "name": "Application Documents Package",
          "description": "Comprehensive document list for Arizona Medicaid (AHCCCS) application",
          "required": true,
          "alternatives": [],
          "where": "Various sources"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Health-e-Arizona Plus (recommended - fastest processing)",
          "url": "https://www.healthearizonaplus.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call AHCCCS Member Services: 1-855-432-7587 for phone application",
          "priority": "medium"
        },
        {
          "step": "Find your local DES office: https://www.azahcccs.gov/Members/GetStarted/HowToApply.html",
          "url": "https://www.azahcccs.gov/Members/GetStarted/HowToApply.html",
          "priority": "medium"
        }
