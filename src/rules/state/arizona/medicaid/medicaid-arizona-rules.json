    {
      "id": "medicaid-az-pregnant-women",
      "programId": "medicaid-arizona",
      "name": "Arizona Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 160% FPL qualify for Medicaid in Arizona",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInArizona"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2023
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Arizona covers pregnant women through Medicaid if household income is at or below 160% of the federal poverty level. This means: 1 person = $2,023/month, 2 people = $2,735/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "isPregnant",
        "householdIncome",
        "householdSize",
        "livesInArizona"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-az",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-az-pregnant",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Health-e-Arizona Plus",
          "url": "https://www.healthearizonaplus.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call AHCCCS Member Services: 1-855-432-7587",
          "priority": "high"
        }
      ],
