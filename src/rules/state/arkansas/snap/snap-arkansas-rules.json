    {
      "id": "snap-ar-application-process",
      "programId": "snap-arkansas",
      "name": "Arkansas SNAP Application Process",
      "description": "Information about applying for SNAP in Arkansas",
      "ruleLogic": {
        "var": "livesInArkansas"
      },
      "ruleType": "conditional",
      "explanation": "To apply for SNAP in Arkansas: 1) Apply online at access.arkansas.gov (fastest), 2) Apply by phone at 1-855-372-1084, 3) Visit a local DHS office, or 4) Download and mail a paper application. You will have an interview (phone or in-person) within 30 days. Expedited service (within 7 days) is available for households with very low income or urgent need. Bring proof of identity, income, expenses, and household composition to your interview.",
      "requiredFields": [
        "livesInArkansas"
      ],
      "requiredDocuments": [
        {
          "id": "ar-application-documents",
          "name": "Application Documents Package",
          "description": "Comprehensive document list for Arkansas SNAP application",
          "required": true,
          "alternatives": [],
          "where": "Various sources"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at access.arkansas.gov (recommended - fastest processing)",
          "url": "https://access.arkansas.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Arkansas DHS Information Line: 1-855-372-1084 for phone application",
          "priority": "medium"
        },
        {
          "step": "Find your local DHS office: https://www.dhs.arkansas.gov/dco/Pages/default.aspx",
          "url": "https://www.dhs.arkansas.gov/dco/Pages/default.aspx",
          "priority": "medium"
        }
      ],
