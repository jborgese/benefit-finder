    {
      "id": "snap-ar-abawd-requirements",
      "programId": "snap-arkansas",
      "name": "Arkansas ABAWD Work Requirements",
      "description": "Arkansas implements federal ABAWD time limit and work requirements",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isABAWD"
            }
          },
          {
            "var": "meetsWorkRequirement"
          },
          {
            "var": "isExemptFromWork"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Arkansas implements federal ABAWD (Able-Bodied Adults Without Dependents) work requirements. If you're 18-49, able-bodied, and without dependents, you must work or participate in work activities at least 80 hours per month to continue receiving SNAP after 3 months in a 36-month period. Exemptions include: age under 18 or over 49, pregnant, caring for a child under 6, physically or mentally unable to work, receiving disability benefits, or meeting other exemptions. Check with your local DHS office for current exemption status.",
      "requiredFields": [
        "isABAWD",
        "meetsWorkRequirement",
        "isExemptFromWork"
      ],
      "requiredDocuments": [
        {
          "id": "proof-work-activity-ar",
          "name": "Proof of Work Activity",
          "description": "Pay stubs, employer letter, or documentation of work activities",
          "required": false,
          "alternatives": [
            "Timesheet",
            "Job training certificate",
            "Community service verification"
          ],
          "where": "Employer or program administrator"
        }
      ],
      "nextSteps": [
        {
          "step": "Contact your local DHS office to verify ABAWD status and exemptions",
          "priority": "high"
        },
        {
          "step": "If subject to work requirements, register with Arkansas Workforce Services for work activities",
          "url": "https://www.arknet.arkansas.gov/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Arkansas SNAP Work Requirements",
          "url": "https://humanservices.arkansas.gov/divisions-shared-services/county-operations/food-assistance",
          "date": "2024-10-01"
        },
        {
          "title": "USDA FNS ABAWD Requirements",
