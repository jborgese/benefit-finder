    {
      "id": "snap-ar-standard-deduction",
      "programId": "snap-arkansas",
      "name": "Arkansas SNAP Standard Deduction",
      "description": "Arkansas uses federal standard deductions for net income calculation",
      "ruleLogic": {
        "if": [
          {
            "<=": [
              {
                "var": "householdSize"
              },
              3
            ]
          },
          193,
          {
            "if": [
              {
                "==": [
                  {
                    "var": "householdSize"
                  },
                  4
                ]
              },
              195,
              {
                "if": [
                  {
                    "==": [
                      {
                        "var": "householdSize"
                      },
                      5
                    ]
                  },
                  228,
                  266
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "benefit_amount",
      "explanation": "Arkansas follows federal standard deduction amounts when calculating net income: Households of 1-3 people: $193/month, 4 people: $195/month, 5 people: $228/month, 6+ people: $266/month. This deduction is automatically applied to your gross income to calculate net income for eligibility determination.",
      "requiredFields": [
        "householdSize"
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "SNAP Deductions",
          "url": "https://www.fns.usda.gov/snap/recipient/eligibility",
          "legalReference": "7 CFR ยง 273.9",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 15,
      "testCases": [
        {
          "id": "test-ar-deduction-small",
          "description": "Standard deduction for household of 2",
          "input": {
