  "rules": [
    {
      "id": "snap-ar-standard-eligibility",
      "programId": "snap-arkansas",
      "name": "Arkansas SNAP Standard Eligibility",
      "description": "Arkansas follows federal SNAP eligibility guidelines with gross income at or below 130% of Federal Poverty Level",
      "ruleLogic": {
        "and": [
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  1643
                ]
              }
            ]
          },
          {
            "==": [
              {
                "var": "has_eligible_citizenship"
              },
              true
            ]
          },
          {
            "==": [
              {
                "var": "state"
              },
              "AR"
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "To qualify for SNAP in Arkansas, your household gross monthly income must be at or below 130% of the Federal Poverty Level for your household size ($1,643/month for 1 person in 2024), and you must be a US citizen or eligible non-citizen. Arkansas uses standard federal income limits and asset tests.",
      "requiredFields": [
        "householdIncome",
        "householdSize",
        "has_eligible_citizenship",
        "state"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ar",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Bank statements",
            "Self-employment records"
          ],
          "where": "Employer or your own records"
        },
        {
          "id": "proof-identity-ar",
          "name": "Proof of Identity",
          "description": "Valid photo ID for all household members applying",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "Birth certificate"
          ],
          "where": "DMV or vital records office"
        },
        {
          "id": "proof-residency-ar",
          "name": "Proof of Arkansas Residency",
          "description": "Document showing you live in Arkansas",
          "required": true,
          "alternatives": [
            "Utility bill",
            "Lease agreement",
            "Rent receipt",
            "Mortgage statement"
          ],
          "where": "Utility company, landlord, or mortgage company"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at access.arkansas.gov or visit your local DHS office",
          "url": "https://access.arkansas.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Arkansas DHS Information Line: 1-855-372-1084",
          "priority": "medium"
        },
        {
          "step": "Find your local DHS office: https://www.dhs.arkansas.gov/dco/Pages/default.aspx",
          "url": "https://www.dhs.arkansas.gov/dco/Pages/default.aspx",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
