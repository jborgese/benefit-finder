          },
          "expected": 195,
          "tags": [
            "deduction",
            "medium-household"
          ]
        },
        {
          "id": "test-ar-deduction-large",
          "description": "Standard deduction for household of 6+",
          "input": {
            "householdSize": 7
          },
          "expected": 266,
          "tags": [
            "deduction",
            "large-household"
          ]
        }
      ],
      "createdAt": 1728777600000,
