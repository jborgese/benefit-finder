  "rules": [
    {
      "id": "medicaid-ar-expansion-adults",
      "programId": "medicaid-arkansas",
      "name": "Arkansas Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Arkansas's ACA expansion (ARHOME)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInArkansas"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  1746
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Arkansas has expanded Medicaid under the Affordable Care Act through the ARHOME program (Arkansas Health and Opportunity for Me). Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage. This means: 1 person = $1,746/month, 2 people = $2,359/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, and preventive services.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInArkansas"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ar-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ar-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport"
          ],
          "where": "DMV or vital records office"
        },
        {
          "id": "proof-citizenship-ar",
          "name": "Proof of Citizenship or Legal Status",
          "description": "Birth certificate, passport, or immigration documents",
          "required": true,
          "alternatives": [
            "Naturalization certificate",
            "Permanent resident card"
          ],
          "where": "Vital records office or immigration services"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at access.arkansas.gov",
          "url": "https://access.arkansas.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Arkansas DHS Information Line: 1-855-372-1084",
          "priority": "high"
        },
        {
          "step": "Visit your local DHS office for in-person application",
          "url": "https://humanservices.arkansas.gov/divisions-shared-services/county-operations/medicaid",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
