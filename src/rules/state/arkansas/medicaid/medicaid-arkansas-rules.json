    {
      "id": "medicaid-ar-pregnant-women",
      "programId": "medicaid-arkansas",
      "name": "Arkansas Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 214% FPL qualify for Medicaid in Arkansas",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInArkansas"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2705
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Arkansas covers pregnant women through Medicaid if household income is at or below 214% of the federal poverty level. This means: 1 person = $2,705/month, 2 people = $3,658/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "isPregnant",
        "householdIncome",
        "householdSize",
        "livesInArkansas"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ar",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-ar-pregnant",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at access.arkansas.gov",
          "url": "https://access.arkansas.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Arkansas DHS Information Line: 1-855-372-1084",
          "priority": "high"
        }
      ],
