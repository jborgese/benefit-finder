    {
      "id": "medicaid-ar-application-process",
      "programId": "medicaid-arkansas",
      "name": "Arkansas Medicaid Application Process",
      "description": "Information about applying for Medicaid in Arkansas",
      "ruleLogic": {
        "var": "livesInArkansas"
      },
      "ruleType": "conditional",
      "explanation": "To apply for Medicaid (ARHOME/ARKids First) in Arkansas: 1) Apply online at access.arkansas.gov (fastest), 2) Apply by phone at 1-855-372-1084, 3) Visit a local DHS office, or 4) Download and mail a paper application. You will need to provide information about household income, assets, and household members. Processing typically takes 45 days for regular applications, or 24 hours for emergency situations. Once approved, you'll receive an Arkansas Medicaid ID card in the mail.",
      "requiredFields": [
        "livesInArkansas"
      ],
      "requiredDocuments": [
        {
          "id": "ar-medicaid-application-documents",
          "name": "Application Documents Package",
          "description": "Comprehensive document list for Arkansas Medicaid application",
          "required": true,
          "alternatives": [],
          "where": "Various sources"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at access.arkansas.gov (recommended - fastest processing)",
          "url": "https://access.arkansas.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Arkansas DHS Information Line: 1-855-372-1084 for phone application",
          "priority": "medium"
        },
        {
          "step": "Find your local DHS office: https://www.dhs.arkansas.gov/dco/Pages/default.aspx",
          "url": "https://www.dhs.arkansas.gov/dco/Pages/default.aspx",
          "priority": "medium"
        }
