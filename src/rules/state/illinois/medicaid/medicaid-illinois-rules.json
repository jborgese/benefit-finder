  "rules": [
    {
      "id": "medicaid-il-expansion-adults",
      "programId": "medicaid-illinois",
      "name": "Illinois Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Illinois's ACA expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIllinois"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Illinois has expanded Medicaid under the Affordable Care Act. Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage through HealthChoice Illinois. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, mental health services, and preventive care. Illinois has NO asset test for most adults.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInIllinois"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-il-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-il-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "School ID with photo"
          ],
          "where": "DMV or issuing agency"
        },
        {
          "id": "proof-residence-il",
          "name": "Proof of Illinois Residence",
          "description": "Mail, utility bill, lease, or employer letter showing Illinois address",
          "required": true,
          "alternatives": [
            "Illinois driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through ABE (recommended - fastest method)",
          "url": "https://abe.illinois.gov/abe/access/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Illinois Department of Healthcare and Family Services: 1-800-843-6154",
          "priority": "high"
        },
        {
          "step": "Visit your local Family Community Resource Center for in-person application",
          "url": "https://www.dhs.state.il.us/page.aspx?module=12&officetype=1&county=",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
