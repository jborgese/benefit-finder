    {
      "id": "medicaid-il-pregnant-women",
      "programId": "medicaid-illinois",
      "name": "Illinois Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 213% FPL qualify for Medicaid in Illinois",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIllinois"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3147
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Illinois covers pregnant women through Medicaid if household income is at or below 213% of the federal poverty level. For 2024, this means: 1 person = $3,147/month, 2 people = $4,261/month, 3 people = $5,375/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInIllinois",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-il",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-il",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through ABE",
          "url": "https://abe.illinois.gov/abe/access/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Illinois offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
