    {
      "id": "medicaid-il-children",
      "programId": "medicaid-illinois",
      "name": "Illinois Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 318% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIllinois"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4701
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Illinois covers children under age 19 through Medicaid if household income is at or below 318% of the federal poverty level. For 2024, this means: 1 child = $4,701/month, 2 children = $6,365/month, 3 children = $8,029/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInIllinois",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-il",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-il",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through ABE",
          "url": "https://abe.illinois.gov/abe/access/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
