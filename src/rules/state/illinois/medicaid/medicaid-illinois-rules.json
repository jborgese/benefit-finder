    {
      "id": "medicaid-il-application-process",
      "programId": "medicaid-illinois",
      "name": "Illinois Medicaid Application Process",
      "description": "Information about applying for Medicaid (HealthChoice Illinois) in Illinois",
      "ruleLogic": {
        "var": "livesInIllinois"
      },
      "ruleType": "conditional",
      "explanation": "Illinois uses the ABE (Application for Benefits Eligibility) online portal for all Medicaid applications. You can apply: 1) **Online through ABE** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-843-6154, 3) **In-person** at your local Family Community Resource Center. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Illinois offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInIllinois"
      ],
      "nextSteps": [
        {
          "step": "Apply online through ABE (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://abe.illinois.gov/abe/access/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Illinois Department of Healthcare and Family Services: 1-800-843-6154",
          "priority": "high"
        },
        {
          "step": "Find your local Family Community Resource Center",
          "url": "https://www.dhs.state.il.us/page.aspx?module=12&officetype=1&county=",
          "priority": "medium"
        }
      ],
