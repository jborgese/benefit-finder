    {
      "id": "medicaid-oh-children",
      "programId": "medicaid-ohio",
      "name": "Ohio Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 211% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInOhio"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3120
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Ohio covers children under age 19 through Medicaid if household income is at or below 211% of the federal poverty level. For 2024, this means: 1 child = $3,120/month, 2 children = $4,227/month, 3 children = $5,334/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInOhio",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-oh",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-oh",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Ohio Benefits",
          "url": "https://benefits.ohio.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
