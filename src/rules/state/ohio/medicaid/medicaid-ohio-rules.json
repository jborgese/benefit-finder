  "metadata": {
    "id": "medicaid-ohio-rules-2024",
    "name": "Ohio Medicaid Eligibility Rules (2024)",
    "description": "Ohio-specific Medicaid eligibility rules for 2024. Ohio expanded Medicaid under the ACA in 2014, so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
