    {
      "id": "medicaid-oh-application-process",
      "programId": "medicaid-ohio",
      "name": "Ohio Medicaid Application Process",
      "description": "Information about applying for Medicaid in Ohio",
      "ruleLogic": {
        "var": "livesInOhio"
      },
      "ruleType": "conditional",
      "explanation": "Ohio uses Ohio Benefits for Medicaid applications. You can apply: 1) **Online through Ohio Benefits** (recommended - fastest, available 24/7), 2) **By phone** at 1-844-640-6446, 3) **In-person** at your local County Department of Job and Family Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Ohio offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInOhio"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Ohio Benefits (recommended)",
          "url": "https://benefits.ohio.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Ohio Medicaid: 1-844-640-6446",
          "priority": "high"
        },
        {
          "step": "Find your local County Department of Job and Family Services office",
          "url": "https://jfs.ohio.gov/county/",
          "priority": "medium"
        }
      ],
