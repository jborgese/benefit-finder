    {
      "id": "medicaid-fl-application-process",
      "programId": "medicaid-florida",
      "name": "Florida Medicaid Application Process",
      "description": "Information about applying for Medicaid in Florida",
      "ruleLogic": {
        "var": "livesInFlorida"
      },
      "ruleType": "conditional",
      "explanation": "Florida uses the ACCESS Florida system for all Medicaid applications. You can apply: 1) **Online through ACCESS Florida** (recommended - fastest, available 24/7), 2) **By phone** at 1-866-762-2237, 3) **In-person** at your local Department of Children and Families office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Florida offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInFlorida"
      ],
      "nextSteps": [
        {
          "step": "Apply online through ACCESS Florida (recommended - handles both Medicaid and other benefits)",
          "url": "https://www.myflfamilies.com/service-programs/access",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call ACCESS Florida: 1-866-762-2237",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Children and Families office",
          "url": "https://www.myflfamilies.com/service-programs/access/community-partners",
          "priority": "medium"
        }
      ],
