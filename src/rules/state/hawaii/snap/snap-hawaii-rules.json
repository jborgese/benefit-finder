  "rules": [
    {
      "id": "snap-hi-standard-eligibility",
      "programId": "snap-hawaii",
      "name": "Hawaii SNAP Standard Eligibility",
      "description": "Standard SNAP eligibility based on household income and size",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInHawaii"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  1643
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "To qualify for SNAP in Hawaii, your household gross monthly income must be at or below 130% of the Federal Poverty Level. For 2024: 1 person = $1,643/month, 2 people = $2,226/month, 3 people = $2,808/month, 4 people = $3,390/month. Hawaii follows federal SNAP income guidelines. Note: While Hawaii has a high cost of living, SNAP uses the same federal poverty level as the continental United States. After applying the standard deduction and other deductions, your net income must be at or below 100% FPL to qualify.",
      "requiredFields": [
        "householdIncome",
        "householdSize",
        "livesInHawaii"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-hi",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Bank statements",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-hi",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "School ID with photo"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Hawaii Department of Human Services Benefits Portal",
          "url": "https://paisbenefits.dhs.hawaii.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Alternative: Apply by phone at 1-877-643-1643 or visit your local Department of Human Services office",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
