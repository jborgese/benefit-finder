    {
      "id": "snap-hi-student-eligibility",
      "programId": "snap-hawaii",
      "name": "Hawaii SNAP Student Eligibility",
      "description": "College students age 18-49 enrolled at least half-time have additional eligibility requirements",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isStudent"
            }
          },
          {
            "<": [
              {
                "var": "age"
              },
              18
            ]
          },
          {
            ">": [
              {
                "var": "age"
              },
              49
            ]
          },
          {
            "var": "worksAtLeast20Hours"
          },
          {
            "var": "hasChildUnder6"
          },
          {
            "var": "hasChildUnder12AndNoChildcare"
          },
          {
            "var": "receivesWorkStudy"
          },
          {
            "var": "inEmploymentTraining"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Hawaii follows federal rules for college students. If you're 18-49 and enrolled at least half-time in college, you must also: work at least 20 hours/week, participate in work-study, care for a child under 6, care for a child 6-12 with inadequate childcare, or participate in an employment training program. High school students, students under 18, and students over 49 are exempt from these additional requirements.",
      "requiredFields": [
        "isStudent",
        "age",
        "worksAtLeast20Hours",
        "hasChildUnder6",
        "hasChildUnder12AndNoChildcare",
        "receivesWorkStudy",
        "inEmploymentTraining"
      ],
      "requiredDocuments": [
        {
          "id": "proof-student-status-hi",
          "name": "Proof of Student Status",
          "description": "School enrollment verification, class schedule",
          "required": false,
          "alternatives": [
            "Student ID",
            "Enrollment letter",
            "Class schedule"
          ],
          "where": "College registrar"
        }
      ],
      "nextSteps": [
        {
          "step": "If you're a student, check if you meet one of the exemptions (working 20+ hours, work-study, childcare responsibilities, etc.)",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
