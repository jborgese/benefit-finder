    {
      "id": "snap-hi-application-process",
      "programId": "snap-hawaii",
      "name": "Hawaii SNAP Application Process",
      "description": "Information about applying for SNAP in Hawaii through the Department of Human Services",
      "ruleLogic": {
        "var": "livesInHawaii"
      },
      "ruleType": "conditional",
      "explanation": "Hawaii uses the PAIS (Public Assistance Information System) Benefits Portal for benefit applications. You can apply: 1) **Online through the PAIS Benefits Portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-877-643-1643, 3) **In-person** at your local Department of Human Services office. Most applications are processed within 30 days. Expedited service (within 7 days) is available for households with urgent need and less than $150 in monthly gross income and liquid resources under $100, or if your rent/mortgage and utilities are more than your income and liquid resources.",
      "requiredFields": [
        "livesInHawaii"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the PAIS Benefits Portal (recommended - fastest method)",
          "url": "https://paisbenefits.dhs.hawaii.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply by phone: 1-877-643-1643",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Human Services office",
          "url": "https://humanservices.hawaii.gov/ssd/contact/office-locations/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
