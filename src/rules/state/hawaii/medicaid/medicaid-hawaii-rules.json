    {
      "id": "medicaid-hi-children",
      "programId": "medicaid-hawaii",
      "name": "Hawaii Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 313% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInHawaii"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4628
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Hawaii covers children under age 19 through Med-QUEST if household income is at or below 313% of the federal poverty level. For 2024, this means: 1 child = $4,628/month, 2 children = $6,264/month, 3 children = $7,900/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInHawaii",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-hi",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-hi",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the PAIS Benefits Portal",
          "url": "https://paisbenefits.dhs.hawaii.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
