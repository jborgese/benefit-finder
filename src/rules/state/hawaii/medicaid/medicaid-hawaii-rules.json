  "rules": [
    {
      "id": "medicaid-hi-expansion-adults",
      "programId": "medicaid-hawaii",
      "name": "Hawaii Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Hawaii's ACA expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInHawaii"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Hawaii has expanded Medicaid under the Affordable Care Act. Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage through Med-QUEST. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, mental health services, and preventive care. Hawaii has NO asset test for most adults.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInHawaii"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-hi-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-hi-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "School ID with photo"
          ],
          "where": "DMV or issuing agency"
        },
        {
          "id": "proof-residence-hi",
          "name": "Proof of Hawaii Residence",
          "description": "Mail, utility bill, lease, or employer letter showing Hawaii address",
          "required": true,
          "alternatives": [
            "Hawaii driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the PAIS Benefits Portal (recommended - fastest method)",
          "url": "https://paisbenefits.dhs.hawaii.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Med-QUEST Division: 1-800-316-8005",
          "priority": "high"
        },
        {
          "step": "Visit your local Department of Human Services office for in-person application",
          "url": "https://humanservices.hawaii.gov/ssd/contact/office-locations/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
