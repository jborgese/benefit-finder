    {
      "id": "medicaid-hi-application-process",
      "programId": "medicaid-hawaii",
      "name": "Hawaii Medicaid Application Process",
      "description": "Information about applying for Medicaid (Med-QUEST) in Hawaii",
      "ruleLogic": {
        "var": "livesInHawaii"
      },
      "ruleType": "conditional",
      "explanation": "Hawaii uses the PAIS (Public Assistance Information System) Benefits Portal for all Medicaid applications. You can apply: 1) **Online through the PAIS Benefits Portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-316-8005 (Med-QUEST Division), 3) **In-person** at your local Department of Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Hawaii offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInHawaii"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the PAIS Benefits Portal (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://paisbenefits.dhs.hawaii.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Med-QUEST Division: 1-800-316-8005",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Human Services office",
          "url": "https://humanservices.hawaii.gov/ssd/contact/office-locations/",
          "priority": "medium"
        }
      ],
