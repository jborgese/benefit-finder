    {
      "id": "snap-ak-application-process",
      "programId": "snap-alaska",
      "name": "Alaska SNAP Application Process",
      "description": "Information about applying for SNAP in Alaska",
      "ruleLogic": {
        "var": "livesInAlaska"
      },
      "ruleType": "conditional",
      "explanation": "To apply for SNAP in Alaska: 1) Apply online at myAlaska.gov (fastest), 2) Apply by phone at 1-800-478-7778, 3) Visit a local Public Assistance office, or 4) Download and mail a paper application. You will have an interview (phone or in-person) within 30 days. Expedited service (within 7 days) is available for households with very low income or urgent need. Bring proof of identity, income, expenses, and household composition to your interview.",
      "requiredFields": [
        "livesInAlaska"
      ],
      "requiredDocuments": [
        {
          "id": "ak-application-documents",
          "name": "Application Documents Package",
          "description": "Comprehensive document list for Alaska SNAP application",
          "required": true,
          "alternatives": [],
          "where": "Various sources"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at myAlaska.gov (recommended - fastest processing)",
          "url": "https://myalaska.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Alaska Public Assistance Information Line: 1-800-478-7778 for phone application",
          "priority": "medium"
        },
        {
          "step": "Find your local Public Assistance office: https://dhss.alaska.gov/dpa/Pages/contact/default.aspx",
          "url": "https://dhss.alaska.gov/dpa/Pages/contact/default.aspx",
          "priority": "medium"
        }
