    {
      "id": "snap-ak-abawd-requirements",
      "programId": "snap-alaska",
      "name": "Alaska ABAWD Work Requirements",
      "description": "Alaska implements federal ABAWD time limit and work requirements with some rural area exemptions",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isABAWD"
            }
          },
          {
            "var": "meetsWorkRequirement"
          },
          {
            "var": "isExemptFromWork"
          },
          {
            "var": "livesInRuralArea"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Alaska implements federal ABAWD (Able-Bodied Adults Without Dependents) work requirements. If you're 18-49, able-bodied, and without dependents, you must work or participate in work activities at least 80 hours per month to continue receiving SNAP after 3 months in a 36-month period. Some rural areas in Alaska may have exemptions from work requirements. Exemptions include: age under 18 or over 49, pregnant, caring for a child under 6, physically or mentally unable to work, receiving disability benefits, or meeting other exemptions. Check with your local Public Assistance office for current exemption status.",
      "requiredFields": [
        "isABAWD",
        "meetsWorkRequirement",
        "isExemptFromWork",
        "livesInRuralArea"
      ],
      "requiredDocuments": [
        {
          "id": "proof-work-activity-ak",
          "name": "Proof of Work Activity",
          "description": "Pay stubs, employer letter, or documentation of work activities",
          "required": false,
          "alternatives": [
            "Timesheet",
            "Job training certificate",
            "Community service verification"
          ],
          "where": "Employer or program administrator"
        }
      ],
      "nextSteps": [
        {
          "step": "Contact your local Public Assistance office to verify ABAWD status and exemptions",
          "priority": "high"
        },
        {
          "step": "If subject to work requirements, register with Alaska Job Center for work activities",
          "url": "https://www.jobs.state.ak.us/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Alaska SNAP Work Requirements",
          "url": "https://dhss.alaska.gov/dpa/Pages/fssd/food-assistance/default.aspx",
          "date": "2024-10-01"
        },
        {
          "title": "USDA FNS ABAWD Requirements",
