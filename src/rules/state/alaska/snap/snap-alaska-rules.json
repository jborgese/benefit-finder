  "metadata": {
    "id": "snap-alaska-rules-2024",
    "name": "Alaska SNAP Eligibility Rules (2024)",
    "description": "Alaska-specific SNAP (Supplemental Nutrition Assistance Program) eligibility rules for 2024. These rules supplement federal SNAP rules and reflect Alaska's implementation of the program through the Alaska Department of Health. Note: Alaska uses higher poverty guidelines due to the higher cost of living.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-AK",
    "programs": [
      "snap-alaska"
    ],
    "tags": [
      "snap",
      "food-assistance",
      "alaska",
      "2024",
      "state-specific"
    ],
    "createdAt": 1728777600000,
    "updatedAt": 1728777600000
  },
  "rules": [
    {
      "id": "snap-ak-standard-eligibility",
      "programId": "snap-alaska",
      "name": "Alaska SNAP Standard Eligibility",
      "description": "Alaska follows federal SNAP eligibility guidelines with gross income at or below 130% of Federal Poverty Level (Alaska uses higher poverty guidelines)",
      "ruleLogic": {
        "and": [
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2050
                ]
              }
            ]
          },
          {
            "==": [
              {
                "var": "has_eligible_citizenship"
              },
              true
            ]
          },
          {
            "==": [
              {
                "var": "state"
              },
              "AK"
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "To qualify for SNAP in Alaska, your household gross monthly income must be at or below 130% of the Federal Poverty Level for your household size. Alaska uses higher poverty guidelines due to the higher cost of living ($2,050/month for 1 person in 2024, compared to $1,643 in most states). You must be a US citizen or eligible non-citizen. Alaska uses standard federal asset tests.",
      "requiredFields": [
        "householdIncome",
        "householdSize",
        "has_eligible_citizenship",
        "state"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ak",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Bank statements",
            "Self-employment records"
          ],
          "where": "Employer or your own records"
        },
        {
          "id": "proof-identity-ak",
          "name": "Proof of Identity",
          "description": "Valid photo ID for all household members applying",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "Birth certificate"
          ],
          "where": "DMV or vital records office"
        },
        {
          "id": "proof-residency-ak",
          "name": "Proof of Alaska Residency",
          "description": "Document showing you live in Alaska",
          "required": true,
          "alternatives": [
            "Utility bill",
            "Lease agreement",
            "Rent receipt",
            "Mortgage statement"
          ],
          "where": "Utility company, landlord, or mortgage company"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at myAlaska.gov or visit your local Public Assistance office",
          "url": "https://myalaska.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Alaska Public Assistance Information Line: 1-800-478-7778",
          "priority": "medium"
        },
        {
          "step": "Find your local Public Assistance office: https://dhss.alaska.gov/dpa/Pages/contact/default.aspx",
          "url": "https://dhss.alaska.gov/dpa/Pages/contact/default.aspx",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
