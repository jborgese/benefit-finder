    {
      "id": "snap-ak-student-eligibility",
      "programId": "snap-alaska",
      "name": "Alaska SNAP Student Eligibility",
      "description": "College students age 18-49 enrolled at least half-time have additional eligibility requirements",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isStudent"
            }
          },
          {
            "<": [
              {
                "var": "age"
              },
              18
            ]
          },
          {
            ">": [
              {
                "var": "age"
              },
              49
            ]
          },
          {
            "var": "worksAtLeast20Hours"
          },
          {
            "var": "hasChildUnder6"
          },
          {
            "var": "hasChildUnder12AndNoChildcare"
          },
          {
            "var": "receivesWorkStudy"
          },
          {
            "var": "inEmploymentTraining"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Alaska follows federal rules for college students. If you're 18-49 and enrolled at least half-time in college, you must also: work at least 20 hours/week, participate in work-study, care for a child under 6, care for a child 6-12 with inadequate childcare, receive TANF, or participate in an employment training program. High school students, students under 18, and students over 49 are exempt from these additional requirements.",
      "requiredFields": [
        "isStudent",
        "age",
        "worksAtLeast20Hours",
        "hasChildUnder6",
        "hasChildUnder12AndNoChildcare",
        "receivesWorkStudy",
        "inEmploymentTraining"
      ],
      "requiredDocuments": [
        {
          "id": "proof-student-status-ak",
          "name": "Proof of Student Status",
          "description": "School enrollment verification",
          "required": false,
          "alternatives": [
            "Class schedule",
            "Student ID",
            "Enrollment letter"
          ],
          "where": "College registrar or student services"
        },
        {
          "id": "proof-student-work-ak",
          "name": "Proof of Work or Work-Study",
          "description": "Pay stubs or work-study award letter",
          "required": false,
          "alternatives": [
            "Employer letter",
            "Work-study documentation"
          ],
          "where": "Employer or financial aid office"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
