    {
      "id": "medicaid-ak-application-process",
      "programId": "medicaid-alaska",
      "name": "Alaska Medicaid Application Process",
      "description": "Information about applying for Medicaid in Alaska",
      "ruleLogic": {
        "var": "livesInAlaska"
      },
      "ruleType": "conditional",
      "explanation": "To apply for Medicaid in Alaska: 1) Apply online at myAlaska.gov (fastest), 2) Apply by phone at 1-800-478-7778, 3) Visit a local Public Assistance office, or 4) Download and mail a paper application. You will need to provide information about household income, assets, and household members. Processing typically takes 30-45 days for regular applications, or 24 hours for emergency situations.",
