    {
      "id": "medicaid-ak-pregnant-women",
      "programId": "medicaid-alaska",
      "name": "Alaska Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 200% FPL qualify for Medicaid in Alaska (Alaska uses higher poverty guidelines)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInAlaska"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3016
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Alaska covers pregnant women through Medicaid if household income is at or below 200% of the federal poverty level. Due to Alaska's higher poverty guidelines, this means: 1 person = $3,016/month, 2 people = $4,080/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "isPregnant",
        "householdIncome",
        "householdSize",
        "livesInAlaska"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ak",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-ak-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at myAlaska.gov",
          "url": "https://myalaska.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Alaska Public Assistance Information Line: 1-800-478-7778",
          "priority": "high"
        },
        {
          "step": "Visit your local Public Assistance office for in-person application",
          "url": "https://dhss.alaska.gov/dpa/Pages/contact/default.aspx",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
