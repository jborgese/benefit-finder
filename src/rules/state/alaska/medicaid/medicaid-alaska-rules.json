  "metadata": {
    "id": "medicaid-alaska-rules-2024",
    "name": "Alaska Medicaid Eligibility Rules (2024)",
    "description": "Alaska-specific Medicaid eligibility rules for 2024. Alaska has NOT expanded Medicaid under the ACA, so adult eligibility is more restrictive than expansion states. These rules reflect Alaska's Medicaid program. Note: Alaska uses higher poverty guidelines due to the higher cost of living.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-AK",
    "programs": [
      "medicaid-alaska"
    ],
    "tags": [
      "medicaid",
      "healthcare",
      "alaska",
      "2024",
      "state-specific"
    ],
    "createdAt": 1728777600000,
    "updatedAt": 1728777600000
  },
  "rules": [
    {
      "id": "medicaid-ak-no-expansion",
      "programId": "medicaid-alaska",
      "name": "Alaska Medicaid - No ACA Expansion Coverage Gap",
      "description": "Identifies adults in Alaska's Medicaid coverage gap who are NOT eligible",
      "ruleLogic": {
        "if": [
          {
            "and": [
              {
                "var": "livesInAlaska"
              },
              {
                ">=": [
                  {
                    "var": "age"
                  },
                  19
                ]
              },
              {
                "<": [
                  {
                    "var": "age"
                  },
                  65
                ]
              },
              {
                "!": {
                  "var": "isPregnant"
                }
              },
              {
                "!": {
                  "var": "hasQualifyingDisability"
                }
              },
              {
                "!": {
                  "var": "hasChildren"
                }
              }
            ]
          },
          false,
          true
        ]
      },
      "ruleType": "eligibility",
      "explanation": "IMPORTANT: Alaska has NOT expanded Medicaid. This means most non-disabled, non-pregnant adults age 19-64 without children do NOT qualify for Medicaid in Alaska, even with very low income. The coverage gap affects approximately 20,000 Alaskans. If you fall into this gap, explore options at HealthCare.gov for marketplace plans with subsidies, or contact a Community Health Center for care on a sliding fee scale.",
      "requiredFields": [
        "age",
        "isPregnant",
        "hasQualifyingDisability",
        "hasChildren",
        "livesInAlaska"
      ],
      "nextSteps": [
        {
          "step": "If you're in the coverage gap, check HealthCare.gov for marketplace plan subsidies",
          "url": "https://www.healthcare.gov",
          "priority": "high"
        },
        {
          "step": "Find a Community Health Center that offers sliding fee scale: https://findahealthcenter.hrsa.gov",
          "url": "https://findahealthcenter.hrsa.gov",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Alaska Medicaid Expansion Status",
          "url": "https://www.kff.org/medicaid/issue-brief/status-of-state-medicaid-expansion-decisions-interactive-map/",
          "date": "2024-10-01",
          "notes": "Alaska has not expanded Medicaid as of 2024"
        },
        {
          "title": "Alaska Medicaid Eligibility",
          "url": "https://dhss.alaska.gov/dpa/Pages/medicaid/default.aspx",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-ak-coverage-gap",
          "description": "Non-disabled adult in coverage gap - NOT eligible",
          "input": {
            "age": 35,
            "isPregnant": false,
            "hasQualifyingDisability": false,
            "hasChildren": false,
            "livesInAlaska": true
          },
          "expected": false,
          "tags": [
            "ineligible",
            "coverage-gap",
            "non-expansion"
          ]
        },
        {
          "id": "test-ak-pregnant",
          "description": "Pregnant woman - eligible under pregnancy rules (not this rule)",
          "input": {
            "age": 30,
            "isPregnant": true,
            "hasQualifyingDisability": false,
            "hasChildren": false,
            "livesInAlaska": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "use-pregnancy-rules"
          ]
        },
        {
          "id": "test-ak-disabled",
          "description": "Disabled adult - eligible under disability rules (not this rule)",
          "input": {
            "age": 40,
            "isPregnant": false,
            "hasQualifyingDisability": true,
            "hasChildren": false,
            "livesInAlaska": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "use-disability-rules"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
