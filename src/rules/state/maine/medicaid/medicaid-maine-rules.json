    {
      "id": "medicaid-me-application-process",
      "programId": "medicaid-maine",
      "name": "Maine Medicaid Application Process",
      "description": "Information about applying for Medicaid in Maine",
      "ruleLogic": {
        "var": "livesInMaine"
      },
      "ruleType": "conditional",
      "explanation": "Maine uses My Maine Connection for all Medicaid applications. You can apply: 1) **Online through My Maine Connection** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-797-4357, 3) **In-person** at your local Maine DHHS office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Maine offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInMaine"
      ],
      "nextSteps": [
        {
          "step": "Apply online through My Maine Connection (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://www1.maine.gov/benefits/account/login.html",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Maine DHHS: 1-855-797-4357",
          "priority": "high"
        },
        {
          "step": "Find your local Maine DHHS office",
          "url": "https://www.maine.gov/dhhs/ofi/about-us/office-locations",
          "priority": "medium"
        }
      ],
