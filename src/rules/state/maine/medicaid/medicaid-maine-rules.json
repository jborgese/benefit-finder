    {
      "id": "medicaid-me-children",
      "programId": "medicaid-maine",
      "name": "Maine Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 200% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMaine"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2956
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Maine covers children under age 19 through Medicaid if household income is at or below 200% of the federal poverty level. For 2024, this means: 1 child = $2,956/month, 2 children = $4,004/month, 3 children = $5,052/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInMaine",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-me",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-me",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through My Maine Connection",
          "url": "https://www1.maine.gov/benefits/account/login.html",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
