    {
      "id": "medicaid-me-pregnant-women",
      "programId": "medicaid-maine",
      "name": "Maine Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 209% FPL qualify for Medicaid in Maine",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMaine"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3089
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Maine covers pregnant women through Medicaid if household income is at or below 209% of the federal poverty level. For 2024, this means: 1 person = $3,089/month, 2 people = $4,183/month, 3 people = $5,277/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInMaine",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-me",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-me",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through My Maine Connection",
          "url": "https://www1.maine.gov/benefits/account/login.html",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Maine offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
