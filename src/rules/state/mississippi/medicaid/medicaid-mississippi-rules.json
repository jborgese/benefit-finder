    {
      "id": "medicaid-ms-application-process",
      "programId": "medicaid-mississippi",
      "name": "Mississippi Medicaid Application Process",
      "description": "Information about applying for Medicaid in Mississippi",
      "ruleLogic": {
        "var": "livesInMississippi"
      },
      "ruleType": "conditional",
      "explanation": "Mississippi uses the Mississippi Department of Human Services (MDHS) for Medicaid applications. You can apply: 1) **Online** (if available), 2) **By phone**, 3) **In-person** at your local MDHS office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs, especially for pregnant women and children.",
      "requiredFields": [
        "livesInMississippi"
      ],
      "nextSteps": [
        {
          "step": "Apply through your local MDHS office or online",
          "url": "https://www.mdhs.ms.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find your local MDHS office",
          "url": "https://www.mdhs.ms.gov/field-offices/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
