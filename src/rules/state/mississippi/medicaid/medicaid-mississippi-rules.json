    {
      "id": "medicaid-ms-children",
      "programId": "medicaid-mississippi",
      "name": "Mississippi Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 209% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMississippi"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3087
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Mississippi covers children under age 19 through Medicaid if household income is at or below 209% of the federal poverty level. For 2024, this means: 1 child = $3,087/month, 2 children = $4,180/month, 3 children = $5,273/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInMississippi",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ms",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ms",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply through your local MDHS office or online",
          "url": "https://www.mdhs.ms.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
