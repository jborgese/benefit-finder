  "rules": [
    {
      "id": "medicaid-ms-coverage-gap",
      "programId": "medicaid-mississippi",
      "name": "Mississippi Medicaid Coverage Gap",
      "description": "Mississippi has not expanded Medicaid, creating a coverage gap for adults without dependents",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMississippi"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "hasDisability"
            }
          },
          {
            "!": {
              "var": "isPregnant"
            }
          },
          {
            ">": [
              {
                "var": "householdIncome"
              },
              0
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Mississippi has NOT expanded Medicaid under the Affordable Care Act. This creates a coverage gap for adults age 19-64 who don't have dependents, aren't pregnant, and don't have a disability. If you're in this situation and have income below 138% FPL ($2,040/month for 1 person), you may not be eligible for Medicaid or subsidized marketplace insurance. You may qualify for: marketplace coverage without subsidies, or other programs if you meet specific criteria (disability, pregnancy, etc.).",
      "requiredFields": [
        "livesInMississippi",
        "age",
        "hasDisability",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "nextSteps": [
        {
          "step": "Check if you qualify for other Medicaid categories (pregnancy, disability, children)",
          "priority": "high"
        },
        {
          "step": "Explore marketplace health insurance options (may not have subsidies due to income level)",
          "priority": "medium"
        },
        {
          "step": "Contact your local MDHS office to discuss all available options",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Mississippi Medicaid Expansion Status",
          "url": "https://www.kff.org/medicaid/state-indicator/medicaid-expansion/",
          "date": "2024-10-01",
          "notes": "Mississippi has not expanded Medicaid under the ACA"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 20,
      "testCases": [
        {
          "id": "test-ms-gap-adult",
          "description": "Adult in coverage gap",
          "input": {
            "livesInMississippi": true,
            "age": 35,
            "hasDisability": false,
            "isPregnant": false,
            "householdIncome": 1800,
            "householdSize": 1
          },
          "expected": false,
          "tags": [
            "ineligible",
            "coverage-gap"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "coverage-gap",
        "non-expansion",
        "adults"
      ],
      "category": "eligibility-limitation",
      "jurisdiction": "US-MS"
    },
    {
      "id": "medicaid-ms-pregnant-women",
      "programId": "medicaid-mississippi",
      "name": "Mississippi Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 194% FPL qualify for Medicaid in Mississippi",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMississippi"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2867
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Mississippi covers pregnant women through Medicaid if household income is at or below 194% of the federal poverty level. For 2024, this means: 1 person = $2,867/month, 2 people = $3,882/month, 3 people = $4,897/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInMississippi",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ms",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ms",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Contact your local MDHS office or apply online",
          "url": "https://www.mdhs.ms.gov/",
          "priority": "high"
        },
        {
          "step": "Mississippi offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Mississippi Medicaid for Pregnant Women",
          "url": "https://www.mdhs.ms.gov/medicaid/",
          "date": "2024-10-01",
