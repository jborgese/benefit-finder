    {
      "id": "medicaid-wa-application-process",
      "programId": "medicaid-washington",
      "name": "Washington Medicaid Application Process",
      "description": "Information about applying for Medicaid (Apple Health) in Washington",
      "ruleLogic": {
        "var": "livesInWashington"
      },
      "ruleType": "conditional",
      "explanation": "Washington uses Washington Healthplanfinder for Medicaid applications. You can apply: 1) **Online through Washington Healthplanfinder** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-923-4633, 3) **In-person** at your local Community Services Office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Washington offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInWashington"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Washington Healthplanfinder (recommended)",
          "url": "https://www.wahealthplanfinder.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Washington Health Benefit Exchange: 1-855-923-4633",
          "priority": "high"
        },
        {
          "step": "Find your local Community Services Office",
          "url": "https://www.dshs.wa.gov/esa/community-services-offices",
          "priority": "medium"
        }
      ],
