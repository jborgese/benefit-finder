    {
      "id": "medicaid-wa-pregnant-women",
      "programId": "medicaid-washington",
      "name": "Washington Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 302% FPL qualify for Medicaid in Washington",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWashington"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4471
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Washington covers pregnant women through Medicaid (Apple Health) if household income is at or below 302% of the federal poverty level. For 2024, this means: 1 person = $4,471/month, 2 people = $6,054/month, 3 people = $7,637/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInWashington",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-wa",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-wa",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through Washington Healthplanfinder",
          "url": "https://www.wahealthplanfinder.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Washington offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
