  "metadata": {
    "id": "medicaid-washington-rules-2024",
    "name": "Washington Medicaid Eligibility Rules (2024)",
    "description": "Washington-specific Medicaid eligibility rules for 2024. Washington expanded Medicaid under the ACA, so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
