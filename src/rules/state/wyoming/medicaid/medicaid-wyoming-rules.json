    {
      "id": "medicaid-wy-children",
      "programId": "medicaid-wyoming",
      "name": "Wyoming Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 133% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWyoming"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  1967
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Wyoming covers children under age 19 through Medicaid if household income is at or below 133% of the federal poverty level. For 2024, this means: 1 child = $1,967/month, 2 children = $2,664/month, 3 children = $3,361/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInWyoming",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-wy",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-wy",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply through Wyoming Department of Health",
          "url": "https://health.wyo.gov/healthcarefin/equalitycare/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
