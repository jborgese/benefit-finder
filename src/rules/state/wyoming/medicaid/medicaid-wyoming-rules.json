    {
      "id": "medicaid-wy-application-process",
      "programId": "medicaid-wyoming",
      "name": "Wyoming Medicaid Application Process",
      "description": "Information about applying for Medicaid (EqualityCare) in Wyoming",
      "ruleLogic": {
        "var": "livesInWyoming"
      },
      "ruleType": "conditional",
      "explanation": "Wyoming uses the Department of Health for Medicaid (EqualityCare) applications. You can apply: 1) **Online through the Wyoming Department of Health portal**, 2) **By phone** at 1-800-246-4221, 3) **In-person** at your local Department of Health office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Wyoming offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInWyoming"
      ],
      "nextSteps": [
        {
          "step": "Apply through Wyoming Department of Health (recommended)",
          "url": "https://health.wyo.gov/healthcarefin/equalitycare/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Wyoming Department of Health: 1-800-246-4221",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Health office",
          "url": "https://health.wyo.gov/healthcarefin/equalitycare/",
          "priority": "medium"
        }
      ],
