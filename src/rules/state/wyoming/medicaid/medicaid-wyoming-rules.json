  },
  "rules": [
    {
      "id": "medicaid-wy-coverage-gap-adults",
      "programId": "medicaid-wyoming",
      "name": "Wyoming Medicaid Coverage Gap for Adults",
      "description": "Wyoming did NOT expand Medicaid - adults 19-64 without dependents or disabilities face a coverage gap",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWyoming"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "isPregnant"
            }
          },
          {
            "!": {
              "var": "hasDependentChildren"
            }
          },
          {
            "!": {
              "var": "hasDisability"
            }
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Wyoming did NOT expand Medicaid under the Affordable Care Act. This means adults age 19-64 who are not pregnant, don't have dependent children, and don't have disabilities typically face a coverage gap - they are NOT eligible for Medicaid regardless of income level. If you're in this situation and your income is below 100% FPL, you may not be eligible for Medicaid OR Marketplace subsidies. You may need to explore other coverage options or wait for Wyoming to expand Medicaid.",
      "requiredFields": [
        "livesInWyoming",
        "age",
        "isPregnant",
        "hasDependentChildren",
        "hasDisability"
      ],
      "requiredDocuments": [],
      "nextSteps": [
        {
          "step": "If you're in the coverage gap, consider contacting Wyoming Department of Health to discuss your options",
          "priority": "medium"
        },
        {
          "step": "Check if you qualify for other assistance programs or charity care",
          "priority": "medium"
        },
        {
          "step": "Monitor for potential Medicaid expansion in Wyoming",
          "priority": "low"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Wyoming Medicaid Non-Expansion Status",
          "url": "https://health.wyo.gov/healthcarefin/equalitycare/",
          "date": "2024-10-01",
          "notes": "Wyoming did not expand Medicaid under the ACA, creating a coverage gap for many adults"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-wy-coverage-gap-adult",
          "description": "Adult in coverage gap (non-expansion state)",
          "input": {
            "livesInWyoming": true,
            "age": 35,
            "isPregnant": false,
            "hasDependentChildren": false,
            "hasDisability": false
          },
          "expected": false,
          "tags": [
            "ineligible",
            "coverage-gap",
            "non-expansion"
          ]
        },
        {
          "id": "test-wy-pregnant-not-gap",
          "description": "Pregnant woman not in coverage gap",
          "input": {
            "livesInWyoming": true,
            "age": 28,
            "isPregnant": true,
            "hasDependentChildren": false,
            "hasDisability": false
          },
          "expected": false,
          "tags": [
            "not-gap",
            "pregnant-women-rule-applies"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "coverage-gap",
        "non-expansion",
        "adults"
      ],
      "category": "eligibility-requirement",
      "jurisdiction": "US-WY"
    },
    {
      "id": "medicaid-wy-pregnant-women",
      "programId": "medicaid-wyoming",
      "name": "Wyoming Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 154% FPL qualify for Medicaid in Wyoming",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInWyoming"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2279
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Wyoming covers pregnant women through Medicaid if household income is at or below 154% of the federal poverty level. For 2024, this means: 1 person = $2,279/month, 2 people = $3,087/month, 3 people = $3,895/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInWyoming",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-wy",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-wy",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply through Wyoming Department of Health",
          "url": "https://health.wyo.gov/healthcarefin/equalitycare/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Wyoming offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
