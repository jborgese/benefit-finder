    {
      "id": "medi-cal-ca-children-immigrant",
      "programId": "medi-cal-children",
      "name": "Medi-Cal for Immigrant Children",
      "description": "California provides Medi-Cal to immigrant children regardless of immigration status",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3938
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California provides Medi-Cal to ALL children under age 19, regardless of immigration status, if household income is at or below 266% of the federal poverty level. This includes undocumented children. For 2024 at 266% FPL: 1 child = $3,938/month, 2 children = $5,333/month, 3 children = $6,728/month. This is unique to California and provides coverage to many immigrant children who would otherwise be ineligible.",
      "requiredFields": [
        "livesInCalifornia",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-immigrant",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-immigrant",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Covered California or BenefitsCal.com",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Contact an immigration-friendly community health center for assistance",
          "priority": "high"
        },
        {
          "step": "Children can enroll year-round regardless of immigration status",
          "priority": "high"
        }
