  "rules": [
    {
      "id": "medi-cal-ca-children-266-percent",
      "programId": "medi-cal-children",
      "name": "Medi-Cal for Children (266% FPL)",
      "description": "Children under 19 qualify for Medi-Cal with income up to 266% FPL (higher than federal minimum)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3938
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California covers children under age 19 in families with incomes up to 266% of the federal poverty level through Medi-Cal. This is higher than the federal minimum (200% FPL). For 2024 at 266% FPL: 1 child = $3,938/month, 2 children = $5,333/month, 3 children = $6,728/month. Children can enroll year-round, not just during open enrollment periods. No asset test for children.",
      "requiredFields": [
        "livesInCalifornia",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ca",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ca",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Covered California or BenefitsCal.com",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        },
        {
          "step": "Contact your local county social services office for assistance",
          "priority": "medium"
        }
      ],
      "version": {
