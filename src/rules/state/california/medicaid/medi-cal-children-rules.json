    {
      "id": "medi-cal-ca-children-year-round",
      "programId": "medi-cal-children",
      "name": "Medi-Cal Children Year-Round Enrollment",
      "description": "Children can enroll in Medi-Cal year-round, not just during open enrollment",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          }
        ]
      },
      "ruleType": "conditional",
      "explanation": "Children can enroll in Medi-Cal year-round, not just during open enrollment periods. This means families can apply for coverage for their children at any time of the year. Coverage typically begins the first day of the month after application is approved. This is different from marketplace plans for adults, which generally require enrollment during open enrollment periods.",
      "requiredFields": [
        "livesInCalifornia",
        "age"
      ],
      "nextSteps": [
        {
          "step": "Children can apply for Medi-Cal at any time - no waiting for open enrollment",
          "priority": "high"
        },
        {
          "step": "Apply online at Covered California or BenefitsCal.com",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        }
      ],
