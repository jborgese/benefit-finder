    {
      "id": "medi-cal-ca-emergency-coverage",
      "programId": "medi-cal-immigrants",
      "name": "Emergency Medi-Cal for All Immigrants",
      "description": "Emergency Medi-Cal covers emergency medical conditions for all immigrants regardless of status",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "var": "hasEmergencyMedicalCondition"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Emergency Medi-Cal covers emergency medical conditions for ALL immigrants in California, regardless of immigration status. This includes undocumented immigrants. Emergency conditions include: labor and delivery, emergency room visits, emergency surgery, and other life-threatening conditions. This is a federal requirement that all states must provide, but California has streamlined the process.",
      "requiredFields": [
        "livesInCalifornia",
        "hasEmergencyMedicalCondition"
      ],
      "requiredDocuments": [
        {
          "id": "proof-emergency-condition",
          "name": "Proof of Emergency Medical Condition",
          "description": "Doctor's note, hospital records, or medical documentation",
          "required": true,
          "alternatives": [
            "Emergency room records",
            "Hospital admission records",
            "Medical provider statement"
          ],
          "where": "Healthcare provider or hospital"
        }
      ],
      "nextSteps": [
        {
          "step": "Emergency Medi-Cal can be applied for at the hospital or emergency room",
          "priority": "high"
        },
        {
          "step": "Contact your local county social services office for assistance",
          "priority": "high"
        }
      ],
