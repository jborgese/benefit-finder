  "rules": [
    {
      "id": "medi-cal-ca-daca-coverage",
      "programId": "medi-cal-daca",
      "name": "Medi-Cal for DACA Recipients",
      "description": "California provides state-funded Medi-Cal to DACA recipients who are ineligible for federal Medicaid",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "==": [
              {
                "var": "immigration_status"
              },
              "daca"
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California provides state-funded Medi-Cal to DACA recipients with household income up to 138% of the federal poverty level. This is unique to California - DACA recipients are not eligible for federal Medicaid in most states. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month. California has NO asset test for most adults, including DACA recipients.",
      "requiredFields": [
        "livesInCalifornia",
        "immigration_status",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-daca-status",
          "name": "Proof of DACA Status",
          "description": "DACA approval notice or Employment Authorization Document (I-766)",
          "required": true,
          "alternatives": [
            "DACA renewal notice",
            "I-94 with DACA status"
          ],
          "where": "USCIS"
        },
        {
          "id": "proof-income-daca",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Covered California (recommended)",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Alternative: Apply at BenefitsCal.com",
          "url": "https://www.benefitscal.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Contact an immigration-friendly community health center for assistance",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
