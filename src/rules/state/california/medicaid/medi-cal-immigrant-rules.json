    {
      "id": "medi-cal-ca-lawfully-present",
      "programId": "medi-cal-lawfully-present",
      "name": "Medi-Cal for Lawfully Present Immigrants",
      "description": "California provides state-funded Medi-Cal to lawfully present immigrants regardless of 5-year waiting period",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "==": [
              {
                "var": "immigration_status"
              },
              "lawfully_present"
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California provides state-funded Medi-Cal to lawfully present immigrants with household income up to 138% of the federal poverty level, regardless of how long they've been in the U.S. This bypasses the federal 5-year waiting period for most immigrants. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month. This is unique to California and provides coverage to many immigrants who would otherwise be ineligible.",
      "requiredFields": [
        "livesInCalifornia",
        "immigration_status",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-lawful-presence",
          "name": "Proof of Lawful Presence",
          "description": "Employment Authorization Document, I-94, or other immigration document",
          "required": true,
          "alternatives": [
            "I-766 Employment Authorization",
            "I-94 Arrival/Departure Record",
            "I-94W Visa Waiver"
          ],
          "where": "USCIS or CBP"
        },
        {
          "id": "proof-income-lawful",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Covered California (recommended)",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Alternative: Apply at BenefitsCal.com",
          "url": "https://www.benefitscal.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Contact an immigration-friendly community health center for assistance",
          "priority": "high"
        }
      ],
