    {
      "id": "medi-cal-ca-immigrant-expansion",
      "programId": "medi-cal-immigrants",
      "name": "Medi-Cal Immigrant Coverage Expansion",
      "description": "California provides state-funded Medi-Cal to immigrants ineligible for federal Medicaid",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "or": [
              {
                "in": [
                  {
                    "var": "immigration_status"
                  },
                  [
                    "us_citizen",
                    "permanent_resident",
                    "refugee",
                    "asylee",
                    "trafficking_victim"
                  ]
                ]
              },
              {
                "and": [
                  {
                    "in": [
                      {
                        "var": "immigration_status"
                      },
                      [
                        "daca",
                        "lawfully_present",
                        "other_qualified"
                      ]
                    ]
                  },
                  {
                    "var": "inCalifornia"
                  }
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California expanded Medi-Cal to immigrants beyond federal Medicaid eligibility using state funds. This includes: U.S. citizens and green card holders (federal eligibility), PLUS state-funded coverage for: DACA recipients, lawfully present immigrants (regardless of 5-year wait), and certain other immigrants who would otherwise be ineligible for federal Medicaid. This is unique to California and provides coverage to hundreds of thousands of immigrants.",
      "requiredFields": [
        "livesInCalifornia",
        "immigration_status",
        "inCalifornia"
      ],
      "requiredDocuments": [
        {
          "id": "proof-immigration-status-ca",
          "name": "Proof of Immigration Status",
          "description": "Immigration documents showing status",
          "required": true,
          "alternatives": [
            "Green Card (I-551)",
            "Employment Authorization (I-766)",
            "I-94 Arrival/Departure Record",
            "Refugee documentation",
            "DACA approval notice"
          ],
          "where": "USCIS"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply even if you're not sure about eligibility - California has expanded rules for immigrants",
          "priority": "high"
        },
        {
          "step": "Contact an immigration-friendly community health center for assistance",
          "priority": "high"
        },
        {
          "step": "Apply online at Covered California or BenefitsCal.com",
          "url": "https://www.coveredca.com/",
          "priority": "high"
        }
      ],
