    {
      "id": "medi-cal-ca-disability",
      "programId": "medi-cal-california",
      "name": "Medi-Cal for People with Disabilities",
      "description": "People with disabilities may qualify for Medi-Cal with income up to 138% FPL or through SSI",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "var": "hasQualifyingDisability"
          },
          {
            "or": [
              {
                "var": "receivesSSI"
              },
              {
                "<=": [
                  {
                    "var": "householdIncome"
                  },
                  {
                    "*": [
                      {
                        "var": "householdSize"
                      },
                      2040
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "People with disabilities qualify for Medi-Cal if they: 1) Receive SSI (automatic eligibility), OR 2) Have income below 138% FPL ($2,040/month for individual in 2024). California has no asset test for most disabled adults. Many people with disabilities also qualify for Medicare and can have both Medicare and Medi-Cal (dual eligibility).",
      "requiredFields": [
        "livesInCalifornia",
        "hasQualifyingDisability",
        "receivesSSI",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-ssi-ca",
          "name": "Proof of SSI (if applicable)",
          "description": "SSI award letter or benefit verification",
          "required": false,
          "alternatives": [
            "Social Security benefit statement",
            "Bank deposits"
          ],
          "where": "Social Security Administration"
        },
        {
          "id": "proof-disability-ca",
          "name": "Proof of Disability",
          "description": "Medical records, SSA disability determination, or doctor's certification",
          "required": true,
          "alternatives": [
            "Medicare card",
            "Doctor's certification",
            "Functional assessment"
          ],
          "where": "SSA or healthcare provider"
        }
      ],
      "nextSteps": [
        {
          "step": "If you receive SSI, you should be automatically enrolled in Medi-Cal",
          "priority": "high"
        },
        {
          "step": "Apply online at Covered California or BenefitsCal.com",
          "url": "https://www.coveredca.com/",
          "priority": "high"
        }
      ],
