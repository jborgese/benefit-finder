  "rules": [
    {
      "id": "medi-cal-ca-adult-expansion",
      "programId": "medi-cal-california",
      "name": "Medi-Cal Adult Expansion (138% FPL)",
      "description": "California expanded Medi-Cal covers adults under 65 with income up to 138% FPL with no asset test",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "isPregnant"
            }
          },
          {
            "!": {
              "var": "hasQualifyingDisability"
            }
          },
          {
            "!": {
              "var": "hasChildren"
            }
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California expanded Medi-Cal covers adults under age 65 with household income up to 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. California has NO asset test for most adults, unlike some other states. This covers the 'coverage gap' that exists in non-expansion states.",
      "requiredFields": [
        "livesInCalifornia",
        "age",
        "isPregnant",
        "hasQualifyingDisability",
        "hasChildren",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-medi-cal",
          "name": "Proof of Income",
          "description": "Recent pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2 forms",
            "1099 forms",
            "Bank statements",
            "Self-employment records"
          ],
          "where": "Employer, IRS, bank, or your own records"
        },
        {
          "id": "proof-identity-medi-cal",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Tribal ID",
            "School ID with photo"
          ],
          "where": "DMV, State Department, or issuing agency"
        },
        {
          "id": "proof-residence-ca",
          "name": "Proof of California Residence",
          "description": "Mail, utility bill, lease, or employer letter showing California address",
          "required": true,
          "alternatives": [
            "California driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Covered California (recommended - fastest)",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Alternative: Apply at BenefitsCal.com (state benefits portal)",
          "url": "https://www.benefitscal.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find local assistance at a county office or community organization",
          "url": "https://www.coveredca.com/help/",
          "priority": "medium"
        }
      ],
