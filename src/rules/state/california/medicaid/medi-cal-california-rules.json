    {
      "id": "medi-cal-ca-application-process",
      "programId": "medi-cal-california",
      "name": "Medi-Cal Application Process and Assistance",
      "description": "Information about applying for Medi-Cal in California with multiple application methods and language support",
      "ruleLogic": {
        "var": "livesInCalifornia"
      },
      "ruleType": "conditional",
      "explanation": "California offers multiple ways to apply for Medi-Cal: 1) **Covered California** (primary application portal, also handles marketplace plans), 2) **BenefitsCal.com** (state benefits portal), 3) **In-person assistance** at county offices or community organizations, 4) **Phone applications** with language support. California provides extensive language access including Spanish, Chinese, Vietnamese, and other languages. Most applications are processed within 45 days, with expedited processing for urgent medical needs.",
      "requiredFields": [
        "livesInCalifornia"
      ],
      "nextSteps": [
        {
          "step": "Apply online at Covered California (recommended - handles both Medi-Cal and marketplace plans)",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Alternative: Apply at BenefitsCal.com (state benefits portal)",
          "url": "https://www.benefitscal.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find local assistance at a county office or community organization",
          "url": "https://www.coveredca.com/help/",
          "priority": "medium"
        },
        {
          "step": "Call Covered California for phone application: 1-800-300-1506",
          "priority": "medium"
        }
      ],
