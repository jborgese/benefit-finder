  "rules": [
    {
      "id": "medi-cal-ca-pregnancy-322-percent",
      "programId": "medi-cal-pregnancy",
      "name": "Medi-Cal for Pregnant Women (322% FPL)",
      "description": "Pregnant women qualify for Medi-Cal with income up to 322% FPL (higher than federal minimum)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4768
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California covers pregnant women through Medi-Cal if household income is at or below 322% of the federal poverty level. This is much higher than the federal minimum (200% FPL). For 2024, this means: 1 person = $4,768/month, 2 people = $6,456/month, 3 people = $8,144/month. Coverage includes prenatal care, delivery, and postpartum care for 12 months after pregnancy ends (extended from federal 60 days).",
      "requiredFields": [
        "livesInCalifornia",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ca",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ca",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online at Covered California or BenefitsCal.com",
          "url": "https://www.coveredca.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "California offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
