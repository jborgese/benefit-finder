    {
      "id": "medi-cal-ca-presumptive-eligibility",
      "programId": "medi-cal-pregnancy",
      "name": "Medi-Cal Presumptive Eligibility for Pregnant Women",
      "description": "California provides immediate temporary coverage for pregnant women while application is processed",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInCalifornia"
          },
          {
            "var": "isPregnant"
          }
        ]
      },
      "ruleType": "conditional",
      "explanation": "California provides presumptive eligibility for pregnant women, which means immediate temporary coverage while the full Medi-Cal application is being processed. This ensures pregnant women can get care right away without waiting for application approval. Coverage typically lasts for 60 days or until the full application is processed, whichever comes first.",
      "requiredFields": [
        "livesInCalifornia",
        "isPregnant"
      ],
      "nextSteps": [
        {
          "step": "Ask about presumptive eligibility at your healthcare provider or clinic",
          "priority": "high"
        },
        {
          "step": "Complete the full Medi-Cal application to maintain coverage beyond 60 days",
          "priority": "high"
        }
      ],
