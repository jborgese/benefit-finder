    {
      "id": "medicaid-ks-children",
      "programId": "medicaid-kansas",
      "name": "Kansas Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 238% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInKansas"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3519
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Kansas covers children under age 19 through Medicaid if household income is at or below 238% of the federal poverty level. For 2024, this means: 1 child = $3,519/month, 2 children = $4,764/month, 3 children = $6,009/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInKansas",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ks",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ks",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Kansas Self-Service Portal",
          "url": "https://cssp.kees.ks.gov/apspssp/cnsld/mnu",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
