  "metadata": {
    "id": "medicaid-kansas-rules-2024",
    "name": "Kansas Medicaid Eligibility Rules (2024)",
    "description": "Kansas-specific Medicaid eligibility rules for 2024. Kansas has NOT expanded Medicaid under the ACA, so most adults without dependents are not eligible. Coverage is available for pregnant women, children, parents with dependent children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-KS",
    "programs": [
      "medicaid-kansas"
    ],
    "tags": [
      "medicaid",
      "healthcare",
