  },
  "rules": [
    {
      "id": "medicaid-ks-coverage-gap",
      "programId": "medicaid-kansas",
      "name": "Kansas Medicaid Coverage Gap",
      "description": "Most adults without dependents are not eligible for Medicaid in Kansas due to non-expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInKansas"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "hasDependentChildren"
            }
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Kansas has NOT expanded Medicaid under the Affordable Care Act. This means most adults age 19-64 without dependent children are not eligible for Medicaid, even if their income is below 138% of the Federal Poverty Level. If your income is between 100% and 138% FPL, you may be eligible for subsidized health insurance through the federal Health Insurance Marketplace instead. However, you may still be eligible for Medicaid if you: are pregnant, have dependent children, have a disability, or meet other categorical eligibility requirements.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "hasDependentChildren",
        "livesInKansas"
      ],
      "nextSteps": [
        {
          "step": "If you're not eligible for Medicaid, check if you qualify for subsidized health insurance through the Health Insurance Marketplace",
          "url": "https://www.healthcare.gov/",
          "priority": "high",
          "estimatedTime": "30-60 minutes"
        },
        {
          "step": "Contact the Kansas Department of Health and Environment or visit your local DCF office to discuss other options",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Kansas Medicaid Expansion Status",
          "url": "https://www.kdhe.ks.gov/1221/Kansas-Medicaid",
          "date": "2024-10-01",
          "notes": "Kansas has not expanded Medicaid under the ACA"
        },
        {
          "title": "Health Insurance Marketplace",
          "url": "https://www.healthcare.gov/",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
