    {
      "id": "medicaid-ks-application-process",
      "programId": "medicaid-kansas",
      "name": "Kansas Medicaid Application Process",
      "description": "Information about applying for Medicaid in Kansas",
      "ruleLogic": {
        "var": "livesInKansas"
      },
      "ruleType": "conditional",
      "explanation": "Kansas uses the Kansas Self-Service Portal for all Medicaid applications. You can apply: 1) **Online through the Kansas Self-Service Portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-888-369-4777, 3) **In-person** at your local Kansas DCF office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Kansas offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInKansas"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Kansas Self-Service Portal (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://cssp.kees.ks.gov/apspssp/cnsld/mnu",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Kansas DCF: 1-888-369-4777",
          "priority": "high"
        },
        {
          "step": "Find your local Kansas DCF office",
          "url": "https://www.dcf.ks.gov/services/Pages/DCFOfficeLocator.aspx",
          "priority": "medium"
        }
      ],
