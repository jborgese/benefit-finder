    {
      "id": "medicaid-ri-application-process",
      "programId": "medicaid-rhode-island",
      "name": "Rhode Island Medicaid Application Process",
      "description": "Information about applying for Medicaid (Rhode Island Medical Assistance) in Rhode Island",
      "ruleLogic": {
        "var": "livesInRhodeIsland"
      },
      "ruleType": "conditional",
      "explanation": "Rhode Island uses DHS Connect or HealthSource RI for Medical Assistance (Medicaid) applications. You can apply: 1) **Online through DHS Connect** (recommended - fastest, available 24/7), 2) **Online through HealthSource RI** (health insurance marketplace), 3) **By phone** at (401) 462-5300, 4) **In-person** at your local Department of Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Rhode Island offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInRhodeIsland"
      ],
      "nextSteps": [
        {
          "step": "Apply online through DHS Connect (recommended)",
          "url": "https://dhsconnect.ri.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through HealthSource RI",
          "url": "https://healthsourceri.com/",
          "priority": "high"
        },
        {
          "step": "Call Rhode Island Department of Human Services: (401) 462-5300",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Human Services office",
          "url": "https://dhs.ri.gov/contact-us",
          "priority": "medium"
        }
      ],
