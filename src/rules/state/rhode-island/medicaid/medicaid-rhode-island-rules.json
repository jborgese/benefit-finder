  "rules": [
    {
      "id": "medicaid-ri-expansion-adults",
      "programId": "medicaid-rhode-island",
      "name": "Rhode Island Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInRhodeIsland"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Rhode Island expanded Medicaid under the Affordable Care Act in 2014. Adults age 19-64 are eligible for Rhode Island Medical Assistance (Medicaid) if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through DHS Connect or HealthSource RI.",
      "requiredFields": [
        "livesInRhodeIsland",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ri-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ri-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through DHS Connect (recommended)",
          "url": "https://dhsconnect.ri.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through HealthSource RI",
          "url": "https://healthsourceri.com/",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Human Services office",
          "url": "https://dhs.ri.gov/contact-us",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
