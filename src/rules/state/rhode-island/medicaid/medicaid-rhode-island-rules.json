    {
      "id": "medicaid-ri-pregnant-women",
      "programId": "medicaid-rhode-island",
      "name": "Rhode Island Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 253% FPL qualify for Medicaid in Rhode Island",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInRhodeIsland"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3743
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Rhode Island covers pregnant women through Rhode Island Medical Assistance (Medicaid) if household income is at or below 253% of the federal poverty level. For 2024, this means: 1 person = $3,743/month, 2 people = $5,068/month, 3 people = $6,393/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInRhodeIsland",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ri",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ri",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through DHS Connect",
          "url": "https://dhsconnect.ri.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Rhode Island offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
