      "programId": "medicaid-rhode-island",
      "name": "Rhode Island Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 261% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInRhodeIsland"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3859
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Rhode Island covers children under age 19 through Rhode Island Medical Assistance (Medicaid) if household income is at or below 261% of the federal poverty level. For 2024, this means: 1 child = $3,859/month, 2 children = $5,226/month, 3 children = $6,593/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInRhodeIsland",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ri",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ri",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through DHS Connect",
          "url": "https://dhsconnect.ri.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
