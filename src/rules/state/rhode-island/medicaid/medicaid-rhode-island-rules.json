  "metadata": {
    "id": "medicaid-rhode-island-rules-2024",
    "name": "Rhode Island Medicaid Eligibility Rules (2024)",
    "description": "Rhode Island-specific Medicaid eligibility rules for 2024. Rhode Island expanded Medicaid under the ACA in 2014, so adults up to 138% FPL are eligible through Rhode Island Medical Assistance. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
