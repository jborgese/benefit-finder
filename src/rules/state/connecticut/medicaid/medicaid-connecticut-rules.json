      "programId": "medicaid-connecticut",
      "name": "Connecticut Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 185% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInConnecticut"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2738
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Connecticut covers children under age 19 through HUSKY Health if household income is at or below 185% of the federal poverty level. For 2024, this means: 1 child = $2,738/month, 2 children = $3,705/month, 3 children = $4,672/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInConnecticut",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ct",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ct",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the ConneCT system",
          "url": "https://www.ct.gov/dss/site/default.asp",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
