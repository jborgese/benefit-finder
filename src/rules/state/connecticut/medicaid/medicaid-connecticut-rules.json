    {
      "id": "medicaid-ct-pregnant-women",
      "programId": "medicaid-connecticut",
      "name": "Connecticut Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 200% FPL qualify for Medicaid in Connecticut",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInConnecticut"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2959
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Connecticut covers pregnant women through HUSKY Health if household income is at or below 200% of the federal poverty level. For 2024, this means: 1 person = $2,959/month, 2 people = $4,004/month, 3 people = $5,048/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInConnecticut",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ct",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ct",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through the ConneCT system",
          "url": "https://www.ct.gov/dss/site/default.asp",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Connecticut offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
