  "rules": [
    {
      "id": "medicaid-ct-expansion-adults",
      "programId": "medicaid-connecticut",
      "name": "Connecticut Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Connecticut's ACA expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInConnecticut"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Connecticut has expanded Medicaid under the Affordable Care Act. Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage through HUSKY Health. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, mental health services, and preventive care. Connecticut has NO asset test for most adults.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInConnecticut"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ct-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ct-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "School ID with photo"
          ],
          "where": "DMV or issuing agency"
        },
        {
          "id": "proof-residence-ct",
          "name": "Proof of Connecticut Residence",
          "description": "Mail, utility bill, lease, or employer letter showing Connecticut address",
          "required": true,
          "alternatives": [
            "Connecticut driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the ConneCT system (recommended - fastest method)",
          "url": "https://www.ct.gov/dss/site/default.asp",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call HUSKY Health Customer Service: 1-855-805-4325",
          "priority": "high"
        },
        {
          "step": "Visit your local Department of Social Services office for in-person application",
          "url": "https://portal.ct.gov/DSS/Local-Offices",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
