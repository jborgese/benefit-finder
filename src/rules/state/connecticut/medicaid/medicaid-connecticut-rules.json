    {
      "id": "medicaid-ct-application-process",
      "programId": "medicaid-connecticut",
      "name": "Connecticut Medicaid Application Process",
      "description": "Information about applying for Medicaid (HUSKY Health) in Connecticut",
      "ruleLogic": {
        "var": "livesInConnecticut"
      },
      "ruleType": "conditional",
      "explanation": "Connecticut uses the ConneCT system for all Medicaid applications. You can apply: 1) **Online through the ConneCT portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-805-4325 (HUSKY Health Customer Service), 3) **In-person** at your local Department of Social Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Connecticut offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInConnecticut"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the ConneCT system (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://www.ct.gov/dss/site/default.asp",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call HUSKY Health Customer Service: 1-855-805-4325",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Social Services office",
          "url": "https://portal.ct.gov/DSS/Local-Offices",
          "priority": "medium"
        }
      ],
