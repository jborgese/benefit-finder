  "metadata": {
    "id": "medicaid-vermont-rules-2024",
    "name": "Vermont Medicaid Eligibility Rules (2024)",
    "description": "Vermont-specific Medicaid eligibility rules for 2024. Vermont expanded Medicaid under the ACA, so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
