  },
  "rules": [
    {
      "id": "medicaid-vt-expansion-adults",
      "programId": "medicaid-vermont",
      "name": "Vermont Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInVermont"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Vermont expanded Medicaid under the Affordable Care Act. Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through myBenefitsVT.",
      "requiredFields": [
        "livesInVermont",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-vt-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-vt-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through myBenefitsVT (recommended)",
          "url": "https://www.mybenefits.vermont.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find your local Economic Services Division office",
          "url": "https://dcf.vermont.gov/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Vermont Medicaid Expansion",
          "url": "https://dvha.vermont.gov/",
          "date": "2024-10-01",
          "notes": "Vermont expanded Medicaid under the ACA, adults eligible up to 138% FPL"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-vt-expansion-eligible",
          "description": "Adult under 138% FPL",
          "input": {
            "livesInVermont": true,
            "age": 35,
            "householdIncome": 1900,
            "householdSize": 1
          },
          "expected": true,
          "tags": [
            "eligible",
            "expansion",
            "138-percent-fpl"
          ]
        },
        {
          "id": "test-vt-expansion-high-income",
          "description": "Adult over 138% FPL",
          "input": {
            "livesInVermont": true,
            "age": 35,
            "householdIncome": 2500,
            "householdSize": 1
          },
          "expected": false,
          "tags": [
            "ineligible",
            "high-income"
          ]
        },
        {
          "id": "test-vt-expansion-age-limit",
          "description": "Person under 19 not eligible under expansion rules",
          "input": {
