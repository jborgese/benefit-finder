    {
      "id": "medicaid-vt-application-process",
      "programId": "medicaid-vermont",
      "name": "Vermont Medicaid Application Process",
      "description": "Information about applying for Medicaid in Vermont",
      "ruleLogic": {
        "var": "livesInVermont"
      },
      "ruleType": "conditional",
      "explanation": "Vermont uses myBenefitsVT for Medicaid applications. You can apply: 1) **Online through myBenefitsVT** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-479-6151, 3) **In-person** at your local Economic Services Division office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Vermont offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInVermont"
      ],
      "nextSteps": [
        {
          "step": "Apply online through myBenefitsVT (recommended)",
          "url": "https://www.mybenefits.vermont.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Vermont Department for Children and Families: 1-800-479-6151",
          "priority": "high"
        },
        {
          "step": "Find your local Economic Services Division office",
          "url": "https://dcf.vermont.gov/",
          "priority": "medium"
        }
      ],
