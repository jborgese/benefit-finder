      "jurisdiction": "US-VT"
    },
    {
      "id": "medicaid-vt-pregnant-women",
      "programId": "medicaid-vermont",
      "name": "Vermont Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 208% FPL qualify for Medicaid in Vermont",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInVermont"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3078
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Vermont covers pregnant women through Medicaid if household income is at or below 208% of the federal poverty level. For 2024, this means: 1 person = $3,078/month, 2 people = $4,167/month, 3 people = $5,256/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInVermont",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-vt",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-vt",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through myBenefitsVT",
          "url": "https://www.mybenefits.vermont.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Vermont offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Vermont Medicaid for Pregnant Women",
