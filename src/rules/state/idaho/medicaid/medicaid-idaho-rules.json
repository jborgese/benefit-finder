    {
      "id": "medicaid-id-application-process",
      "programId": "medicaid-idaho",
      "name": "Idaho Medicaid Application Process",
      "description": "Information about applying for Medicaid in Idaho",
      "ruleLogic": {
        "var": "livesInIdaho"
      },
      "ruleType": "conditional",
      "explanation": "Idaho uses the Idalink online portal for all Medicaid applications. You can apply: 1) **Online through Idalink** (recommended - fastest, available 24/7), 2) **By phone** at 1-877-456-1233, 3) **In-person** at your local Health and Welfare office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Idaho offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInIdaho"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Idalink (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://www.idalink.idaho.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Idaho Health and Welfare: 1-877-456-1233",
          "priority": "high"
        },
        {
          "step": "Find your local Health and Welfare office",
          "url": "https://healthandwelfare.idaho.gov/about/regional-offices",
          "priority": "medium"
        }
      ],
