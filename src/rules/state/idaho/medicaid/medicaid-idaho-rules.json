    {
      "id": "medicaid-id-children",
      "programId": "medicaid-idaho",
      "name": "Idaho Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 142% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIdaho"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2101
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Idaho covers children under age 19 through Medicaid if household income is at or below 142% of the federal poverty level. For 2024, this means: 1 child = $2,101/month, 2 children = $2,846/month, 3 children = $3,591/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInIdaho",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-id",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-id",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Idalink",
          "url": "https://www.idalink.idaho.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
