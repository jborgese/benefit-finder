  "metadata": {
    "id": "medicaid-tennessee-rules-2024",
    "name": "Tennessee Medicaid Eligibility Rules (2024)",
    "description": "Tennessee-specific Medicaid eligibility rules for 2024. Tennessee has NOT expanded Medicaid under the ACA, so adults 19-64 without children or disabilities generally fall into a coverage gap. Coverage is available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-TN",
    "programs": [
      "medicaid-tennessee"
    ],
    "tags": [
      "medicaid",
      "healthcare",
      "tennessee",
      "2024",
      "state-specific",
      "non-expansion",
      "coverage-gap"
    ],
    "createdAt": 1728777600000,
    "updatedAt": 1728777600000
  },
  "rules": [
    {
      "id": "medicaid-tn-coverage-gap-adults",
      "programId": "medicaid-tennessee",
      "name": "Tennessee Medicaid Coverage Gap for Adults",
      "description": "Adults 19-64 without children or disabilities are generally not eligible for Medicaid (coverage gap)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTennessee"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "hasDisability"
            }
          },
          {
            "!": {
              "var": "isPregnant"
            }
          },
          {
            ">": [
              {
                "var": "householdIncome"
              },
              0
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Tennessee has NOT expanded Medicaid under the Affordable Care Act. This means adults age 19-64 who don't have children, aren't pregnant, and don't have a qualifying disability generally cannot get Medicaid coverage, even if their income is very low. This is called the 'coverage gap.' If you're in this situation, you may qualify for coverage through the Health Insurance Marketplace (HealthCare.gov) or may need to seek other healthcare options. Children, pregnant women, parents with dependent children, and people with disabilities may still qualify for Medicaid.",
      "requiredFields": [
        "livesInTennessee",
        "age",
        "hasDisability",
        "isPregnant",
        "householdIncome"
      ],
      "requiredDocuments": [],
      "nextSteps": [
        {
          "step": "Check if you qualify for coverage through the Health Insurance Marketplace at HealthCare.gov",
          "url": "https://www.healthcare.gov/",
          "priority": "high",
          "estimatedTime": "30-60 minutes"
        },
        {
          "step": "Contact your local Department of Human Services office to discuss other healthcare options",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Tennessee Medicaid Eligibility",
          "url": "https://www.tn.gov/tenncare.html",
          "date": "2024-10-01",
          "notes": "Tennessee has not expanded Medicaid under the ACA"
        },
        {
          "title": "HealthCare.gov - Coverage Gap",
          "url": "https://www.healthcare.gov/medicaid-chip/medicaid-expansion-and-you/",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-tn-coverage-gap-adult",
          "description": "Adult 19-64 without disability or pregnancy (coverage gap)",
          "input": {
            "livesInTennessee": true,
            "age": 35,
            "hasDisability": false,
            "isPregnant": false,
            "householdIncome": 1000
          },
          "expected": false,
          "tags": [
            "ineligible",
            "coverage-gap",
            "non-expansion"
          ]
        },
        {
          "id": "test-tn-adult-with-disability",
          "description": "Adult with disability may qualify (check disability rules)",
          "input": {
            "livesInTennessee": true,
            "age": 35,
            "hasDisability": true,
            "isPregnant": false,
            "householdIncome": 1000
          },
          "expected": false,
          "tags": [
            "check-disability-rules",
            "may-qualify"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "coverage-gap",
        "non-expansion",
        "adults"
      ],
      "category": "categorical-eligibility",
      "jurisdiction": "US-TN"
    },
    {
      "id": "medicaid-tn-pregnant-women",
      "programId": "medicaid-tennessee",
      "name": "Tennessee Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 195% FPL qualify for Medicaid in Tennessee",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTennessee"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2887
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Tennessee covers pregnant women through TennCare (Medicaid) if household income is at or below 195% of the federal poverty level. For 2024, this means: 1 person = $2,887/month, 2 people = $3,908/month, 3 people = $4,929/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInTennessee",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-tn",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-tn",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through OneAppTN",
          "url": "https://onedhs.tn.gov/csp/dhs/faces/requestservices",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Tennessee offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Tennessee Medicaid for Pregnant Women",
          "url": "https://www.tn.gov/tenncare.html",
          "date": "2024-10-01",
          "notes": "Tennessee covers pregnant women up to 195% FPL"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 25,
