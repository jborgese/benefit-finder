    {
      "id": "medicaid-tn-application-process",
      "programId": "medicaid-tennessee",
      "name": "Tennessee Medicaid Application Process",
      "description": "Information about applying for Medicaid (TennCare) in Tennessee",
      "ruleLogic": {
        "var": "livesInTennessee"
      },
      "ruleType": "conditional",
      "explanation": "Tennessee uses OneAppTN for Medicaid (TennCare) applications. You can apply: 1) **Online through OneAppTN** (recommended - fastest, available 24/7), 2) **By phone** at 1-866-311-4287, 3) **In-person** at your local Department of Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Tennessee offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInTennessee"
      ],
      "nextSteps": [
        {
          "step": "Apply online through OneAppTN (recommended)",
          "url": "https://onedhs.tn.gov/csp/dhs/faces/requestservices",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Tennessee Department of Human Services: 1-866-311-4287",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Human Services office",
          "url": "https://www.tn.gov/humanservices/for-families/regional-offices.html",
          "priority": "medium"
        }
      ],
