    {
      "id": "medicaid-tn-children",
      "programId": "medicaid-tennessee",
      "name": "Tennessee Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 218% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTennessee"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3218
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Tennessee covers children under age 19 through TennCare (Medicaid) if household income is at or below 218% of the federal poverty level. For 2024, this means: 1 child = $3,218/month, 2 children = $4,358/month, 3 children = $5,498/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInTennessee",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-tn",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-tn",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through OneAppTN",
          "url": "https://onedhs.tn.gov/csp/dhs/faces/requestservices",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
