      "programId": "medicaid-pennsylvania",
      "name": "Pennsylvania Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 317% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInPennsylvania"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4686
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Pennsylvania covers children under age 19 through Medical Assistance (Medicaid) if household income is at or below 317% of the federal poverty level. For 2024, this means: 1 child = $4,686/month, 2 children = $6,347/month, 3 children = $8,008/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInPennsylvania",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-pa",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-pa",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through COMPASS",
          "url": "https://www.compass.state.pa.us/compass.web/Public/CMPHome",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
