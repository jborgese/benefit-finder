    {
      "id": "medicaid-pa-pregnant-women",
      "programId": "medicaid-pennsylvania",
      "name": "Pennsylvania Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 215% FPL qualify for Medicaid in Pennsylvania",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInPennsylvania"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3180
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Pennsylvania covers pregnant women through Medical Assistance (Medicaid) if household income is at or below 215% of the federal poverty level. For 2024, this means: 1 person = $3,180/month, 2 people = $4,307/month, 3 people = $5,434/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInPennsylvania",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-pa",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-pa",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through COMPASS",
          "url": "https://www.compass.state.pa.us/compass.web/Public/CMPHome",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Pennsylvania offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
