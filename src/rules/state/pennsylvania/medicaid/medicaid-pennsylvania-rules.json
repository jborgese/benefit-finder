    {
      "id": "medicaid-pa-application-process",
      "programId": "medicaid-pennsylvania",
      "name": "Pennsylvania Medicaid Application Process",
      "description": "Information about applying for Medicaid (Medical Assistance) in Pennsylvania",
      "ruleLogic": {
        "var": "livesInPennsylvania"
      },
      "ruleType": "conditional",
      "explanation": "Pennsylvania uses COMPASS for Medical Assistance (Medicaid) applications. You can apply: 1) **Online through COMPASS** (recommended - fastest, available 24/7), 2) **By phone** at 1-866-550-4355, 3) **In-person** at your local County Assistance Office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Pennsylvania offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInPennsylvania"
      ],
      "nextSteps": [
        {
          "step": "Apply online through COMPASS (recommended)",
          "url": "https://www.compass.state.pa.us/compass.web/Public/CMPHome",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Pennsylvania Department of Human Services: 1-866-550-4355",
          "priority": "high"
        },
        {
          "step": "Find your local County Assistance Office",
          "url": "https://www.dhs.pa.gov/Services/Assistance/Pages/CAO-Contact.aspx",
          "priority": "medium"
        }
