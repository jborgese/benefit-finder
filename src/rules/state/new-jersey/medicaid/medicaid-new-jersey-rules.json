    {
      "id": "medicaid-nj-children",
      "programId": "medicaid-new-jersey",
      "name": "New Jersey Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 355% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNewJersey"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  5248
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "New Jersey covers children under age 19 through Medicaid if household income is at or below 355% of the federal poverty level. For 2024, this means: 1 child = $5,248/month, 2 children = $7,109/month, 3 children = $8,970/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInNewJersey",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-nj",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-nj",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through NJOneApp or GetCoveredNJ",
          "url": "https://njoneapp.dhs.state.nj.us/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
