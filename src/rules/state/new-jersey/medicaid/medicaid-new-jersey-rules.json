    {
      "id": "medicaid-nj-application-process",
      "programId": "medicaid-new-jersey",
      "name": "New Jersey Medicaid Application Process",
      "description": "Information about applying for Medicaid in New Jersey",
      "ruleLogic": {
        "var": "livesInNewJersey"
      },
      "ruleType": "conditional",
      "explanation": "New Jersey uses NJOneApp and GetCoveredNJ for Medicaid applications. You can apply: 1) **Online through NJOneApp** (recommended - fastest, available 24/7), 2) **Online through GetCoveredNJ** for health coverage, 3) **By phone** at 1-800-701-0710, 4) **In-person** at your local Board of Social Services. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. New Jersey offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInNewJersey"
      ],
      "nextSteps": [
        {
          "step": "Apply online through NJOneApp (recommended)",
          "url": "https://njoneapp.dhs.state.nj.us/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through GetCoveredNJ",
          "url": "https://www.getcovered.nj.gov/",
          "priority": "high"
        },
        {
          "step": "Call New Jersey Medicaid: 1-800-701-0710",
          "priority": "high"
        }
      ],
