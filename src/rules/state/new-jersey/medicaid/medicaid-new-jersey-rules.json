  "rules": [
    {
      "id": "medicaid-nj-expansion-adults",
      "programId": "medicaid-new-jersey",
      "name": "New Jersey Medicaid for ACA Expansion Adults (NJ FamilyCare)",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNewJersey"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "New Jersey expanded Medicaid under the Affordable Care Act in 2014 (NJ FamilyCare). Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through NJOneApp or GetCoveredNJ.",
      "requiredFields": [
        "livesInNewJersey",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-nj-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-nj-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through NJOneApp (recommended)",
          "url": "https://njoneapp.dhs.state.nj.us/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through GetCoveredNJ",
          "url": "https://www.getcovered.nj.gov/",
          "priority": "high"
        },
        {
          "step": "Find your local Board of Social Services",
          "url": "https://www.nj.gov/humanservices/dfd/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
