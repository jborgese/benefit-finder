    {
      "id": "medicaid-ok-pregnant-women",
      "programId": "medicaid-oklahoma",
      "name": "Oklahoma Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 185% FPL qualify for Medicaid in Oklahoma",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInOklahoma"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2736
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Oklahoma covers pregnant women through SoonerCare (Medicaid) if household income is at or below 185% of the federal poverty level. For 2024, this means: 1 person = $2,736/month, 2 people = $3,704/month, 3 people = $4,672/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInOklahoma",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ok",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ok",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through OKDHSLive",
          "url": "https://okdhslive.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Oklahoma offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
