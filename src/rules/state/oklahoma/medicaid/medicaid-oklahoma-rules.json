  "rules": [
    {
      "id": "medicaid-ok-expansion-adults",
      "programId": "medicaid-oklahoma",
      "name": "Oklahoma Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Medicaid expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInOklahoma"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Oklahoma expanded Medicaid under the Affordable Care Act in June 2021 through State Question 802. Adults age 19-64 are eligible for SoonerCare (Oklahoma Medicaid) if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care. You can apply through OKDHSLive.",
      "requiredFields": [
        "livesInOklahoma",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ok-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ok-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through OKDHSLive (recommended)",
          "url": "https://okdhslive.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Find your local Oklahoma Department of Human Services office",
          "url": "https://www.okdhs.org/about/Pages/locator.aspx",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
