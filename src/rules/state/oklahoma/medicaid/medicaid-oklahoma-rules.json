    {
      "id": "medicaid-ok-application-process",
      "programId": "medicaid-oklahoma",
      "name": "Oklahoma Medicaid Application Process",
      "description": "Information about applying for Medicaid (SoonerCare) in Oklahoma",
      "ruleLogic": {
        "var": "livesInOklahoma"
      },
      "ruleType": "conditional",
      "explanation": "Oklahoma uses OKDHSLive for SoonerCare (Medicaid) applications. You can apply: 1) **Online through OKDHSLive** (recommended - fastest, available 24/7), 2) **By phone** at 1-405-522-5050, 3) **In-person** at your local Oklahoma Department of Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Oklahoma offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInOklahoma"
      ],
      "nextSteps": [
        {
          "step": "Apply online through OKDHSLive (recommended)",
          "url": "https://okdhslive.org/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Oklahoma Department of Human Services: 1-405-522-5050",
          "priority": "high"
        },
        {
          "step": "Find your local Oklahoma Department of Human Services office",
          "url": "https://www.okdhs.org/about/Pages/locator.aspx",
          "priority": "medium"
        }
      ],
