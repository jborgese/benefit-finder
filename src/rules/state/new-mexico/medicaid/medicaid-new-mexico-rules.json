  "metadata": {
    "id": "medicaid-new-mexico-rules-2024",
    "name": "New Mexico Medicaid Eligibility Rules (2024)",
    "description": "New Mexico-specific Medicaid eligibility rules for 2024. New Mexico expanded Medicaid under the ACA in 2014 (Centennial Care), so adults up to 138% FPL are eligible. Coverage is also available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
