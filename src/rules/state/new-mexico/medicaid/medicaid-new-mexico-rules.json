    {
      "id": "medicaid-nm-application-process",
      "programId": "medicaid-new-mexico",
      "name": "New Mexico Medicaid Application Process",
      "description": "Information about applying for Medicaid in New Mexico",
      "ruleLogic": {
        "var": "livesInNewMexico"
      },
      "ruleType": "conditional",
      "explanation": "New Mexico uses YESNM and beWellnm for Medicaid applications. You can apply: 1) **Online through YESNM** (recommended - fastest, available 24/7), 2) **Online through beWellnm** for health coverage, 3) **By phone** at 1-888-997-2583, 4) **In-person** at your local Income Support Division office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. New Mexico offers language assistance for non-English speakers, including Spanish.",
      "requiredFields": [
        "livesInNewMexico"
      ],
      "nextSteps": [
        {
          "step": "Apply online through YESNM (recommended)",
          "url": "https://www.yes.state.nm.us/yesnm/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Or apply through beWellnm",
          "url": "https://www.bewellnm.com/",
          "priority": "high"
        },
        {
          "step": "Call New Mexico Medicaid: 1-888-997-2583",
          "priority": "high"
        }
      ],
