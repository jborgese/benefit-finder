    {
      "id": "medicaid-nm-pregnant-women",
      "programId": "medicaid-new-mexico",
      "name": "New Mexico Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 250% FPL qualify for Medicaid in New Mexico",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInNewMexico"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3695
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "New Mexico covers pregnant women through Medicaid if household income is at or below 250% of the federal poverty level. For 2024, this means: 1 person = $3,695/month, 2 people = $5,005/month, 3 people = $6,315/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInNewMexico",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-nm",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-nm",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through YESNM or beWellnm",
          "url": "https://www.yes.state.nm.us/yesnm/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "New Mexico offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
