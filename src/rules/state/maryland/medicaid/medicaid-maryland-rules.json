    {
      "id": "medicaid-md-children",
      "programId": "medicaid-maryland",
      "name": "Maryland Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 300% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMaryland"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4435
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Maryland covers children under age 19 through Medicaid if household income is at or below 300% of the federal poverty level. For 2024, this means: 1 child = $4,435/month, 2 children = $6,006/month, 3 children = $7,577/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInMaryland",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-md",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-md",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Maryland Health Connection",
          "url": "https://www.marylandhealthconnection.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
