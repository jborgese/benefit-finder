    {
      "id": "medicaid-md-application-process",
      "programId": "medicaid-maryland",
      "name": "Maryland Medicaid Application Process",
      "description": "Information about applying for Medicaid in Maryland",
      "ruleLogic": {
        "var": "livesInMaryland"
      },
      "ruleType": "conditional",
      "explanation": "Maryland uses Maryland Health Connection for Medicaid and marketplace applications. You can apply: 1) **Online through Maryland Health Connection** (recommended - fastest, available 24/7, integrated marketplace and Medicaid), 2) **Through myMDTHINK** (for certain benefits), 3) **By phone** at 1-855-642-8572, 4) **In-person** at a local health department or enrollment center. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Maryland offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInMaryland"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Maryland Health Connection (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://www.marylandhealthconnection.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Maryland Health Connection: 1-855-642-8572",
          "priority": "high"
        },
        {
          "step": "Find a local enrollment center or health department",
          "url": "https://www.marylandhealthconnection.gov/enroll/get-help/",
          "priority": "medium"
        }
      ],
