    {
      "id": "medicaid-md-pregnant-women",
      "programId": "medicaid-maryland",
      "name": "Maryland Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 259% FPL qualify for Medicaid in Maryland",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMaryland"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3832
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Maryland covers pregnant women through Medicaid if household income is at or below 259% of the federal poverty level. For 2024, this means: 1 person = $3,832/month, 2 people = $5,191/month, 3 people = $6,550/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInMaryland",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-md",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-md",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through Maryland Health Connection",
          "url": "https://www.marylandhealthconnection.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Maryland offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
