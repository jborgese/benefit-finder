  "rules": [
    {
      "id": "medicaid-de-expansion-adults",
      "programId": "medicaid-delaware",
      "name": "Delaware Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Delaware's ACA expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInDelaware"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Delaware has expanded Medicaid under the Affordable Care Act. Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage through the Diamond State Health Plan. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, mental health services, and preventive care. Delaware has NO asset test for most adults.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInDelaware"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-de-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-de-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "School ID with photo"
          ],
          "where": "DMV or issuing agency"
        },
        {
          "id": "proof-residence-de",
          "name": "Proof of Delaware Residence",
          "description": "Mail, utility bill, lease, or employer letter showing Delaware address",
          "required": true,
          "alternatives": [
            "Delaware driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through Delaware ASSIST (recommended - fastest method)",
          "url": "https://assist.dhss.delaware.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Delaware Medicaid Customer Service: 1-800-372-2022",
          "priority": "high"
        },
        {
          "step": "Visit your local Division of Social Services office for in-person application",
          "url": "https://www.dhss.delaware.gov/dhss/dss/dss_office_locations.html",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
