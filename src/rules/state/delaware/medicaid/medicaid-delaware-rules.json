    {
      "id": "medicaid-de-application-process",
      "programId": "medicaid-delaware",
      "name": "Delaware Medicaid Application Process",
      "description": "Information about applying for Medicaid (Diamond State Health Plan) in Delaware",
      "ruleLogic": {
        "var": "livesInDelaware"
      },
      "ruleType": "conditional",
      "explanation": "Delaware uses the ASSIST (Application for Social Services Internet Technology) system for all Medicaid applications. You can apply: 1) **Online through Delaware ASSIST** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-372-2022 (Delaware Medicaid Customer Service), 3) **In-person** at your local Division of Social Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Delaware offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInDelaware"
      ],
      "nextSteps": [
        {
          "step": "Apply online through Delaware ASSIST (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://assist.dhss.delaware.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Delaware Medicaid Customer Service: 1-800-372-2022",
          "priority": "high"
        },
        {
          "step": "Find your local Division of Social Services office",
          "url": "https://www.dhss.delaware.gov/dhss/dss/dss_office_locations.html",
          "priority": "medium"
        }
      ],
