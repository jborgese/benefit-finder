    {
      "id": "medicaid-in-pregnant-women",
      "programId": "medicaid-indiana",
      "name": "Indiana Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 208% FPL qualify for Medicaid in Indiana",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIndiana"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3071
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Indiana covers pregnant women through Medicaid if household income is at or below 208% of the federal poverty level. For 2024, this means: 1 person = $3,071/month, 2 people = $4,159/month, 3 people = $5,247/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInIndiana",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-in",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-in",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through the FSSA Benefits Portal",
          "url": "https://www.in.gov/fssa/dfr/dfr-applications-and-forms/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Indiana offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
