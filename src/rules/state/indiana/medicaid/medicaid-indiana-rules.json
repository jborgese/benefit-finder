  "rules": [
    {
      "id": "medicaid-in-expansion-adults",
      "programId": "medicaid-indiana",
      "name": "Indiana Medicaid Expansion - Adults (HIP 2.0)",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid through HIP 2.0",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIndiana"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Indiana has expanded Medicaid under the Affordable Care Act through HIP 2.0 (Healthy Indiana Plan). Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. HIP 2.0 provides comprehensive health coverage including doctor visits, prescriptions, hospital care, mental health services, and preventive care. Indiana has NO asset test for most adults. Note: HIP 2.0 includes a POWER Account requirement for most enrollees above the federal poverty level, which requires small monthly contributions (typically $1-$27).",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInIndiana"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-in-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-in-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "School ID with photo"
          ],
          "where": "DMV or issuing agency"
        },
        {
          "id": "proof-residence-in",
          "name": "Proof of Indiana Residence",
          "description": "Mail, utility bill, lease, or employer letter showing Indiana address",
          "required": true,
          "alternatives": [
            "Indiana driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the FSSA Benefits Portal (recommended - fastest method)",
          "url": "https://www.in.gov/fssa/dfr/dfr-applications-and-forms/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Indiana FSSA: 1-800-403-0864",
          "priority": "high"
        },
        {
          "step": "Visit your local Division of Family Resources office for in-person application",
          "url": "https://www.in.gov/fssa/dfr/2997.htm",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
