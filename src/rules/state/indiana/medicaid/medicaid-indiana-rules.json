    {
      "id": "medicaid-in-application-process",
      "programId": "medicaid-indiana",
      "name": "Indiana Medicaid Application Process",
      "description": "Information about applying for Medicaid (HIP 2.0) in Indiana",
      "ruleLogic": {
        "var": "livesInIndiana"
      },
      "ruleType": "conditional",
      "explanation": "Indiana uses the FSSA Benefits Portal for all Medicaid applications. You can apply: 1) **Online through the FSSA Benefits Portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-800-403-0864, 3) **In-person** at your local Division of Family Resources office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Indiana offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInIndiana"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the FSSA Benefits Portal (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://www.in.gov/fssa/dfr/dfr-applications-and-forms/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Indiana FSSA: 1-800-403-0864",
          "priority": "high"
        },
        {
          "step": "Find your local Division of Family Resources office",
          "url": "https://www.in.gov/fssa/dfr/2997.htm",
          "priority": "medium"
        }
      ],
