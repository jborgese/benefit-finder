    {
      "id": "medicaid-in-children",
      "programId": "medicaid-indiana",
      "name": "Indiana Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 255% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIndiana"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3769
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Indiana covers children under age 19 through Medicaid if household income is at or below 255% of the federal poverty level. For 2024, this means: 1 child = $3,769/month, 2 children = $5,103/month, 3 children = $6,437/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInIndiana",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-in",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-in",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the FSSA Benefits Portal",
          "url": "https://www.in.gov/fssa/dfr/dfr-applications-and-forms/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
