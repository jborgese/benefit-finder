    {
      "id": "medicaid-sc-children",
      "programId": "medicaid-south-carolina",
      "name": "South Carolina Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 213% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInSouthCarolina"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3150
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "South Carolina covers children under age 19 through Medicaid if household income is at or below 213% of the federal poverty level. For 2024, this means: 1 child = $3,150/month, 2 children = $4,266/month, 3 children = $5,382/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInSouthCarolina",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-sc",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-sc",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through SC Connect",
          "url": "https://ssp.sc.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
