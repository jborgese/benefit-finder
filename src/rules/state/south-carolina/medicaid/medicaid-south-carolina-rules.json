  "metadata": {
    "id": "medicaid-south-carolina-rules-2024",
    "name": "South Carolina Medicaid Eligibility Rules (2024)",
    "description": "South Carolina-specific Medicaid eligibility rules for 2024. South Carolina has NOT expanded Medicaid under the ACA, so adults 19-64 without children or disabilities generally fall into a coverage gap. Coverage is available for pregnant women, children, and people with disabilities.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-SC",
    "programs": [
      "medicaid-south-carolina"
    ],
    "tags": [
      "medicaid",
      "healthcare",
      "south-carolina",
      "2024",
      "state-specific",
      "non-expansion",
      "coverage-gap"
    ],
    "createdAt": 1728777600000,
