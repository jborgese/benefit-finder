    {
      "id": "medicaid-sc-application-process",
      "programId": "medicaid-south-carolina",
      "name": "South Carolina Medicaid Application Process",
      "description": "Information about applying for Medicaid in South Carolina",
      "ruleLogic": {
        "var": "livesInSouthCarolina"
      },
      "ruleType": "conditional",
      "explanation": "South Carolina uses SC Connect for Medicaid applications. You can apply: 1) **Online through SC Connect** (recommended - fastest, available 24/7), 2) **By phone** at 1-888-549-0820, 3) **In-person** at your local Department of Health and Human Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. South Carolina offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInSouthCarolina"
      ],
      "nextSteps": [
        {
          "step": "Apply online through SC Connect (recommended)",
          "url": "https://ssp.sc.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call South Carolina Department of Health and Human Services: 1-888-549-0820",
          "priority": "high"
        },
        {
          "step": "Find your local Department of Health and Human Services office",
          "url": "https://www.scdhhs.gov/offices",
          "priority": "medium"
        }
      ],
