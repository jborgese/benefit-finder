  "rules": [
    {
      "id": "medicaid-sc-coverage-gap-adults",
      "programId": "medicaid-south-carolina",
      "name": "South Carolina Medicaid Coverage Gap for Adults",
      "description": "Adults 19-64 without children or disabilities are generally not eligible for Medicaid (coverage gap)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInSouthCarolina"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "hasDisability"
            }
          },
          {
            "!": {
              "var": "isPregnant"
            }
          },
          {
            ">": [
              {
                "var": "householdIncome"
              },
              0
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "South Carolina has NOT expanded Medicaid under the Affordable Care Act. This means adults age 19-64 who don't have children, aren't pregnant, and don't have a qualifying disability generally cannot get Medicaid coverage, even if their income is very low. This is called the 'coverage gap.' If you're in this situation, you may qualify for coverage through the Health Insurance Marketplace (HealthCare.gov) or may need to seek other healthcare options. Children, pregnant women, parents with dependent children, and people with disabilities may still qualify for Medicaid.",
      "requiredFields": [
        "livesInSouthCarolina",
        "age",
        "hasDisability",
        "isPregnant",
        "householdIncome"
      ],
      "requiredDocuments": [],
      "nextSteps": [
        {
          "step": "Check if you qualify for coverage through the Health Insurance Marketplace at HealthCare.gov",
          "url": "https://www.healthcare.gov/",
          "priority": "high",
          "estimatedTime": "30-60 minutes"
        },
        {
          "step": "Contact your local Department of Health and Human Services office to discuss other healthcare options",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "South Carolina Medicaid Eligibility",
          "url": "https://www.scdhhs.gov/apply",
          "date": "2024-10-01",
          "notes": "South Carolina has not expanded Medicaid under the ACA"
        },
        {
          "title": "HealthCare.gov - Coverage Gap",
          "url": "https://www.healthcare.gov/medicaid-chip/medicaid-expansion-and-you/",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-sc-coverage-gap-adult",
          "description": "Adult 19-64 without disability or pregnancy (coverage gap)",
          "input": {
            "livesInSouthCarolina": true,
            "age": 35,
            "hasDisability": false,
            "isPregnant": false,
            "householdIncome": 1000
          },
          "expected": false,
          "tags": [
            "ineligible",
            "coverage-gap",
            "non-expansion"
          ]
        },
        {
          "id": "test-sc-adult-with-disability",
          "description": "Adult with disability may qualify (check disability rules)",
          "input": {
            "livesInSouthCarolina": true,
            "age": 35,
            "hasDisability": true,
            "isPregnant": false,
            "householdIncome": 1000
          },
          "expected": false,
          "tags": [
            "check-disability-rules",
            "may-qualify"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "coverage-gap",
        "non-expansion",
        "adults"
      ],
      "category": "categorical-eligibility",
      "jurisdiction": "US-SC"
    },
    {
      "id": "medicaid-sc-pregnant-women",
      "programId": "medicaid-south-carolina",
      "name": "South Carolina Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 194% FPL qualify for Medicaid in South Carolina",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInSouthCarolina"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2870
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "South Carolina covers pregnant women through Medicaid if household income is at or below 194% of the federal poverty level. For 2024, this means: 1 person = $2,870/month, 2 people = $3,886/month, 3 people = $4,902/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInSouthCarolina",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-sc",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-sc",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through SC Connect",
          "url": "https://ssp.sc.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "South Carolina offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "South Carolina Medicaid for Pregnant Women",
          "url": "https://www.scdhhs.gov/apply",
          "date": "2024-10-01",
          "notes": "South Carolina covers pregnant women up to 194% FPL"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 25,
