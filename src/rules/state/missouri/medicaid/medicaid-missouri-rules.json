    {
      "id": "medicaid-mo-children",
      "programId": "medicaid-missouri",
      "name": "Missouri Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 150% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMissouri"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2258
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Missouri covers children under age 19 through Medicaid if household income is at or below 150% of the federal poverty level. For 2024, this means: 1 child = $2,258/month, 2 children = $3,058/month, 3 children = $3,858/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInMissouri",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-mo",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-mo",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through myDSS",
          "url": "https://mydss.mo.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
