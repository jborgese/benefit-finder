    {
      "id": "medicaid-mo-application-process",
      "programId": "medicaid-missouri",
      "name": "Missouri Medicaid Application Process",
      "description": "Information about applying for Medicaid in Missouri",
      "ruleLogic": {
        "var": "livesInMissouri"
      },
      "ruleType": "conditional",
      "explanation": "Missouri uses myDSS for all Medicaid applications. You can apply: 1) **Online through myDSS** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-373-4636, 3) **In-person** at your local Family Support Division office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Missouri offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInMissouri"
      ],
      "nextSteps": [
        {
          "step": "Apply online through myDSS (recommended)",
          "url": "https://mydss.mo.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Missouri Family Support Division: 1-855-373-4636",
          "priority": "high"
        },
        {
          "step": "Find your local Family Support Division office",
          "url": "https://dss.mo.gov/fsd/",
          "priority": "medium"
        }
      ],
