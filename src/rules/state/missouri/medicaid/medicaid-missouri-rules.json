    {
      "id": "medicaid-mo-pregnant-women",
      "programId": "medicaid-missouri",
      "name": "Missouri Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 196% FPL qualify for Medicaid in Missouri",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInMissouri"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2898
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Missouri covers pregnant women through Medicaid if household income is at or below 196% of the federal poverty level. For 2024, this means: 1 person = $2,898/month, 2 people = $3,925/month, 3 people = $4,952/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInMissouri",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-mo",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-mo",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through myDSS",
          "url": "https://mydss.mo.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Missouri offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
