    {
      "id": "medicaid-al-children",
      "programId": "medicaid-alabama",
      "name": "Alabama Medicaid for Children",
      "description": "Children up to age 19 with income up to 141% FPL qualify for Medicaid",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInAlabama"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2090
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Alabama covers children under age 19 through Medicaid if household income is at or below 141% of the federal poverty level. This provides comprehensive health coverage including doctor visits, prescriptions, dental, vision, and hospital care. Children up to age 1 can qualify with income up to 141% FPL, and children ages 1-18 can qualify with income up to 141% FPL.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInAlabama"
      ],
      "requiredDocuments": [
        {
          "id": "proof-age-al",
          "name": "Proof of Age",
          "description": "Birth certificate or other official document showing child's age",
          "required": true,
          "alternatives": [
            "Passport",
            "School records"
          ],
          "where": "Vital records office or school"
        },
        {
          "id": "proof-income-al-children",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements for all household members",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Bank statements"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at MyAlabama.gov",
          "url": "https://www.myalabama.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
