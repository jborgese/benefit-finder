    {
      "id": "medicaid-al-pregnant-women",
      "programId": "medicaid-alabama",
      "name": "Alabama Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 141% FPL qualify for Medicaid in Alabama",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInAlabama"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2090
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Alabama covers pregnant women through Medicaid if household income is at or below 141% of the federal poverty level. For 2024, this means: 1 person = $2,090/month, 2 people = $2,826/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "isPregnant",
        "householdIncome",
        "householdSize",
        "livesInAlabama"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-al",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-al-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at MyAlabama.gov",
          "url": "https://www.myalabama.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Alabama Medicaid Hotline: 1-800-362-1504",
          "priority": "high"
        },
        {
          "step": "Visit your local DHR office for in-person application",
          "url": "https://dhr.alabama.gov/office-locator/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
