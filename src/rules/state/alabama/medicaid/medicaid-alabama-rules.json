    {
      "id": "medicaid-al-application-process",
      "programId": "medicaid-alabama",
      "name": "Alabama Medicaid Application Process",
      "description": "Information about applying for Medicaid in Alabama",
      "ruleLogic": {
        "var": "livesInAlabama"
      },
      "ruleType": "conditional",
      "explanation": "To apply for Medicaid in Alabama: 1) Apply online at MyAlabama.gov (fastest), 2) Apply by phone at 1-800-362-1504, 3) Visit a local Department of Human Resources (DHR) office, or 4) Download and mail a paper application. You will need to provide information about household income, assets, and household members. Processing typically takes 30-45 days for regular applications, or 24 hours for emergency situations.",
