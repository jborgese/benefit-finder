  "metadata": {
    "id": "medicaid-alabama-rules-2024",
    "name": "Alabama Medicaid Eligibility Rules (2024)",
    "description": "Alabama-specific Medicaid eligibility rules for 2024. Alabama has NOT expanded Medicaid under the ACA, so adult eligibility is more restrictive than expansion states. These rules reflect Alabama's Medicaid and ALL Kids programs.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-AL",
    "programs": [
      "medicaid-alabama",
      "all-kids-alabama"
    ],
    "tags": [
      "medicaid",
      "healthcare",
      "alabama",
      "2024",
      "state-specific",
      "all-kids"
    ],
    "createdAt": 1728777600000,
    "updatedAt": 1728777600000
  },
  "rules": [
    {
      "id": "medicaid-al-no-expansion",
      "programId": "medicaid-alabama",
      "name": "Alabama Medicaid - No ACA Expansion Coverage Gap",
      "description": "Identifies adults in Alabama's Medicaid coverage gap who are NOT eligible",
      "ruleLogic": {
        "if": [
          {
            "and": [
              {
                "var": "livesInAlabama"
              },
              {
                ">=": [
                  {
                    "var": "age"
                  },
                  19
                ]
              },
              {
                "<": [
                  {
                    "var": "age"
                  },
                  65
                ]
              },
              {
                "!": {
                  "var": "isPregnant"
                }
              },
              {
                "!": {
                  "var": "hasQualifyingDisability"
                }
              },
              {
                "!": {
                  "var": "hasChildren"
                }
              }
            ]
          },
          false,
          true
        ]
      },
      "ruleType": "eligibility",
      "explanation": "IMPORTANT: Alabama has NOT expanded Medicaid. This means most non-disabled, non-pregnant adults age 19-64 without children do NOT qualify for Medicaid in Alabama, even with very low income. The coverage gap affects approximately 300,000 Alabamians. If you fall into this gap, explore options at HealthCare.gov for marketplace plans with subsidies, or contact a Community Health Center for care on a sliding fee scale.",
      "requiredFields": [
        "age",
        "isPregnant",
        "hasQualifyingDisability",
        "hasChildren",
        "livesInAlabama"
      ],
      "nextSteps": [
        {
          "step": "If you're in the coverage gap, check HealthCare.gov for marketplace plan subsidies",
          "url": "https://www.healthcare.gov",
          "priority": "high"
        },
        {
          "step": "Find a Community Health Center that offers sliding fee scale: https://findahealthcenter.hrsa.gov",
          "url": "https://findahealthcenter.hrsa.gov",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Alabama Medicaid Expansion Status",
          "url": "https://www.kff.org/medicaid/issue-brief/status-of-state-medicaid-expansion-decisions-interactive-map/",
          "date": "2024-10-01",
          "notes": "Alabama has not expanded Medicaid as of 2024"
        },
        {
          "title": "Alabama Medicaid Eligibility",
          "url": "https://medicaid.alabama.gov/",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-al-coverage-gap",
          "description": "Non-disabled adult in coverage gap - NOT eligible",
          "input": {
            "age": 35,
            "isPregnant": false,
            "hasQualifyingDisability": false,
            "hasChildren": false,
            "livesInAlabama": true
          },
          "expected": false,
          "tags": [
            "ineligible",
            "coverage-gap",
            "non-expansion"
          ]
        },
        {
          "id": "test-al-pregnant",
          "description": "Pregnant woman - eligible under pregnancy rules (not this rule)",
          "input": {
            "age": 30,
            "isPregnant": true,
            "hasQualifyingDisability": false,
            "hasChildren": false,
            "livesInAlabama": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "use-pregnancy-rules"
          ]
        },
        {
          "id": "test-al-disabled",
          "description": "Disabled adult - eligible under disability rules (not this rule)",
          "input": {
            "age": 40,
            "isPregnant": false,
            "hasQualifyingDisability": true,
            "hasChildren": false,
            "livesInAlabama": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "use-disability-rules"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "expansion",
