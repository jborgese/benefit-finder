        "livesInAlabama"
      ],
      "requiredDocuments": [
        {
          "id": "al-medicaid-application-documents",
          "name": "Application Documents Package",
          "description": "Comprehensive document list for Alabama Medicaid application",
          "required": true,
          "alternatives": [],
          "where": "Various sources"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at MyAlabama.gov (recommended - fastest processing)",
          "url": "https://www.myalabama.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Alabama Medicaid Hotline: 1-800-362-1504 for phone application",
          "priority": "medium"
        },
