    {
      "id": "medicaid-tx-non-expansion-adults",
      "programId": "medicaid-texas",
      "name": "Texas Medicaid - Non-Expansion State (Adults Without Children)",
      "description": "Texas has NOT expanded Medicaid - adults without children generally do not qualify unless they have disabilities or are elderly",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "!": {
              "var": "isPregnant"
            }
          },
          {
            "!": {
              "var": "hasQualifyingDisability"
            }
          },
          {
            "!": {
              "var": "hasChildren"
            }
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas has NOT expanded Medicaid under the Affordable Care Act. This means adults under age 65 who are not pregnant, disabled, or caring for children generally do NOT qualify for Medicaid regardless of income. This creates a 'coverage gap' where many low-income adults have no affordable health insurance options. Consider: 1) Marketplace plans with subsidies, 2) Community health centers for basic care, 3) Emergency Medicaid for emergencies only.",
      "requiredFields": [
        "livesInTexas",
        "age",
        "isPregnant",
        "hasQualifyingDisability",
        "hasChildren"
      ],
      "nextSteps": [
        {
          "step": "Consider marketplace health insurance with subsidies at healthcare.gov",
          "url": "https://www.healthcare.gov/",
          "priority": "high",
          "estimatedTime": "30-60 minutes"
        },
        {
          "step": "Find community health centers for basic care at reduced cost",
          "url": "https://findahealthcenter.hrsa.gov/",
          "priority": "high"
        },
        {
          "step": "Emergency Medicaid covers emergency medical conditions regardless of immigration status",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
