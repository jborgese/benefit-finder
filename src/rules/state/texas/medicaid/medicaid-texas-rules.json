    {
      "id": "medicaid-tx-pregnant-women",
      "programId": "medicaid-pregnancy-texas",
      "name": "Texas Medicaid for Pregnant Women",
      "description": "Pregnant women qualify for Medicaid with income up to 200% FPL (federal minimum)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2960
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas covers pregnant women through Medicaid if household income is at or below 200% of the federal poverty level. For 2024, this means: 1 person = $2,960/month, 2 people = $4,007/month, 3 people = $5,054/month. Coverage includes prenatal care, delivery, and postpartum care for up to 60 days after pregnancy ends.",
      "requiredFields": [
        "livesInTexas",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-tx",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-tx",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online at YourTexasBenefits.com",
          "url": "https://www.yourtexasbenefits.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Texas offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
