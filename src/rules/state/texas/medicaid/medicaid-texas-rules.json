    {
      "id": "medicaid-tx-disability",
      "programId": "medicaid-disabled-texas",
      "name": "Texas Medicaid for People with Disabilities",
      "description": "People with disabilities may qualify for Medicaid with income up to 74% FPL or through SSI",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "var": "hasQualifyingDisability"
          },
          {
            "or": [
              {
                "var": "receivesSSI"
              },
              {
                "<=": [
                  {
                    "var": "householdIncome"
                  },
                  {
                    "*": [
                      {
                        "var": "householdSize"
                      },
                      1095
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "People with disabilities qualify for Medicaid if they: 1) Receive SSI (automatic eligibility), OR 2) Have income below 74% FPL ($1,095/month for individual in 2024). Texas has asset limits for disabled adults. Many people with disabilities also qualify for Medicare and can have both Medicare and Medicaid (dual eligibility).",
      "requiredFields": [
        "livesInTexas",
        "hasQualifyingDisability",
        "receivesSSI",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-ssi-tx",
          "name": "Proof of SSI (if applicable)",
          "description": "SSI award letter or benefit verification",
          "required": false,
          "alternatives": [
            "Social Security benefit statement",
            "Bank deposits"
          ],
          "where": "Social Security Administration"
        },
        {
          "id": "proof-disability-tx",
          "name": "Proof of Disability",
          "description": "Medical records, SSA disability determination, or doctor's certification",
          "required": true,
          "alternatives": [
            "Medicare card",
            "Doctor's certification",
            "Functional assessment"
          ],
          "where": "SSA or healthcare provider"
        }
      ],
      "nextSteps": [
        {
          "step": "If you receive SSI, you should be automatically enrolled in Medicaid",
          "priority": "high"
        },
        {
          "step": "Apply online at YourTexasBenefits.com",
          "url": "https://www.yourtexasbenefits.com/",
          "priority": "high"
        }
      ],
      "version": {
