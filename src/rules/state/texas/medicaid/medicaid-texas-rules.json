  "rules": [
    {
      "id": "medicaid-tx-children",
      "programId": "medicaid-children-texas",
      "name": "Texas Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 200% FPL (federal minimum)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2960
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas covers children under age 19 in families with incomes up to 200% of the federal poverty level through Medicaid. For 2024 at 200% FPL: 1 child = $2,960/month, 2 children = $4,007/month, 3 children = $5,054/month. Children can enroll year-round, not just during open enrollment periods.",
      "requiredFields": [
        "livesInTexas",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-tx",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-tx",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at YourTexasBenefits.com (fastest method)",
          "url": "https://www.yourtexasbenefits.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        },
        {
          "step": "Find local assistance at a Texas Health and Human Services office",
          "url": "https://www.hhs.texas.gov/",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
