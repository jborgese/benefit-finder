      "programId": "medicaid-texas",
      "name": "Texas Medicaid Application Process",
      "description": "Information about applying for Medicaid in Texas through YourTexasBenefits.com",
      "ruleLogic": {
        "var": "livesInTexas"
      },
      "ruleType": "conditional",
      "explanation": "Texas offers multiple ways to apply for Medicaid: 1) **YourTexasBenefits.com** (primary online portal), 2) **Phone applications** at 2-1-1 or 1-877-541-7905, 3) **In-person assistance** at local Health and Human Services offices, 4) **Paper applications** available for download. Most applications are processed within 45 days, with expedited processing for urgent medical needs.",
      "requiredFields": [
        "livesInTexas"
      ],
      "nextSteps": [
        {
          "step": "Apply online at YourTexasBenefits.com (recommended - fastest)",
          "url": "https://www.yourtexasbenefits.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call 2-1-1 for phone application assistance",
          "priority": "high",
          "estimatedTime": "30-60 minutes"
        },
        {
          "step": "Find your local Health and Human Services office",
          "url": "https://www.hhs.texas.gov/",
          "priority": "medium"
        }
      ],
