    {
      "id": "tanf-tx-citizenship",
      "programId": "tanf-texas",
      "name": "Texas TANF Citizenship Requirements",
      "description": "Must be U.S. citizen or qualified immigrant to receive TANF",
      "ruleLogic": {
        "or": [
          {
            "in": [
              {
                "var": "citizenship"
              },
              [
                "us_citizen",
                "us_national"
              ]
            ]
          },
          {
            "and": [
              {
                "in": [
                  {
                    "var": "citizenship"
                  },
                  [
                    "permanent_resident",
                    "refugee",
                    "asylee",
                    "trafficking_victim"
                  ]
                ]
              },
              {
                ">=": [
                  {
                    "var": "yearsInUS"
                  },
                  5
                ]
              }
            ]
          },
          {
            "in": [
              {
                "var": "citizenship"
              },
              [
                "refugee",
                "asylee",
                "trafficking_victim",
                "cuban_haitian"
              ]
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "U.S. citizens and nationals qualify for TANF. Lawfully present immigrants may qualify, but most must wait 5 years after obtaining qualified status. Exceptions to the 5-year wait include: refugees, asylees, trafficking victims, Cuban/Haitian entrants, certain military members and families. Mixed-status households can receive TANF for eligible members.",
      "requiredFields": [
        "citizenship",
        "yearsInUS"
      ],
      "requiredDocuments": [
        {
          "id": "proof-citizenship-tanf-tx",
          "name": "Proof of Citizenship or Immigration Status",
          "description": "Birth certificate, passport, green card, or refugee documentation",
          "required": true,
          "alternatives": [
            "U.S. Passport",
            "Naturalization Certificate",
            "Green Card (I-551)",
            "I-94 with refugee/asylee status"
          ],
          "where": "Government agency that issued the document"
        }
      ],
      "nextSteps": [
        {
          "step": "Mixed-status households can apply for eligible family members",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Texas TANF Citizenship Requirements",
          "url": "https://www.hhs.texas.gov/services/financial/cash-assistance",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 20,
      "testCases": [
