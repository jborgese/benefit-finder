    {
      "id": "tanf-tx-work-requirements",
      "programId": "tanf-texas",
      "name": "Texas TANF Work Requirements",
      "description": "TANF recipients must participate in work activities for required hours per week",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isTANFRecipient"
            }
          },
          {
            "var": "meetsWorkRequirements"
          },
          {
            "var": "isExemptFromWorkRequirements"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "TANF recipients must participate in work activities for: 20 hours/week for single parents with child under 6, 30 hours/week for single parents with child 6+, 35 hours/week for two-parent families. Work activities include: employment, job search, job training, education, community service. Exemptions: pregnant (3rd trimester), caring for disabled child/adult, age 60+, disabled, child under 1 year.",
      "requiredFields": [
        "isTANFRecipient",
        "meetsWorkRequirements",
        "isExemptFromWorkRequirements"
      ],
      "requiredDocuments": [
        {
          "id": "proof-work-activities-tx",
          "name": "Proof of Work Activities",
          "description": "Pay stubs, work program participation, job search logs",
          "required": false,
          "alternatives": [
            "Employer letter",
            "Training certificates",
            "Community service verification"
          ],
          "where": "Employer, work program, or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "If you receive TANF, you must participate in work activities or face benefit reduction",
          "priority": "high"
        },
        {
          "step": "Contact your caseworker about work program options",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
