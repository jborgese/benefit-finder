  "rules": [
    {
      "id": "tanf-tx-income-limit",
      "programId": "tanf-texas",
      "name": "Texas TANF Income Limit",
      "description": "Household income must be below 185% FPL to qualify for TANF",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "<": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2738
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas TANF has an income limit of 185% of the federal poverty level. For 2024 at 185% FPL: 1 person = $2,738/month, 2 people = $3,707/month, 3 people = $4,676/month, 4 people = $5,645/month. This is a gross income test - if you pass this, you may still need to meet other requirements.",
      "requiredFields": [
        "livesInTexas",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-tanf-tx",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Bank statements",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at YourTexasBenefits.com",
          "url": "https://www.yourtexasbenefits.com/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Texas TANF Income Limits",
          "url": "https://www.hhs.texas.gov/services/financial/cash-assistance",
          "date": "2024-10-01",
          "notes": "Texas TANF income limit: 185% FPL"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-tx-tanf-income-eligible",
          "description": "Household under 185% FPL income",
          "input": {
            "livesInTexas": true,
            "householdIncome": 2000,
            "householdSize": 1
          },
          "expected": true,
          "tags": [
            "eligible",
            "income",
            "185-percent-fpl"
          ]
        },
        {
          "id": "test-tx-tanf-income-ineligible",
          "description": "Household over 185% FPL income",
          "input": {
            "livesInTexas": true,
            "householdIncome": 3500,
            "householdSize": 1
          },
          "expected": false,
          "tags": [
            "ineligible",
            "high-income"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
        "income",
        "185-percent-fpl"
      ],
      "category": "financial-eligibility",
      "jurisdiction": "US-TX"
    },
    {
      "id": "tanf-tx-children-requirement",
      "programId": "tanf-texas",
      "name": "Texas TANF Children Requirement",
      "description": "Must have a child under 18 (or 19 if still in high school) to qualify for TANF",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "var": "hasChildren"
          },
          {
            "or": [
              {
                "<": [
                  {
                    "var": "ageOfYoungestChild"
                  },
                  18
                ]
              },
              {
                "and": [
                  {
                    "<=": [
                      {
                        "var": "ageOfYoungestChild"
                      },
                      19
                    ]
                  },
                  {
                    "var": "childInHighSchool"
                  }
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "TANF is designed to help families with children. You must have at least one child under age 18, or age 18-19 if still attending high school full-time. The child must live with you and be related to you (biological, adopted, stepchild, or other relative).",
      "requiredFields": [
        "livesInTexas",
        "hasChildren",
        "ageOfYoungestChild",
        "childInHighSchool"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-tanf-tx",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, school records, or hospital records",
          "required": true,
          "alternatives": [
            "Passport",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, school, or hospital"
        },
        {
          "id": "proof-school-enrollment-tx",
          "name": "Proof of School Enrollment (if applicable)",
          "description": "School enrollment verification for children 18-19",
          "required": false,
          "alternatives": [
            "Class schedule",
            "School ID",
            "Report card"
          ],
          "where": "School registrar"
        }
      ],
      "nextSteps": [
        {
          "step": "Gather birth certificates for all children",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Texas TANF Children Requirements",
          "url": "https://www.hhs.texas.gov/services/financial/cash-assistance",
          "date": "2024-10-01",
          "notes": "Must have child under 18 or 18-19 in high school"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 25,
      "testCases": [
        {
          "id": "test-tx-tanf-child-eligible",
          "description": "Household with child under 18",
          "input": {
            "livesInTexas": true,
            "hasChildren": true,
            "ageOfYoungestChild": 10,
            "childInHighSchool": false
          },
          "expected": true,
          "tags": [
            "eligible",
            "child",
            "under-18"
          ]
        },
        {
          "id": "test-tx-tanf-child-high-school",
          "description": "Household with child 18-19 in high school",
          "input": {
            "livesInTexas": true,
            "hasChildren": true,
            "ageOfYoungestChild": 18,
            "childInHighSchool": true
          },
          "expected": true,
          "tags": [
            "eligible",
            "child",
            "high-school"
          ]
        },
        {
          "id": "test-tx-tanf-no-children",
          "description": "Household without children",
          "input": {
            "livesInTexas": true,
            "hasChildren": false,
            "ageOfYoungestChild": 0,
            "childInHighSchool": false
          },
          "expected": false,
          "tags": [
            "ineligible",
            "no-children"
          ]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": [
