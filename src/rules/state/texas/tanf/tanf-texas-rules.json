    {
      "id": "tanf-tx-time-limits",
      "programId": "tanf-texas",
      "name": "Texas TANF Time Limits",
      "description": "TANF benefits are limited to 60 months (5 years) lifetime maximum",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isTANFRecipient"
            }
          },
          {
            "<": [
              {
                "var": "monthsOfTANFReceived"
              },
              60
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas TANF has a 60-month (5-year) lifetime limit for cash assistance. This limit applies to the adult recipient, not the children. Once you reach 60 months, you cannot receive TANF cash assistance again, even if you become eligible later. Some months may not count toward the limit if you were working or in certain circumstances.",
      "requiredFields": [
        "isTANFRecipient",
        "monthsOfTANFReceived"
      ],
      "requiredDocuments": [
        {
          "id": "proof-tanf-history-tx",
          "name": "Proof of TANF History",
          "description": "Previous TANF award letters or case records",
          "required": false,
          "alternatives": [
            "Benefit statements",
            "Case worker records"
          ],
          "where": "Health and Human Services office"
        }
      ],
      "nextSteps": [
        {
          "step": "Check your TANF history to see how many months you have remaining",
          "priority": "high"
        },
        {
          "step": "Consider other assistance programs if you're near the time limit",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Texas TANF Time Limits",
          "url": "https://www.hhs.texas.gov/services/financial/cash-assistance",
          "date": "2024-10-01",
          "notes": "60-month lifetime limit for TANF cash assistance"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 15,
      "testCases": [
