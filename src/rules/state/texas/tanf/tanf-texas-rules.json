    {
      "id": "tanf-tx-asset-limit",
      "programId": "tanf-texas",
      "name": "Texas TANF Asset Limit",
      "description": "Household assets must be below $1,000 to qualify for TANF",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "<": [
              {
                "var": "householdAssets"
              },
              1000
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas TANF has a strict asset limit of $1,000. Assets include: cash, bank accounts, stocks, bonds, real estate (except home), vehicles (except one), personal property. Excluded: home you live in, one vehicle per household, household goods, clothing, tools of trade.",
      "requiredFields": [
        "livesInTexas",
        "householdAssets"
      ],
      "requiredDocuments": [
        {
          "id": "proof-assets-tanf-tx",
          "name": "Proof of Assets",
          "description": "Bank statements, vehicle titles, property deeds",
          "required": true,
          "alternatives": [
            "Investment statements",
            "Savings account statements",
            "Vehicle registration"
          ],
          "where": "Bank, DMV, or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Gather documentation of all assets including bank accounts and vehicles",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Texas TANF Asset Limits",
          "url": "https://www.hhs.texas.gov/services/financial/cash-assistance",
          "date": "2024-10-01",
          "notes": "Asset limit: $1,000 for TANF"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 20,
