    {
      "id": "wic-tx-nutritional-risk",
      "programId": "wic-texas",
      "name": "Texas WIC Nutritional Risk Assessment",
      "description": "WIC participants must have nutritional risk factors",
      "ruleLogic": {
        "or": [
          {
            "!": {
              "var": "isWICApplicant"
            }
          },
          {
            "var": "hasNutritionalRisk"
          },
          {
            "var": "hasMedicalRisk"
          },
          {
            "var": "hasDietaryRisk"
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "WIC participants must have nutritional risk factors determined by a health professional. Risk factors include: anemia, underweight, overweight, poor diet, pregnancy complications, breastfeeding difficulties, or other medical conditions. This assessment is done at the WIC clinic by a nutritionist or health professional.",
      "requiredFields": [
        "isWICApplicant",
        "hasNutritionalRisk",
        "hasMedicalRisk",
        "hasDietaryRisk"
      ],
      "requiredDocuments": [
        {
          "id": "proof-nutritional-risk-wic-tx",
          "name": "Proof of Nutritional Risk",
          "description": "Medical records, lab results, or health professional assessment",
          "required": false,
          "alternatives": [
            "Doctor's note",
            "Blood test results",
            "Growth charts"
          ],
          "where": "Healthcare provider or WIC clinic"
        }
      ],
      "nextSteps": [
        {
          "step": "Visit your local WIC clinic for nutritional risk assessment",
          "priority": "high",
          "estimatedTime": "1-2 hours"
        },
        {
          "step": "Bring any medical records or lab results you have",
          "priority": "medium"
        }
      ],
      "version": {
