    {
      "id": "wic-tx-pregnant-women",
      "programId": "wic-texas",
      "name": "Texas WIC for Pregnant Women",
      "description": "Pregnant women qualify for WIC with income up to 185% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2738
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Pregnant women qualify for WIC if household income is at or below 185% of the federal poverty level. WIC provides: nutritious foods, nutrition education, breastfeeding support, and referrals to healthcare. Coverage continues through pregnancy and up to 6 months after delivery if breastfeeding, or up to 6 months after delivery if not breastfeeding.",
      "requiredFields": [
        "livesInTexas",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-wic-tx",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-wic-tx",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply at your local WIC clinic as soon as you know you're pregnant",
          "priority": "high",
          "estimatedTime": "1-2 hours"
        },
        {
          "step": "WIC provides nutrition education and breastfeeding support",
          "priority": "high"
        }
      ],
      "version": {
