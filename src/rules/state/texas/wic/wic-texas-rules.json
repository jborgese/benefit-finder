  "rules": [
    {
      "id": "wic-tx-income-limit",
      "programId": "wic-texas",
      "name": "Texas WIC Income Limit (185% FPL)",
      "description": "Household income must be at or below 185% FPL to qualify for WIC",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInTexas"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2738
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Texas WIC follows federal guidelines with an income limit of 185% of the federal poverty level. For 2024 at 185% FPL: 1 person = $2,738/month, 2 people = $3,707/month, 3 people = $4,676/month, 4 people = $5,645/month. This is a gross income test - if you pass this, you may still need to meet other requirements.",
      "requiredFields": [
        "livesInTexas",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-wic-tx",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Bank statements",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply at your local WIC clinic",
          "url": "https://www.hhs.texas.gov/services/health/wic",
          "priority": "high",
          "estimatedTime": "1-2 hours"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Texas WIC Income Limits",
          "url": "https://www.hhs.texas.gov/services/health/wic",
          "date": "2024-10-01",
          "notes": "Texas WIC income limit: 185% FPL"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-tx-wic-income-eligible",
          "description": "Household under 185% FPL income",
