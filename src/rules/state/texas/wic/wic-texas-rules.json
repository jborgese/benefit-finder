    {
      "id": "wic-tx-application-process",
      "programId": "wic-texas",
      "name": "Texas WIC Application Process",
      "description": "Information about applying for WIC in Texas at local WIC clinics",
      "ruleLogic": {
        "var": "livesInTexas"
      },
      "ruleType": "conditional",
      "explanation": "Texas WIC applications are processed at local WIC clinics throughout the state. You must apply in person at a WIC clinic near you. The process includes: 1) Income verification, 2) Nutritional risk assessment, 3) Health screening, 4) Nutrition education. Most applications are processed the same day. You can find your local WIC clinic online or by calling 2-1-1.",
      "requiredFields": [
        "livesInTexas"
      ],
      "nextSteps": [
        {
          "step": "Find your local WIC clinic",
