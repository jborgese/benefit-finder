    {
      "id": "medicaid-ia-children",
      "programId": "medicaid-iowa",
      "name": "Iowa Medicaid for Children (HAWK-I)",
      "description": "Children under 19 qualify for Medicaid with income up to 302% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIowa"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  4468
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Iowa covers children under age 19 through Medicaid (including HAWK-I) if household income is at or below 302% of the federal poverty level. For 2024, this means: 1 child = $4,468/month, 2 children = $6,050/month, 3 children = $7,632/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInIowa",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ia",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ia",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Iowa Benefits Portal",
          "url": "https://hhs.iowa.gov/imex/app",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
