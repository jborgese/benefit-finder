    {
      "id": "medicaid-ia-application-process",
      "programId": "medicaid-iowa",
      "name": "Iowa Medicaid Application Process",
      "description": "Information about applying for Medicaid in Iowa",
      "ruleLogic": {
        "var": "livesInIowa"
      },
      "ruleType": "conditional",
      "explanation": "Iowa uses the Iowa Benefits Portal for all Medicaid applications. You can apply: 1) **Online through the Iowa Benefits Portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-889-7985, 3) **In-person** at your local Iowa HHS office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Iowa offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInIowa"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Iowa Benefits Portal (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://hhs.iowa.gov/imex/app",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Iowa HHS: 1-855-889-7985",
          "priority": "high"
        },
        {
          "step": "Find your local Iowa HHS office",
          "url": "https://hhs.iowa.gov/about-hhs/field-offices",
          "priority": "medium"
        }
      ],
