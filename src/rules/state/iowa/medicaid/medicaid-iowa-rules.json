    {
      "id": "medicaid-ia-pregnant-women",
      "programId": "medicaid-iowa",
      "name": "Iowa Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 375% FPL qualify for Medicaid in Iowa",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIowa"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  5547
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Iowa covers pregnant women through Medicaid if household income is at or below 375% of the federal poverty level. For 2024, this means: 1 person = $5,547/month, 2 people = $7,513/month, 3 people = $9,479/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInIowa",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ia",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ia",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through the Iowa Benefits Portal",
          "url": "https://hhs.iowa.gov/imex/app",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Iowa offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
