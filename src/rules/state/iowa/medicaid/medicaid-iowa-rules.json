  "rules": [
    {
      "id": "medicaid-ia-expansion-adults",
      "programId": "medicaid-iowa",
      "name": "Iowa Medicaid Expansion - Adults",
      "description": "Adults age 19-64 with income up to 138% FPL qualify for Medicaid under Iowa's ACA expansion",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInIowa"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Iowa has expanded Medicaid under the Affordable Care Act. Adults age 19-64 with household income at or below 138% of the Federal Poverty Level qualify for Medicaid coverage (HAWK-I for adults). For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. This provides comprehensive health coverage including doctor visits, prescriptions, hospital care, mental health services, and preventive care. Iowa has NO asset test for most adults.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInIowa"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ia-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ia-medicaid",
          "name": "Proof of Identity",
          "description": "Valid photo ID",
          "required": true,
          "alternatives": [
            "Driver's license",
            "State ID",
            "Passport",
            "School ID with photo"
          ],
          "where": "DMV or issuing agency"
        },
        {
          "id": "proof-residence-ia",
          "name": "Proof of Iowa Residence",
          "description": "Mail, utility bill, lease, or employer letter showing Iowa address",
          "required": true,
          "alternatives": [
            "Iowa driver's license",
            "Homeless shelter statement",
            "Statement from person you live with"
          ],
          "where": "Varies by document type"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Iowa Benefits Portal (recommended - fastest method)",
          "url": "https://hhs.iowa.gov/imex/app",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Iowa HHS: 1-855-889-7985",
          "priority": "high"
        },
        {
          "step": "Visit your local Iowa HHS office for in-person application",
          "url": "https://hhs.iowa.gov/about-hhs/field-offices",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
