  "rules": [
    {
      "id": "medicaid-ky-expansion-adults",
      "programId": "medicaid-kentucky",
      "name": "Kentucky Medicaid for ACA Expansion Adults",
      "description": "Adults 19-64 eligible through Kentucky HEALTH expansion up to 138% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInKentucky"
          },
          {
            ">=": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<": [
              {
                "var": "age"
              },
              65
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Kentucky has expanded Medicaid under the Affordable Care Act through the Kentucky HEALTH (Helping to Engage and Achieve Long Term Health) program. Adults age 19-64 are eligible for Medicaid if household income is at or below 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,486/month, 4 people = $4,209/month. Kentucky HEALTH includes health savings accounts and community engagement requirements. Coverage includes doctor visits, prescriptions, hospital care, mental health services, and preventive care.",
      "requiredFields": [
        "livesInKentucky",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-income-ky-adult",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records",
            "Bank statements"
          ],
          "where": "Employer or your records"
        },
        {
          "id": "proof-identity-ky-adult",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": [
            "Birth certificate",
            "Social Security card"
          ],
          "where": "DMV or State Department"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Kentucky Benefits portal",
          "url": "https://benefind.ky.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Kentucky HEALTH includes community engagement requirements - check with your local office for details",
          "priority": "medium"
        }
      ],
      "version": {
