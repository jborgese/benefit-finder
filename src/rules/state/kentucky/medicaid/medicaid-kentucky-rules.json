    {
      "id": "medicaid-ky-children",
      "programId": "medicaid-kentucky",
      "name": "Kentucky Medicaid for Children",
      "description": "Children under 19 qualify for Medicaid with income up to 159% FPL",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInKentucky"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2351
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Kentucky covers children under age 19 through Medicaid if household income is at or below 159% of the federal poverty level. For 2024, this means: 1 child = $2,351/month, 2 children = $3,184/month, 3 children = $4,017/month. Children can enroll year-round, not just during open enrollment periods. Coverage includes doctor visits, prescriptions, dental care, vision care, and immunizations.",
      "requiredFields": [
        "livesInKentucky",
        "age",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ky",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income-ky",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Kentucky Benefits portal",
          "url": "https://benefind.ky.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Children can enroll year-round - no waiting for open enrollment",
          "priority": "high"
        }
      ],
