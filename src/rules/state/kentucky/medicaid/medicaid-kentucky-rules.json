    {
      "id": "medicaid-ky-pregnant-women",
      "programId": "medicaid-kentucky",
      "name": "Kentucky Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 195% FPL qualify for Medicaid in Kentucky",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInKentucky"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  2882
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Kentucky covers pregnant women through Medicaid if household income is at or below 195% of the federal poverty level. For 2024, this means: 1 person = $2,882/month, 2 people = $3,904/month, 3 people = $4,926/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "livesInKentucky",
        "isPregnant",
        "householdIncome",
        "householdSize"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ky",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant-ky",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply immediately when you learn you're pregnant - coverage can start right away",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Apply online through the Kentucky Benefits portal",
          "url": "https://benefind.ky.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Kentucky offers presumptive eligibility for pregnant women - temporary coverage while application is processed",
          "priority": "high"
        }
      ],
