    {
      "id": "medicaid-ky-application-process",
      "programId": "medicaid-kentucky",
      "name": "Kentucky Medicaid Application Process",
      "description": "Information about applying for Medicaid in Kentucky",
      "ruleLogic": {
        "var": "livesInKentucky"
      },
      "ruleType": "conditional",
      "explanation": "Kentucky uses the Kentucky Benefits portal for all Medicaid applications. You can apply: 1) **Online through the Kentucky Benefits portal** (recommended - fastest, available 24/7), 2) **By phone** at 1-855-306-8959, 3) **In-person** at your local Kentucky Cabinet for Health and Family Services office. Most applications are processed within 45 days. Expedited processing is available for urgent medical needs. Kentucky offers language assistance for non-English speakers.",
      "requiredFields": [
        "livesInKentucky"
      ],
      "nextSteps": [
        {
          "step": "Apply online through the Kentucky Benefits portal (recommended - handles both Medicaid and marketplace plans)",
          "url": "https://benefind.ky.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Kentucky Cabinet for Health and Family Services: 1-855-306-8959",
          "priority": "high"
        },
        {
          "step": "Find your local Kentucky Cabinet for Health and Family Services office",
          "url": "https://chfs.ky.gov/agencies/dms/health/Pages/default.aspx",
          "priority": "medium"
        }
      ],
