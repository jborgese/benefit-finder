    {
      "id": "medicaid-ga-parents-caretakers",
      "programId": "medicaid-georgia",
      "name": "Georgia Medicaid for Parents and Caretakers",
      "description": "Parents and caretaker relatives with very low income may qualify for Medicaid",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInGeorgia"
          },
          {
            "or": [
              {
                "var": "isParentOfDependentChild"
              },
              {
                "var": "isCaretakerOfRelativeChild"
              }
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  444
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Georgia Medicaid covers parents and caretaker relatives, but only with VERY low income - about 30% of the federal poverty level. For 2024, this means: 1 person = $444/month, 2 people = $600/month, 3 people = $756/month. This is one of the lowest income limits in the nation. If you're a parent with slightly higher income, you likely fall into Georgia's coverage gap and should explore marketplace plans at HealthCare.gov.",
      "requiredFields": [
        "isParentOfDependentChild",
        "isCaretakerOfRelativeChild",
        "householdIncome",
        "householdSize",
        "livesInGeorgia"
      ],
      "requiredDocuments": [
        {
          "id": "proof-caretaker-status",
          "name": "Proof of Parent/Caretaker Status",
          "description": "Birth certificates showing relationship to children",
          "required": true,
          "alternatives": [
            "Court documents",
            "Custody papers",
            "School records"
          ],
          "where": "Vital records or court"
        },
        {
          "id": "proof-very-low-income",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or statement of no income",
          "required": true,
          "alternatives": [
            "Zero income statement",
            "Unemployment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "If income is slightly higher, check Healthcare.gov for marketplace subsidies",
          "url": "https://www.healthcare.gov",
          "priority": "high"
        }
      ],
