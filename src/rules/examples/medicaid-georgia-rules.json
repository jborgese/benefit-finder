    {
      "id": "medicaid-ga-disability-aged",
      "programId": "medicaid-georgia",
      "name": "Georgia Medicaid for Aged, Blind, and Disabled",
      "description": "Aged (65+), blind, or disabled individuals may qualify if receiving SSI or meeting income/asset limits",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInGeorgia"
          },
          {
            "or": [
              {
                ">=": [
                  {
                    "var": "age"
                  },
                  65
                ]
              },
              {
                "var": "isBlind"
              },
              {
                "var": "hasQualifyingDisability"
              }
            ]
          },
          {
            "or": [
              {
                "var": "receivesSSI"
              },
              {
                "and": [
                  {
                    "<=": [
                      {
                        "var": "householdIncome"
                      },
                      {
                        "*": [
                          {
                            "var": "householdSize"
                          },
                          1133
                        ]
                      }
                    ]
                  },
                  {
                    "<=": [
                      {
                        "var": "countableAssets"
                      },
                      2000
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Georgia Medicaid covers aged (65+), blind, or disabled individuals if they: 1) Receive SSI (automatic eligibility), OR 2) Have income below approximately 74% FPL ($1,133/month for individual in 2024) AND countable assets under $2,000 ($3,000 for couples). Home, one vehicle, and certain other assets don't count. Many people in this category also qualify for Medicare.",
      "requiredFields": [
        "age",
        "isBlind",
        "hasQualifyingDisability",
        "receivesSSI",
        "householdIncome",
        "householdSize",
        "countableAssets",
        "livesInGeorgia"
      ],
      "requiredDocuments": [
        {
          "id": "proof-ssi-ga",
          "name": "Proof of SSI (if applicable)",
          "description": "SSI award letter or benefit verification",
          "required": false,
          "alternatives": [
            "Social Security benefit statement",
            "Bank deposits"
          ],
          "where": "Social Security Administration"
        },
        {
          "id": "proof-disability-ga",
          "name": "Proof of Disability/Blindness/Age",
          "description": "Medical records, SSA disability determination, or birth certificate",
          "required": true,
          "alternatives": [
            "Medicare card",
            "Doctor's certification"
          ],
          "where": "SSA or healthcare provider"
        }
      ],
      "nextSteps": [
        {
          "step": "If you receive SSI, you should be automatically enrolled in Medicaid",
          "priority": "high"
        },
        {
          "step": "Contact Georgia Medicaid at 1-877-423-4746 to verify eligibility",
          "priority": "high"
        }
      ],
      "version": {
