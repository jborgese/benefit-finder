    {
      "id": "medicaid-ga-children-peachcare",
      "programId": "medicaid-georgia",
      "name": "Georgia Medicaid and PeachCare for Kids",
      "description": "Children under 19 qualify for Medicaid (up to 209% FPL) or PeachCare (209-247% FPL)",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInGeorgia"
          },
          {
            "<": [
              {
                "var": "age"
              },
              19
            ]
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3659
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Georgia children have two programs: 1) **Medicaid**: Free for children in families with income up to 209% FPL ($3,096/month for a family of 3). 2) **PeachCare for Kids**: Low-cost coverage ($20-35/month) for families with income 209-247% FPL ($3,659/month for a family of 3). Both programs cover doctor visits, immunizations, dental, vision, and hospital care. Children can enroll year-round.",
      "requiredFields": [
        "age",
        "householdIncome",
        "householdSize",
        "livesInGeorgia"
      ],
      "requiredDocuments": [
        {
          "id": "proof-child-age-ga",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": [
            "School records",
            "Adoption papers",
            "Religious records"
          ],
          "where": "Vital records or hospital"
        },
        {
          "id": "proof-household-income-ga",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply for Medicaid or PeachCare at Gateway.GA.gov",
          "url": "https://gateway.ga.gov/",
          "priority": "high"
        },
        {
          "step": "Call PeachCare for Kids: 1-877-427-3224",
          "priority": "high"
        }
      ],
