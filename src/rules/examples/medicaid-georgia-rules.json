  "rules": [
    {
      "id": "medicaid-ga-no-expansion",
      "programId": "medicaid-georgia",
      "name": "Georgia Medicaid - No ACA Expansion Coverage Gap",
      "description": "Identifies adults in Georgia's Medicaid coverage gap who are NOT eligible",
      "ruleLogic": {
        "if": [
          {
            "and": [
              {
                "var": "livesInGeorgia"
              },
              {
                ">=": [
                  {
                    "var": "age"
                  },
                  19
                ]
              },
              {
                "<": [
                  {
                    "var": "age"
                  },
                  65
                ]
              },
              {
                "!": {
                  "var": "isPregnant"
                }
              },
              {
                "!": {
                  "var": "hasQualifyingDisability"
                }
              },
              {
                "!": {
                  "var": "hasChildren"
                }
              }
            ]
          },
          false,
          true
        ]
      },
      "ruleType": "eligibility",
      "explanation": "IMPORTANT: Georgia has NOT expanded Medicaid. This means most non-disabled, non-pregnant adults age 19-64 without children do NOT qualify for Medicaid in Georgia, even with very low income. The coverage gap affects approximately 350,000 Georgians. If you fall into this gap, explore options at HealthCare.gov for marketplace plans with subsidies, or contact a Community Health Center for care on a sliding fee scale.",
      "requiredFields": [
        "age",
        "isPregnant",
        "hasQualifyingDisability",
        "hasChildren",
        "livesInGeorgia"
      ],
      "nextSteps": [
        {
          "step": "If you're in the coverage gap, check HealthCare.gov for marketplace plan subsidies",
          "url": "https://www.healthcare.gov",
          "priority": "high"
        },
        {
          "step": "Find a Community Health Center that offers sliding fee scale: https://findahealthcenter.hrsa.gov",
          "url": "https://findahealthcenter.hrsa.gov",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "Georgia Medicaid Expansion Status",
          "url": "https://www.kff.org/medicaid/issue-brief/status-of-state-medicaid-expansion-decisions-interactive-map/",
          "date": "2024-10-01",
          "notes": "Georgia has not expanded Medicaid as of 2024"
        },
        {
          "title": "Georgia Medicaid Eligibility",
