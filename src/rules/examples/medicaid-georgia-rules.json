    {
      "id": "medicaid-ga-pregnant-women",
      "programId": "medicaid-georgia",
      "name": "Georgia Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 220% FPL qualify for Medicaid in Georgia",
      "ruleLogic": {
        "and": [
          {
            "var": "livesInGeorgia"
          },
          {
            "var": "isPregnant"
          },
          {
            "<=": [
              {
                "var": "householdIncome"
              },
              {
                "*": [
                  {
                    "var": "householdSize"
                  },
                  3256
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Georgia covers pregnant women through Medicaid if household income is at or below 220% of the federal poverty level. This is higher than the federal minimum (200% FPL). For 2024, this means: 1 person = $3,256/month, 2 people = $4,408/month. Coverage includes prenatal care, delivery, and postpartum care for 60 days after pregnancy ends. Apply immediately when you learn you're pregnant - coverage can start right away.",
      "requiredFields": [
        "isPregnant",
        "householdIncome",
        "householdSize",
        "livesInGeorgia"
      ],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy-ga",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note, positive pregnancy test, or prenatal clinic verification",
          "required": true,
          "alternatives": [
            "Ultrasound results",
            "Prenatal appointment records"
          ],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-ga-medicaid",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2",
            "1099",
            "Self-employment records"
          ],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at Gateway.GA.gov",
          "url": "https://gateway.ga.gov/",
          "priority": "high",
          "estimatedTime": "30-45 minutes"
        },
        {
          "step": "Call Georgia Medicaid Hotline: 1-877-423-4746",
          "priority": "high"
        }
      ],
      "version": {
