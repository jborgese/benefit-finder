    {
      "id": "medicaid-federal-disability",
      "programId": "medicaid-federal",
      "name": "Medicaid for People with Disabilities (SSI)",
      "description": "People receiving SSI (Supplemental Security Income) automatically qualify for Medicaid",
      "ruleLogic": {
        "or": [
          { "var": "receivesSSI" },
          {
            "and": [
              { "var": "hasQualifyingDisability" },
              {
                "<=": [
                  { "var": "householdIncome" },
                  {
                    "*": [
                      { "var": "householdSize" },
                      1133
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "If you receive Supplemental Security Income (SSI), you automatically qualify for Medicaid in most states. Even if you don't receive SSI, you may qualify for Medicaid if you have a disability and meet income requirements (typically around 74% of FPL for individuals). States may have different rules for people with disabilities under Medicaid Buy-In programs.",
      "requiredFields": ["receivesSSI", "hasQualifyingDisability", "householdIncome", "householdSize"],
      "requiredDocuments": [
        {
          "id": "proof-ssi",
          "name": "Proof of SSI",
          "description": "SSI award letter or benefit verification",
          "required": false,
          "alternatives": ["Social Security benefit statement", "Bank statement showing SSI deposits"],
          "where": "Social Security Administration"
        },
        {
          "id": "proof-disability",
          "name": "Proof of Disability",
          "description": "Social Security disability determination or doctor's certification",
          "required": false,
          "alternatives": ["SSDI award letter", "Medical records", "Functional assessment"],
          "where": "Social Security Administration or healthcare provider"
        }
      ],
      "nextSteps": [
        {
          "step": "If you receive SSI, contact your state Medicaid office - you should be automatically enrolled",
          "priority": "high"
        },
        {
          "step": "If you have a disability but don't receive SSI, ask about Medicaid Buy-In programs and Katie Beckett waivers",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
