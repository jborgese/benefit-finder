    {
      "id": "medicaid-federal-residence-requirement",
      "programId": "medicaid-federal",
      "name": "Medicaid State Residence Requirement",
      "description": "Must be a resident of the state where applying",
      "ruleLogic": {
        "var": "livesInState"
      },
      "ruleType": "eligibility",
      "explanation": "You must be a resident of the state where you apply for Medicaid. Residency means you live in the state and intend to stay or return. You don't need a permanent address - homeless individuals can qualify. You cannot receive Medicaid from more than one state at the same time.",
      "requiredFields": ["livesInState"],
      "requiredDocuments": [
        {
          "id": "proof-residence-medicaid",
          "name": "Proof of State Residence",
          "description": "Mail, utility bill, lease, or employer letter showing state address",
          "required": false,
          "alternatives": ["Driver's license", "Homeless shelter statement", "Statement from person you live with"],
          "where": "Varies by document type"
        }
      ],
