    {
      "id": "medicaid-federal-expansion-income",
      "programId": "medicaid-federal",
      "name": "Medicaid Expansion Income Limit (Adults)",
      "description": "In expansion states, adults under 65 qualify if income is at or below 138% FPL",
      "ruleLogic": {
        "and": [
          { "var": "stateHasExpanded" },
          { "<": [{ "var": "age" }, 65] },
          {
            "<=": [
              { "var": "householdIncome" },
              {
                "*": [
                  { "var": "householdSize" },
                  2040
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Under the Affordable Care Act, states that expanded Medicaid cover adults under age 65 with household income up to 138% of the federal poverty level. For 2024, this means: 1 person = $2,040/month, 2 people = $2,763/month, 3 people = $3,485/month, 4 people = $4,208/month. Currently, 40 states plus DC have expanded Medicaid.",
      "requiredFields": ["stateHasExpanded", "age", "householdIncome", "householdSize"],
      "requiredDocuments": [
        {
          "id": "proof-income",
          "name": "Proof of Income",
          "description": "Recent pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": ["W-2 forms", "1099 forms", "Bank statements", "Self-employment records"],
          "where": "Employer, IRS, bank, or your own records"
        },
        {
          "id": "proof-identity",
          "name": "Proof of Identity",
          "description": "Driver's license, state ID, or passport",
          "required": true,
          "alternatives": ["Birth certificate", "Tribal ID", "School ID with photo"],
          "where": "DMV, State Department, or issuing agency"
        }
      ],
      "nextSteps": [
        {
          "step": "Check if your state has expanded Medicaid at healthcare.gov or medicaid.gov",
          "url": "https://www.medicaid.gov/medicaid/program-information/medicaid-and-chip-eligibility-levels/index.html",
          "priority": "high",
          "estimatedTime": "5 minutes"
        },
        {
          "step": "Apply through your state Medicaid agency or healthcare.gov marketplace",
          "url": "https://www.healthcare.gov",
          "priority": "high",
          "estimatedTime": "30-60 minutes"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
