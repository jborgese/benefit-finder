    {
      "id": "medicaid-federal-citizenship",
      "programId": "medicaid-federal",
      "name": "Medicaid Citizenship Requirement",
      "description": "Must be U.S. citizen, national, or qualified immigrant",
      "ruleLogic": {
        "or": [
          {
            "in": [
              { "var": "citizenship" },
              ["us_citizen", "us_national"]
            ]
          },
          {
            "and": [
              {
                "in": [
                  { "var": "citizenship" },
                  ["permanent_resident", "refugee", "asylee", "trafficking_victim", "cuban_haitian"]
                ]
              },
              { ">=": [{ "var": "yearsInUS" }, 5] }
            ]
          },
          {
            "in": [
              { "var": "citizenship" },
              ["refugee", "asylee", "trafficking_victim", "cuban_haitian"]
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "U.S. citizens and nationals qualify for Medicaid. Lawfully present immigrants may qualify, but most must wait 5 years after obtaining qualified status. Exceptions to the 5-year wait include: refugees, asylees, trafficking victims, Cuban/Haitian entrants, certain military members and families. Emergency Medicaid is available to all regardless of immigration status.",
      "requiredFields": ["citizenship", "yearsInUS"],
      "requiredDocuments": [
        {
          "id": "proof-citizenship-medicaid",
          "name": "Proof of Citizenship or Immigration Status",
          "description": "Birth certificate, passport, green card, or refugee documentation",
          "required": true,
          "alternatives": [
            "U.S. Passport",
            "Naturalization Certificate",
            "Green Card (I-551)",
            "I-94 with refugee/asylee status",
            "I-766 Employment Authorization Document"
          ],
          "where": "Government agency that issued the document"
        }
      ],
      "nextSteps": [
        {
          "step": "Emergency Medicaid covers emergency medical conditions regardless of immigration status",
          "priority": "high"
        },
        {
          "step": "Children and pregnant women may have different immigration requirements - check state rules",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
