    {
      "id": "medicaid-federal-children",
      "programId": "medicaid-federal",
      "name": "Medicaid for Children",
      "description": "Children under 19 in families with income up to 200% FPL (varies by state)",
      "ruleLogic": {
        "and": [
          { "<": [{ "var": "age" }, 19] },
          {
            "<=": [
              { "var": "householdIncome" },
              {
                "*": [
                  { "var": "householdSize" },
                  2960
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "All states cover children under age 19 in families with incomes up to 200% of the federal poverty level through Medicaid or CHIP (Children's Health Insurance Program). Many states cover children at even higher income levels. For 2024 at 200% FPL: 1 child = $2,960/month, 2 children = $4,007/month, 3 children = $5,054/month. Check your state's CHIP program for specific income limits.",
      "requiredFields": ["age", "householdIncome", "householdSize"],
      "requiredDocuments": [
        {
          "id": "proof-age",
          "name": "Proof of Child's Age",
          "description": "Birth certificate, hospital records, or passport",
          "required": true,
          "alternatives": ["School records", "Religious records", "Adoption papers"],
          "where": "Vital records office, hospital, or school"
        },
        {
          "id": "proof-household-income",
          "name": "Proof of Household Income",
          "description": "Parents' pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": ["W-2", "1099", "Self-employment records"],
          "where": "Parents' employers or tax records"
        }
      ],
      "nextSteps": [
        {
          "step": "Check your state's CHIP income limits - they may be higher than 200% FPL",
          "url": "https://www.insurekidsnow.gov",
          "priority": "high"
        },
        {
          "step": "Children can often enroll year-round, not just during open enrollment",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
