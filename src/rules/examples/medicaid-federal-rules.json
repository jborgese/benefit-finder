    {
      "id": "medicaid-federal-pregnant-women",
      "programId": "medicaid-federal",
      "name": "Medicaid for Pregnant Women",
      "description": "Pregnant women with income up to 200% FPL (minimum federal requirement)",
      "ruleLogic": {
        "and": [
          { "var": "isPregnant" },
          {
            "<=": [
              { "var": "householdIncome" },
              {
                "*": [
                  { "var": "householdSize" },
                  2960
                ]
              }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "Pregnant women are covered by Medicaid in all states if family income is at or below 200% of the federal poverty level. Many states cover pregnant women at higher income levels. Coverage includes prenatal care, delivery, and postpartum care for up to 60 days after pregnancy ends. For 2024 at 200% FPL: 1 person = $2,960/month, 2 people = $4,007/month.",
      "requiredFields": ["isPregnant", "householdIncome", "householdSize"],
      "requiredDocuments": [
        {
          "id": "proof-pregnancy",
          "name": "Proof of Pregnancy",
          "description": "Doctor's note or positive pregnancy test",
          "required": true,
          "alternatives": ["Prenatal clinic verification", "Hospital records"],
          "where": "Healthcare provider or clinic"
        },
        {
          "id": "proof-income-pregnant",
          "name": "Proof of Income",
          "description": "Pay stubs, tax returns, or benefit statements",
          "required": true,
          "alternatives": ["W-2", "Unemployment benefits statement"],
          "where": "Employer or government agency"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply as soon as you know you're pregnant - coverage can start immediately",
          "priority": "high",
          "estimatedTime": "Same day to 2 weeks"
        },
        {
          "step": "Many states offer presumptive eligibility for pregnant women, providing temporary coverage while application is processed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
