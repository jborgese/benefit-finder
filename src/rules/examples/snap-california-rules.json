    {
      "id": "calfresh-college-students-expanded",
      "programId": "calfresh",
      "name": "CalFresh College Student Expanded Eligibility",
      "description": "California expanded CalFresh eligibility for college students beyond federal requirements",
      "ruleLogic": {
        "or": [
          { "!": { "var": "isStudent" } },
          { "<": [{ "var": "age" }, 18] },
          { ">": [{ "var": "age" }, 49] },
          { "var": "worksAtLeast20Hours" },
          { "var": "hasChildUnder6" },
          { "var": "hasChildUnder12AndNoChildcare" },
          { "var": "receivesWorkStudy" },
          { "var": "inEmploymentTraining" },
          { "var": "receivesCalWorks" },
          { "var": "expectedFamilyContributionZero" },
          { "var": "receivesGeneralAssistance" }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California expanded CalFresh for college students beyond federal rules. Students age 18-49 enrolled at least half-time can qualify if they: work 20+ hours/week, have work-study, care for a child under 12, participate in employment training, receive CalWORKs or General Assistance, OR have an Expected Family Contribution (EFC) of $0 on their FAFSA. The EFC rule is California-specific and helps many low-income students qualify.",
      "requiredFields": [
        "isStudent",
        "age",
        "worksAtLeast20Hours",
        "hasChildUnder6",
        "hasChildUnder12AndNoChildcare",
        "receivesWorkStudy",
        "inEmploymentTraining",
        "receivesCalWorks",
        "expectedFamilyContributionZero",
        "receivesGeneralAssistance"
      ],
      "requiredDocuments": [
        {
          "id": "proof-student-status-ca",
          "name": "Proof of Student Status",
          "description": "School enrollment verification, class schedule",
          "required": false,
          "alternatives": ["Student ID", "Enrollment letter", "Class schedule"],
          "where": "College registrar"
        },
        {
          "id": "proof-efc-zero",
          "name": "Proof of EFC $0 (if applicable)",
          "description": "FAFSA Student Aid Report showing $0 EFC",
          "required": false,
          "alternatives": ["Financial aid award letter", "FAFSA summary"],
          "where": "College financial aid office or studentaid.gov"
        }
      ],
      "nextSteps": [
        {
          "step": "If you have $0 EFC on FAFSA, bring your Student Aid Report to prove eligibility",
          "priority": "high"
        },
        {
          "step": "Contact your college's CalFresh outreach office - many California colleges have staff to help students apply",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
