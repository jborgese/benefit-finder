    "id": "snap-california-rules-2024",
    "name": "California CalFresh Eligibility Rules (2024)",
    "description": "California-specific SNAP (known as CalFresh in California) eligibility rules for 2024. California has expanded eligibility beyond federal minimums and offers various state-specific benefits and exemptions.",
    "version": {
      "major": 1,
      "minor": 0,
      "patch": 0
    },
    "author": {
      "name": "BenefitFinder Team",
      "organization": "BenefitFinder"
    },
    "jurisdiction": "US-CA",
    "programs": ["calfresh", "snap-california"],
    "tags": ["calfresh", "snap", "food-assistance", "california", "2024", "state-specific"],
    "createdAt": 1728777600000,
    "updatedAt": 1728777600000
  },
  "rules": [
    {
      "id": "calfresh-bbce-no-asset-limit",
      "programId": "calfresh",
      "name": "CalFresh Broad-Based Categorical Eligibility (No Asset Limit)",
      "description": "California eliminates asset tests for most CalFresh applicants through BBCE",
      "ruleLogic": {
        "and": [
          {
            "<=": [
              { "var": "householdIncome" },
              {
                "*": [
                  { "var": "householdSize" },
                  2960
                ]
              }
            ]
          },
          { "!": { "var": "allMembersCategoricallyIneligible" } }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California uses Broad-Based Categorical Eligibility (BBCE), which means: 1) NO asset limit for most households (you can have savings, own a home, have vehicles), and 2) Gross income limit of 200% FPL ($2,960/month for 1 person in 2024). This is much more generous than federal SNAP. Almost all California applicants qualify under BBCE unless all household members are categorically ineligible (undocumented immigrants, students not meeting exemptions, etc.).",
      "requiredFields": ["householdIncome", "householdSize", "allMembersCategoricallyIneligible"],
      "requiredDocuments": [
        {
          "id": "proof-income-ca",
          "name": "Proof of Income",
          "description": "Pay stubs from last 30 days, tax returns, or benefit statements",
          "required": true,
          "alternatives": ["W-2", "1099", "Bank statements", "Self-employment records"],
          "where": "Employer or your records"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply online at BenefitsCal.com (fastest) or GetCalFresh.org (simplified application)",
          "url": "https://www.benefitscal.com/",
          "priority": "high"
        },
        {
          "step": "Alternative: Apply via GetCalFresh.org (mobile-friendly, helps with application)",
          "url": "https://www.getcalfresh.org/",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
      "citations": [
        {
          "title": "CalFresh Eligibility",
          "url": "https://www.cdss.ca.gov/calfresh",
          "date": "2024-10-01",
          "notes": "California BBCE: 200% FPL, no asset limit"
        },
        {
          "title": "CalFresh Handbook Section 63-300",
          "url": "https://www.cdss.ca.gov/food-nutrition/calfresh",
          "date": "2024-10-01"
        }
      ],
      "active": true,
      "draft": false,
      "priority": 30,
      "testCases": [
        {
          "id": "test-ca-bbce-eligible",
          "description": "Household under 200% FPL with assets",
          "input": {
            "householdIncome": 2500,
            "householdSize": 1,
            "allMembersCategoricallyIneligible": false
          },
          "expected": true,
          "tags": ["eligible", "bbce", "california"]
        },
        {
          "id": "test-ca-bbce-eligible-high-assets",
          "description": "Household under 200% FPL with high assets (still eligible due to no asset limit)",
          "input": {
            "householdIncome": 2800,
            "householdSize": 1,
            "allMembersCategoricallyIneligible": false
          },
          "expected": true,
          "tags": ["eligible", "bbce", "no-asset-test"]
        },
        {
          "id": "test-ca-bbce-ineligible-income",
          "description": "Income above 200% FPL",
          "input": {
            "householdIncome": 3500,
            "householdSize": 1,
            "allMembersCategoricallyIneligible": false
          },
          "expected": false,
          "tags": ["ineligible", "high-income"]
        },
        {
          "id": "test-ca-categorically-ineligible",
          "description": "All members categorically ineligible",
          "input": {
            "householdIncome": 2000,
            "householdSize": 1,
            "allMembersCategoricallyIneligible": true
          },
          "expected": false,
          "tags": ["ineligible", "categorical"]
        }
      ],
      "createdAt": 1728777600000,
      "updatedAt": 1728777600000,
      "tags": ["bbce", "no-asset-limit", "200-percent-fpl", "california-specific"],
      "category": "categorical-eligibility",
      "jurisdiction": "US-CA"
