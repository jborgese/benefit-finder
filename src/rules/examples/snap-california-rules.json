    {
      "id": "calfresh-elderly-simplified-reporting",
      "programId": "calfresh",
      "name": "CalFresh Elderly Simplified Reporting",
      "description": "Elderly and disabled households in California have simplified reporting requirements",
      "ruleLogic": {
        "or": [
          { ">=": [{ "var": "age" }, 60] },
          { "var": "hasQualifyingDisability" }
        ]
      },
      "ruleType": "conditional",
      "explanation": "California provides simplified reporting for elderly (60+) or disabled CalFresh recipients. Benefits: 1) 12-month certification period (instead of 6), 2) Only report changes if income exceeds 130% FPL, 3) Simplified annual renewal process. This reduces paperwork burden and helps ensure continuous coverage.",
      "requiredFields": ["age", "hasQualifyingDisability"],
      "nextSteps": [
        {
          "step": "If you're 60+ or disabled, you automatically get simplified reporting - no extra application needed",
          "priority": "low"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
