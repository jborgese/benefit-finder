    {
      "id": "calfresh-application-assistance",
      "programId": "calfresh",
      "name": "CalFresh Application Process and Assistance",
      "description": "Information about applying for CalFresh in California with multiple application methods",
      "ruleLogic": {
        "var": "livesInCalifornia"
      },
      "ruleType": "conditional",
      "explanation": "California offers multiple ways to apply for CalFresh: 1) **BenefitsCal.com** (official state website), 2) **GetCalFresh.org** (mobile-friendly, helps with application, run by Code for America), 3) Phone: 1-877-847-3663, 4) In-person at county social services office. GetCalFresh.org is popular because it's easy to use on mobile devices and provides application assistance. Most applicants are interviewed within 30 days. Expedited service (within 3 days) available for households with urgent need.",
      "requiredFields": ["livesInCalifornia"],
      "nextSteps": [
        {
          "step": "Apply via GetCalFresh.org (recommended - easiest, mobile-friendly)",
          "url": "https://www.getcalfresh.org/",
          "priority": "high",
          "estimatedTime": "15-30 minutes"
        },
        {
          "step": "Or apply via official BenefitsCal.com",
          "url": "https://www.benefitscal.com/",
          "priority": "high"
        },
        {
          "step": "Find your county social services office",
          "url": "https://www.cdss.ca.gov/county-offices",
          "priority": "medium"
        }
      ],
