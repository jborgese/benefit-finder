    {
      "id": "calfresh-immigrant-eligibility",
      "programId": "calfresh",
      "name": "CalFresh Immigrant Eligibility Expansion",
      "description": "California provides CalFresh to certain immigrants using state funds, even if not eligible for federal SNAP",
      "ruleLogic": {
        "or": [
          {
            "in": [
              { "var": "immigration_status" },
              ["us_citizen", "permanent_resident", "refugee", "asylee", "trafficking_victim"]
            ]
          },
          {
            "and": [
              {
                "in": [
                  { "var": "immigration_status" },
                  ["lawfully_present", "daca", "other_qualified"]
                ]
              },
              { "var": "inCalifornia" }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "California expanded CalFresh to immigrants beyond federal SNAP eligibility. Using state funds, California provides CalFresh to: lawfully residing immigrants (regardless of 5-year wait), DACA recipients, and certain other immigrants who would otherwise be ineligible for federal SNAP. U.S. citizens, green card holders, refugees, and asylees qualify for federal SNAP. Others may qualify for state-funded CalFresh.",
      "requiredFields": ["immigration_status", "inCalifornia"],
      "requiredDocuments": [
        {
          "id": "proof-immigration-status-ca",
          "name": "Proof of Immigration Status",
          "description": "Immigration documents showing status",
          "required": true,
          "alternatives": [
            "Green Card (I-551)",
            "Employment Authorization (I-766)",
            "I-94 Arrival/Departure Record",
            "Refugee documentation",
            "DACA approval notice"
          ],
          "where": "USCIS"
        }
      ],
      "nextSteps": [
        {
          "step": "Apply even if you're not sure about eligibility - California has expanded rules for immigrants",
          "priority": "high"
        },
        {
          "step": "Contact an immigration-friendly food bank or legal aid for assistance with application",
          "priority": "medium"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
