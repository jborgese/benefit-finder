    {
      "id": "wic-federal-adjunctive-eligibility",
      "programId": "wic-federal",
      "name": "WIC Adjunctive Eligibility",
      "description": "Automatically income-eligible if receiving SNAP, Medicaid, or TANF",
      "ruleLogic": {
        "or": [
          { "var": "receivesSNAP" },
          { "var": "receivesMedicaid" },
          { "var": "receivesTANF" }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "If you or your child already receives SNAP (food stamps), Medicaid, or TANF (Temporary Assistance for Needy Families), you automatically meet WIC's income requirement. This is called 'adjunctive eligibility.' You still need to meet the categorical requirements (be pregnant, postpartum, breastfeeding, infant, or child under 5) and nutritional risk criteria.",
      "requiredFields": ["receivesSNAP", "receivesMedicaid", "receivesTANF"],
      "requiredDocuments": [
        {
          "id": "proof-snap",
          "name": "Proof of SNAP Benefits",
          "description": "EBT card or SNAP award letter",
          "required": false,
          "alternatives": ["State benefit card", "Award letter from SNAP office"],
          "where": "State SNAP office"
        },
        {
          "id": "proof-medicaid-wic",
          "name": "Proof of Medicaid",
          "description": "Medicaid card or eligibility letter",
          "required": false,
          "alternatives": ["State Medicaid card", "Healthcare.gov confirmation"],
          "where": "State Medicaid office"
        },
        {
          "id": "proof-tanf",
          "name": "Proof of TANF Benefits",
          "description": "TANF award letter or benefit card",
          "required": false,
          "alternatives": ["State benefit card", "Award letter"],
          "where": "State TANF office"
        }
      ],
      "nextSteps": [
        {
          "step": "Bring your benefit card or award letter to WIC appointment - no additional income documentation needed",
          "priority": "high"
        }
      ],
      "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
      },
