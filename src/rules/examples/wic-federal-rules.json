    {
      "id": "wic-federal-income-limit",
      "programId": "wic-federal",
      "name": "WIC Income Limit",
      "description": "Household income must be at or below 185% of the federal poverty level",
      "ruleLogic": {
        "<=": [
          { "var": "householdIncome" },
          {
            "*": [
              { "var": "householdSize" },
              2743
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "To qualify for WIC, your household's gross income must be at or below 185% of the federal poverty level. For 2024, this means: 1 person = $2,743/month, 2 people = $3,707/month, 3 people = $4,671/month, 4 people = $5,635/month. Add $964 per additional person. If you're already receiving SNAP, Medicaid, or TANF, you automatically meet the income requirement.",
      "requiredFields": ["householdIncome", "householdSize"],
      "requiredDocuments": [
        {
          "id": "proof-income-wic",
          "name": "Proof of Income",
          "description": "Pay stubs from the last 4 weeks, tax returns, or benefit statements",
          "required": true,
          "alternatives": [
            "W-2 forms",
            "1099 forms",
            "SNAP award letter",
            "Medicaid card",
            "TANF award letter",
            "Unemployment benefits statement",
            "Child support records"
          ],
          "where": "Employer, government agency, or your own records"
        }
      ],
      "nextSteps": [
        {
          "step": "If you receive SNAP, Medicaid, or TANF, bring proof - you automatically meet WIC income requirements",
          "priority": "high"
        }
      ],
