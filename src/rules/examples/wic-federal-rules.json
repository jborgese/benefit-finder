    "updatedAt": 1728777600000
  },
  "rules": [
    {
      "id": "wic-federal-categorical-eligibility",
      "programId": "wic-federal",
      "name": "WIC Categorical Eligibility",
      "description": "Must be pregnant, postpartum (up to 6 months), breastfeeding (up to 1 year), infant (under 1 year), or child (1-5 years)",
      "ruleLogic": {
        "or": [
          { "var": "isPregnant" },
          {
            "and": [
              { "var": "isPostpartum" },
              { "<=": [{ "var": "monthsSinceBirth" }, 6] }
            ]
          },
          {
            "and": [
              { "var": "isBreastfeeding" },
              { "<=": [{ "var": "monthsSinceBirth" }, 12] }
            ]
          },
          {
            "and": [
              { "var": "isInfant" },
              { "<": [{ "var": "ageInMonths" }, 12] }
            ]
          },
          {
            "and": [
              { "var": "isChild" },
              { ">=": [{ "var": "ageInMonths" }, 12] },
              { "<": [{ "var": "ageInMonths" }, 60] }
            ]
          }
        ]
      },
      "ruleType": "eligibility",
      "explanation": "WIC serves specific groups: pregnant women (throughout pregnancy and up to 6 weeks after), postpartum women (up to 6 months after delivery), breastfeeding women (up to 1 year after delivery), infants (under 1 year old), and children (ages 1 to 5 years old). You must fall into one of these categories to be eligible.",
      "requiredFields": [
        "isPregnant",
        "isPostpartum",
        "monthsSinceBirth",
        "isBreastfeeding",
        "isInfant",
        "isChild",
        "ageInMonths"
      ],
      "requiredDocuments": [
        {
          "id": "proof-identity-wic",
          "name": "Proof of Identity",
          "description": "Photo ID, driver's license, or other identification",
          "required": true,
          "alternatives": ["Passport", "State ID", "Work ID badge", "Student ID"],
          "where": "DMV or issuing organization"
        },
        {
          "id": "proof-pregnancy-wic",
          "name": "Proof of Pregnancy (if applicable)",
          "description": "Doctor's note or positive pregnancy test",
          "required": false,
          "alternatives": ["Prenatal clinic verification", "Hospital records"],
          "where": "Healthcare provider"
        },
        {
          "id": "child-birth-certificate",
          "name": "Child's Birth Certificate or Hospital Records (if applicable)",
          "description": "Proof of child's age",
          "required": false,
          "alternatives": ["Immunization records", "Hospital discharge papers", "Passport"],
          "where": "Vital records office or hospital"
        }
      ],
      "nextSteps": [
        {
          "step": "Find your local WIC clinic",
          "url": "https://www.fns.usda.gov/wic/wic-contacts",
          "priority": "high",
          "estimatedTime": "5 minutes"
        }
      ],
      "version": {
        "major": 1,
